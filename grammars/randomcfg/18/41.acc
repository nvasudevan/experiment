%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: TK_AZ JZ;

NL: TK_PGEZ | NL TOYPM NI TK_SS | 'X' BV LN;

NI: NL BCSI TK_ISIWH | RKUUX TK_LFTP TK_ADRF | RIC RIC | NL AAA BCSI | TOYPM TK_ON BV QBP;

AAA: TK_KPO | TK_PGEZ NI GVTPU | RJ 'D' QBP TK_PGEZ | BCSI TK_IMM TK_IMM TK_ON | JZ;

GVTPU: LN RF | RF TK_LFTP GSBBT TK_SS LN | ;

JZ: TK_BQEDS GVTPU | RKUUX RJ | GVTPU 'D' 'X' TOYPM | ;

GSBBT: TK_ON TK_SS 'Y' 'D' | TK_IMM TK_DXSZ BY TK_OC | AAA TK_DXSZ TK_ADRF BV GSBBT | TK_ADRF;

LN: QBP TK_ARJ | TK_KPO | RJ AAA V;

RF: LN 'D' TK_ISIWH | ;

RIC: 'X' TK_YUZOZ;

BV: TK_DXSZ | TK_PGEZ | 'X' TK_ARJ TK_IMM | TK_LFTP TK_ON TK_OC JZ | TK_DXSZ JZ;

RJ: TK_PGEZ RKUUX TK_ISIWH 'X' | AAA JZ TK_ON | TK_PGEZ V AAA TK_AZ | RIC TK_ARJ GVTPU;

V: RJ NL | 'D' TK_PGEZ TK_PGEZ TOYPM | TK_LFTP RF 'Y' 'Y';

TOYPM: TK_ARJ BV NL | TK_ARJ 'X' RKUUX TK_LFTP TK_ISIWH | TK_ADRF TK_KPO TK_SS;

BCSI: RF TK_AZ TK_KPO TK_ON | 'X' TK_PGEZ QBP TK_BQEDS V | 'D' TK_LFTP JZ | TK_BQEDS BCSI | TK_YUZOZ TK_AZ TK_IMM 'K' BCSI;

QBP: GVTPU NL 'Y' QBP | RIC TK_ISIWH TK_ISIWH TK_IMM | TK_LFTP 'K' RF TK_IMM BV | TK_ON TK_ADRF TK_YUZOZ BY | TK_AZ V TK_AZ TK_PGEZ TK_AZ;

BY: BV 'Y' TK_AZ GSBBT | TK_SS TK_BQEDS | TK_ARJ TK_KPO | 'Y' | TK_PGEZ LN;

RKUUX: TK_PGEZ 'X' TK_AZ QBP | BY TK_KPO RIC LN BCSI | JZ QBP TK_DXSZ TK_LFTP GSBBT;

