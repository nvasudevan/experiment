%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: RF TK_KPO TK_PGEZ TK_SS 'K' | TK_DXSZ | GSBBT TK_SS;

NL: TOYPM TK_ARJ GVTPU AAA NI | TK_KPO AAA AAA RJ 'X' | RF TOYPM TK_DXSZ AAA | GVTPU TK_DXSZ GVTPU;

NI: TK_ARJ V TK_ISIWH TK_LFTP | RJ NL | RKUUX 'K' TK_OC GVTPU;

AAA: TK_DXSZ NI BV | QBP BCSI BCSI RKUUX | 'X' NL TK_AZ TK_ISIWH TK_SS | TK_DXSZ;

GVTPU: TOYPM | TK_BQEDS GVTPU GSBBT V TK_BQEDS | BV TK_PGEZ 'D' GSBBT BY | TK_ADRF RKUUX TK_PGEZ TK_KPO RF;

JZ: BV 'K' | 'K';

GSBBT: RF | TK_KPO TK_KPO RJ BCSI | TK_IMM TK_ADRF RIC | AAA LN | RF LN BV AAA LN;

LN: 'D' V 'Y' | RJ TOYPM | GSBBT GSBBT TK_AZ NI | RF TK_LFTP | ;

RF: TK_SS | TK_IMM TK_IMM TK_DXSZ TK_OC | ;

RIC: TK_BQEDS TK_AZ BV GVTPU TK_AZ | QBP | QBP NI TK_KPO TK_ON;

BV: 'X' RKUUX 'X';

RJ: TK_ADRF GSBBT TK_ON 'K';

V: GVTPU GVTPU TK_KPO TK_BQEDS BY | TK_BQEDS TK_ADRF RF TK_IMM TK_ARJ | NL TK_LFTP NI;

TOYPM: GSBBT | 'Y' | TK_AZ RKUUX RIC TK_OC BCSI | TK_PGEZ | TK_PGEZ TK_ADRF;

BCSI: 'K' TK_LFTP RF V 'K' | 'X' TK_ADRF;

QBP: RJ | TK_LFTP QBP TK_ARJ TK_YUZOZ | TK_LFTP 'D' TK_IMM;

BY: TK_YUZOZ JZ TK_PGEZ 'D' V | TK_ON;

RKUUX: 'D' BY | TK_ADRF TK_BQEDS TK_ON | QBP TK_ISIWH TK_OC V;

