%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: RIC RKUUX | RF BY TK_DXSZ 'Y' NL | TK_YUZOZ BY | RJ;

NL: RF JZ GSBBT | TK_DXSZ BV TK_ISIWH;

NI: TK_DXSZ RKUUX TK_OC TK_ARJ TK_DXSZ | TK_IMM TK_IMM;

AAA: V GVTPU | TK_OC BY JZ QBP BY;

GVTPU: TK_DXSZ | NI RJ TK_ISIWH | BY NL JZ TK_PGEZ TK_ISIWH | TK_ADRF RF GVTPU TK_LFTP;

JZ: TK_OC TK_ON TK_YUZOZ TK_KPO RJ | TOYPM TK_LFTP BV RKUUX NI | LN LN TK_KPO JZ | TK_ARJ RF LN TK_ISIWH;

GSBBT: TK_SS GSBBT TK_KPO TK_ARJ | ;

LN: TK_YUZOZ TK_BQEDS V BCSI BY | RIC | TK_ON;

RF: TK_ON TK_DXSZ RKUUX TK_ON | BCSI TK_KPO 'Y' | TK_ARJ JZ JZ | TK_PGEZ TK_KPO TK_BQEDS TK_ON | RKUUX TK_PGEZ;

RIC: TK_ADRF TK_OC TK_KPO;

BV: NL GSBBT | 'Y' NI;

RJ: GVTPU;

V: TK_LFTP TK_YUZOZ | 'K' RKUUX LN BCSI TK_ISIWH | 'Y' RJ JZ | 'X' QBP TOYPM | TK_IMM;

TOYPM: RJ BCSI TK_SS RIC | TK_ADRF | LN TK_ADRF QBP TK_AZ TK_ON;

BCSI: TK_ARJ GSBBT TK_OC TK_OC NL | TK_PGEZ TK_AZ 'X' JZ | RF AAA 'X' BY | ;

QBP: BCSI NI TOYPM TK_KPO JZ;

BY: RKUUX | TK_ADRF 'D' BCSI TK_ON | TK_BQEDS NL | TK_ISIWH TK_SS NI TK_BQEDS | GSBBT NI 'D';

RKUUX: TK_ISIWH JZ GSBBT | 'D' TK_OC | QBP;

