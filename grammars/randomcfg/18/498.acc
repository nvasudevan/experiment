%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: NL RJ | TK_SS RJ BY LN;

NL: 'K' TK_ADRF;

NI: TK_IMM TK_IMM RF TK_ISIWH TK_ON | RJ AAA V TK_DXSZ 'D' | TK_ISIWH | RKUUX | JZ RJ;

AAA: TK_ON TK_AZ TK_AZ TK_KPO TK_ON | TK_LFTP | TK_AZ TK_YUZOZ RJ NI TK_LFTP;

GVTPU: JZ TK_YUZOZ | RKUUX AAA TK_OC JZ | NI TK_BQEDS TK_ADRF NI NL | ;

JZ: GSBBT BY TK_LFTP QBP 'K' | TK_YUZOZ NL TOYPM QBP | TK_ON;

GSBBT: TK_SS | TK_IMM GVTPU TK_PGEZ;

LN: TK_SS | RF TK_SS;

RF: TK_BQEDS TOYPM NL | BV TK_AZ TK_ARJ TK_PGEZ 'Y' | LN JZ;

RIC: BCSI TK_AZ | GSBBT TK_KPO | ;

BV: TK_ISIWH TK_PGEZ | TK_ON TK_YUZOZ BV TK_KPO TK_OC | GSBBT RJ TK_SS TK_ARJ | RKUUX TK_LFTP | TK_IMM TK_IMM;

RJ: TK_ON RF | BCSI RJ | RJ BV TK_ADRF TK_SS 'X' | BCSI LN 'D';

V: TK_SS QBP GVTPU TK_LFTP | RF;

TOYPM: 'K';

BCSI: TK_ON TK_IMM RJ GVTPU | TOYPM TK_ISIWH QBP 'Y';

QBP: RJ V TK_OC 'Y' | TK_ARJ TK_KPO;

BY: TK_ARJ TK_ADRF TK_ARJ TK_OC 'K' | RIC GSBBT RF | RKUUX | TK_ON GSBBT;

RKUUX: LN 'K' TK_PGEZ TK_ON BY;

