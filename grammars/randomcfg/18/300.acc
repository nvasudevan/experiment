%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: TK_ISIWH | TK_KPO TK_ON TK_SS | TK_KPO TK_LFTP BCSI LN;

NL: TK_AZ V | GVTPU;

NI: RKUUX;

AAA: TK_OC | TOYPM RJ TK_LFTP TOYPM TOYPM | QBP BV NL TK_YUZOZ AAA | TK_KPO | LN 'K';

GVTPU: TK_YUZOZ RJ TK_ADRF | RKUUX;

JZ: TOYPM LN GVTPU 'Y' | TK_YUZOZ NL | ;

GSBBT: TOYPM TK_DXSZ TK_BQEDS TK_LFTP | 'Y' V TK_BQEDS GVTPU | NI 'Y' TK_YUZOZ GVTPU TK_ISIWH;

LN: TK_ADRF RF TK_YUZOZ | TK_KPO TK_OC | BV TK_ON;

RF: LN TK_KPO GSBBT NI | 'K' TK_ADRF | RKUUX TK_KPO GVTPU 'Y' | TK_KPO | NI NI;

RIC: GVTPU TK_ON NI | TK_IMM BY RKUUX 'D' V | TK_ADRF RJ TK_IMM GVTPU | RJ GVTPU RIC;

BV: 'K' TK_LFTP JZ TK_YUZOZ | 'D';

RJ: TK_KPO RF TK_YUZOZ V | ;

V: 'K' V TK_DXSZ V 'Y' | RF RJ AAA BY TK_KPO | RIC GSBBT TK_PGEZ RIC GVTPU | V TK_ON TK_OC BCSI TK_DXSZ;

TOYPM: TK_DXSZ AAA TK_ON TK_ON | TK_KPO TK_ARJ | TK_PGEZ TK_PGEZ | 'X' BCSI GVTPU NI;

BCSI: TK_SS | GSBBT NI TK_IMM TK_LFTP TK_AZ | TK_ON | TK_KPO TK_YUZOZ;

QBP: NL NL BCSI;

BY: RIC GVTPU BCSI TK_PGEZ | 'D' TK_OC | BV | TK_ISIWH TK_BQEDS TK_BQEDS | 'D' TK_ON;

RKUUX: JZ | RIC AAA TK_ISIWH 'K' | TK_ISIWH;

