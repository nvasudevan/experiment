%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: V TOYPM BY BV | 'Y' | V TK_ON TK_AZ TK_PGEZ TOYPM | TK_BQEDS GVTPU TK_DXSZ TK_AZ RF;

NL: NL | TK_YUZOZ TK_ARJ NI TK_OC | BV 'X' AAA | ;

NI: GVTPU TK_YUZOZ RJ TK_OC 'K';

AAA: GSBBT QBP 'K' RJ TK_AZ | GVTPU;

GVTPU: TK_PGEZ TK_PGEZ AAA NI TOYPM | TK_PGEZ BV BCSI | TK_DXSZ | RF;

JZ: BCSI TK_SS LN TK_SS TK_YUZOZ | 'D' NL NI TK_ON TK_BQEDS;

GSBBT: TK_YUZOZ TK_AZ GVTPU TK_LFTP;

LN: RJ BCSI | V;

RF: NL TK_AZ 'Y' | 'D' GSBBT TK_LFTP GVTPU TK_BQEDS | BY | JZ AAA TK_LFTP RKUUX BV;

RIC: NL BV RKUUX TK_DXSZ | TK_ISIWH TK_LFTP 'K' TK_BQEDS TK_SS | BCSI TK_IMM QBP TK_KPO | ;

BV: BCSI | QBP AAA LN TK_DXSZ RKUUX | NI | JZ TK_ISIWH TK_ISIWH;

RJ: TK_ISIWH TOYPM | TK_DXSZ BV;

V: TOYPM TK_SS | TK_ARJ 'K' BV TK_KPO | 'Y' V BCSI TK_ARJ TK_OC | TK_ISIWH TK_DXSZ TK_OC JZ TK_SS;

TOYPM: TK_AZ QBP LN TK_BQEDS BCSI;

BCSI: BV TK_ADRF TK_ON TK_BQEDS TK_AZ | AAA TK_AZ TK_SS | TK_ISIWH TK_IMM TK_ON | GVTPU 'K' TK_ADRF TOYPM | AAA JZ TK_AZ RKUUX RF;

QBP: 'Y' | RIC | BV NI V;

BY: 'D' V | TK_LFTP RF 'D' 'X';

RKUUX: RIC | TK_DXSZ | TK_AZ AAA | TK_AZ BY TK_ADRF TK_LFTP LN;

