%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: TK_KPO | GSBBT | TK_OC QBP GVTPU RJ V | NI TK_OC;

NL: RJ | RKUUX LN RJ RIC AAA | ;

NI: QBP 'X' QBP TK_KPO | 'X' V RKUUX TK_ON JZ | 'K' GSBBT 'X' LN | JZ 'D' | BY TK_DXSZ;

AAA: TK_SS JZ GVTPU TK_ADRF TK_LFTP | 'X' TK_DXSZ;

GVTPU: RF | BV RF GVTPU | 'D' GSBBT;

JZ: TK_KPO | LN TK_YUZOZ RJ TK_BQEDS BY | TK_ADRF BY 'D' NI;

GSBBT: 'D' | NI | TK_ON TK_BQEDS BY 'D';

LN: 'X' TK_OC GVTPU TK_OC BCSI | 'K' BCSI JZ TK_BQEDS RF | 'X' AAA V TK_ARJ TK_ARJ | 'K';

RF: GVTPU 'X' BV TK_YUZOZ | TK_ON NI | BY TK_OC BCSI RKUUX RIC;

RIC: 'D' | TK_PGEZ BCSI BV | TOYPM RKUUX BCSI TK_BQEDS | TK_ISIWH | RF;

BV: JZ BV TK_ISIWH GVTPU TK_SS | GVTPU;

RJ: RJ | RKUUX TK_ON LN AAA TK_OC | JZ NI 'X' V GVTPU;

V: TK_ISIWH TK_OC BCSI TK_KPO 'K';

TOYPM: QBP | ;

BCSI: TK_DXSZ 'K' TK_DXSZ RJ TK_ADRF | TOYPM TK_ON GSBBT | TK_IMM TK_PGEZ TK_KPO;

QBP: RF BY TOYPM TK_ADRF | AAA 'D' GVTPU | V TOYPM TOYPM | TK_LFTP LN;

BY: GVTPU BY 'D' TK_SS TK_KPO | NI GSBBT TK_ADRF | TK_SS GVTPU TK_PGEZ RIC | 'Y' V | GSBBT BCSI BY;

RKUUX: RIC TK_IMM 'K';

