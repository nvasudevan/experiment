%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: TK_SS LN | TK_DXSZ 'X' TK_YUZOZ BCSI | QBP TK_BQEDS TK_BQEDS | TK_DXSZ TK_AZ 'D' RKUUX RKUUX | BCSI TK_ISIWH 'D';

NL: V TK_YUZOZ | TK_YUZOZ AAA TK_ISIWH TK_LFTP RF | TK_ON TK_YUZOZ AAA TK_OC NL | TOYPM TK_IMM;

NI: AAA QBP TK_KPO TK_OC | TK_AZ 'X' BV TK_DXSZ RJ;

