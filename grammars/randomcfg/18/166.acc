%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: NL TK_ISIWH | TK_ADRF | TK_ADRF TK_ADRF RIC;

NL: GVTPU | RIC TK_IMM TK_PGEZ | TK_ADRF RKUUX TK_AZ | LN JZ | RKUUX TK_OC TK_ON TK_IMM 'K';

NI: TK_ISIWH TK_IMM TK_AZ | TK_PGEZ V;

AAA: AAA RKUUX | NL AAA RIC TK_LFTP | V NI TK_KPO TK_LFTP | RJ BY;

GVTPU: TK_ARJ | TOYPM RF GVTPU | TK_KPO TK_BQEDS NI V | RIC GSBBT;

JZ: TK_ARJ TK_KPO | TK_YUZOZ GVTPU | BV V JZ | ;

GSBBT: TK_ADRF TK_KPO | TK_ON TK_PGEZ TK_YUZOZ RF TK_OC | NI BY NI | TK_DXSZ TK_SS;

LN: QBP BY | TK_OC JZ TK_LFTP TK_YUZOZ RF | TK_OC NI | TK_BQEDS RJ TK_DXSZ | TK_DXSZ LN JZ TK_YUZOZ;

RF: AAA 'K' | GSBBT | V 'K' GSBBT TK_DXSZ | TK_ISIWH V RIC;

RIC: NL TK_ADRF TK_SS | TK_OC BCSI TK_IMM BY | TK_DXSZ V RJ | JZ TK_ON NI TOYPM TK_PGEZ | TK_AZ RKUUX LN;

BV: TK_SS GSBBT TK_ADRF;

RJ: TK_BQEDS 'K' BY TOYPM | RJ TK_DXSZ | V | GVTPU 'X' 'K' AAA | RKUUX TK_OC GSBBT;

V: TK_SS TK_YUZOZ 'Y' JZ | RKUUX BCSI 'X' | TK_ADRF BCSI;

TOYPM: V LN TK_KPO | TK_YUZOZ | QBP 'D' AAA QBP TK_PGEZ | ;

BCSI: 'D' JZ RIC TK_ISIWH | AAA BY | TK_SS TK_ISIWH RKUUX BY | ;

QBP: BY RJ TK_KPO TK_ARJ | RIC 'Y' TK_ADRF NL BCSI | TK_OC RJ NI | JZ RKUUX TK_IMM TK_ADRF | 'X' TK_BQEDS RJ NL;

BY: TK_ISIWH | TK_AZ TK_ARJ;

RKUUX: TK_AZ | TK_ADRF BV TK_YUZOZ RKUUX TK_PGEZ | TK_KPO TK_ISIWH | BV | 'X' AAA TOYPM TK_OC BCSI;

