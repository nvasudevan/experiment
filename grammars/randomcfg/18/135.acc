%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: RJ | BV RF | TK_SS TK_AZ;

NL: TOYPM TOYPM | RF JZ TK_PGEZ NL GSBBT;

NI: TK_SS NI TK_AZ | ;

AAA: LN RJ TK_ON BY 'D' | QBP 'K' TK_ISIWH TK_LFTP TK_PGEZ | 'D' | QBP TK_ADRF RJ;

GVTPU: RIC 'Y' | TOYPM NL GVTPU | 'X' V NL TK_PGEZ | TK_IMM TK_LFTP BV TK_KPO;

JZ: BY;

GSBBT: RJ | NI NL | RF | BY TK_ISIWH RIC | QBP V 'D' V 'D';

LN: RIC TK_ON GSBBT | 'X' NI | JZ V TK_ISIWH TK_LFTP | TK_LFTP RIC 'D' | GVTPU;

RF: RIC RKUUX;

RIC: TOYPM TK_BQEDS TK_OC TK_ARJ;

BV: LN | NL | TK_IMM LN TK_SS TK_ON BCSI | TK_ON RJ;

RJ: RIC JZ TK_ON QBP | TK_YUZOZ BY TK_ON | TK_BQEDS V TK_DXSZ GVTPU TK_ISIWH;

V: 'K' TK_LFTP TK_ON 'K' 'Y' | GVTPU | TK_ARJ NL NI | TK_DXSZ TOYPM TK_IMM TK_BQEDS GSBBT;

TOYPM: QBP AAA TK_ARJ TK_ADRF | 'Y' LN | TK_DXSZ;

BCSI: TK_DXSZ | 'Y' | GVTPU V TOYPM | TK_PGEZ TK_AZ RJ | RKUUX 'D' RJ LN;

QBP: 'X' | TK_AZ | RJ LN TK_AZ GVTPU BY | 'Y';

BY: TK_IMM LN RF JZ TK_YUZOZ | V 'D' TK_PGEZ RF;

RKUUX: TK_AZ RKUUX TK_DXSZ | GSBBT QBP 'D' TK_DXSZ TK_SS | TK_AZ TK_ON TK_ADRF RJ 'D' | RIC TK_ARJ | ;

