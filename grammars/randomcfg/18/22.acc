%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: 'X' V TK_AZ RJ AAA | RIC GSBBT TK_BQEDS | BY TK_SS | RF TK_KPO TK_ISIWH 'K' V | 'D' NL RJ;

NL: QBP;

NI: TK_PGEZ;

AAA: QBP GVTPU TK_BQEDS TOYPM | 'Y' RJ NI;

GVTPU: LN TOYPM TK_LFTP TK_ISIWH JZ | 'X' TK_AZ TK_BQEDS RIC TOYPM;

JZ: BY TK_ARJ TK_ISIWH LN TK_AZ | TK_ADRF BCSI 'D' JZ TK_IMM | BV TK_LFTP TK_DXSZ TK_ARJ TK_SS | TK_ISIWH BCSI GVTPU | TK_PGEZ TK_ISIWH;

GSBBT: AAA BY TK_DXSZ NL;

LN: BCSI 'K' TK_SS BY TK_ISIWH | TK_IMM LN NI | ;

RF: NI GSBBT TK_ISIWH NL | RIC TK_LFTP TK_DXSZ | TK_PGEZ AAA RJ GSBBT | RIC TK_AZ TK_ON TK_DXSZ BY;

RIC: TK_LFTP | BV;

BV: GVTPU RKUUX;

RJ: TK_ON JZ TK_SS | TOYPM | TK_AZ TK_ON JZ TK_BQEDS | GVTPU | ;

V: 'Y' | TK_KPO NL QBP TK_SS | TK_ON 'K' 'K' QBP 'Y' | TK_DXSZ;

TOYPM: GVTPU TK_PGEZ TK_ADRF TK_LFTP TOYPM | TK_IMM TK_PGEZ BCSI BCSI | NL 'D' TK_LFTP TK_YUZOZ TK_IMM;

BCSI: TK_OC TK_BQEDS;

QBP: RF BCSI QBP | TOYPM NL | RKUUX 'Y' LN 'K' 'Y' | LN | TK_ISIWH;

BY: GSBBT | JZ RIC 'X' TK_DXSZ NI;

RKUUX: TK_ON 'Y' NL V | TK_DXSZ QBP TK_DXSZ RJ;

