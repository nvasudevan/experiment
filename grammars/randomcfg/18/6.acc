%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: BV TK_OC 'D' RF V | GVTPU NI TK_IMM TK_SS | GVTPU | LN NL TK_BQEDS NI NL;

NL: RKUUX TK_IMM TK_KPO TK_DXSZ RIC | RJ RKUUX 'D' AAA JZ | RKUUX BY | TK_BQEDS | TK_ADRF 'X' LN 'K' TK_YUZOZ;

NI: TK_YUZOZ TK_OC RKUUX JZ TK_YUZOZ | V GVTPU TK_ARJ TK_LFTP | BY 'X' RKUUX RJ RKUUX | 'D' TK_LFTP;

AAA: TK_ON | TK_KPO TK_BQEDS | QBP RIC 'K' RJ RKUUX;

GVTPU: TK_DXSZ TOYPM | 'X' | TK_IMM 'K' TK_PGEZ;

JZ: TK_SS 'X' 'K' RIC | TK_LFTP NI TK_AZ RF | ;

GSBBT: TK_ON;

LN: QBP BY TK_YUZOZ | TK_SS NL | TK_BQEDS BCSI TK_ISIWH NI RIC | TK_KPO LN;

RF: TK_KPO | AAA BY TK_ON;

RIC: TK_IMM TK_ON 'D' | V TK_AZ BY | GSBBT TK_ADRF;

BV: GSBBT TK_DXSZ RJ TK_IMM | QBP 'D' | TK_PGEZ NI;

RJ: LN TK_IMM | V TK_SS | AAA BV | TOYPM RF TK_PGEZ TK_AZ | ;

V: TK_IMM BY TK_ON | GSBBT TK_BQEDS | 'Y' 'X' NI TK_ADRF NI | BCSI | 'K';

TOYPM: TK_BQEDS TK_BQEDS TOYPM RJ | TK_YUZOZ BY;

BCSI: RJ TK_ON TK_ADRF GSBBT | TK_ISIWH | 'Y' TK_DXSZ | 'K' 'D' TK_ISIWH BV 'X' | 'X' RIC LN V;

QBP: 'D' TK_ISIWH BV | TK_DXSZ GSBBT;

BY: TK_OC NL;

RKUUX: JZ LN | 'D' | TK_DXSZ TK_PGEZ RKUUX | TK_SS TK_SS LN TK_AZ;

