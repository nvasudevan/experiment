%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: RKUUX QBP 'K' TK_OC AAA | 'Y' | BCSI TK_SS | 'D' NL TK_ADRF RJ TOYPM | TK_ARJ TK_OC GVTPU TK_LFTP RKUUX;

NL: TOYPM GVTPU TK_YUZOZ;

NI: V TK_DXSZ | 'D' BV | TK_OC RKUUX 'K' BV RJ;

AAA: TK_ADRF TK_PGEZ 'X' | JZ GVTPU JZ TK_ARJ TK_ADRF | RIC LN | V TK_SS GSBBT TK_KPO | TK_DXSZ TK_LFTP TK_AZ;

GVTPU: TK_LFTP TK_ON TK_ISIWH TK_BQEDS NL | TK_DXSZ GSBBT NL | BV QBP TK_DXSZ;

JZ: NI BCSI | TK_YUZOZ;

GSBBT: BY TK_YUZOZ TK_ISIWH TK_YUZOZ | NL TK_SS V;

LN: TK_KPO 'X' NL TK_IMM BY | TK_ADRF TK_IMM RF GVTPU | V QBP 'Y';

RF: RF | TK_YUZOZ | TK_SS TK_PGEZ GSBBT 'Y' TK_KPO;

RIC: TK_LFTP BY 'K' | ;

BV: TK_PGEZ LN TK_ADRF LN 'K' | BCSI TK_DXSZ QBP V | TK_KPO TK_DXSZ 'X' TK_ON | QBP;

RJ: BV TK_ARJ;

V: BY NL BY TK_SS | TK_OC TK_PGEZ TK_PGEZ 'K' TK_BQEDS | TK_PGEZ TK_ADRF TK_OC BV | ;

TOYPM: TK_LFTP TK_IMM GVTPU TK_OC NI | BY NI TK_SS | RJ RKUUX JZ NL;

BCSI: BV TK_OC TOYPM 'Y' RF | TK_IMM AAA TOYPM 'Y' AAA | NL RF | TK_ARJ RF QBP;

QBP: TK_ON 'Y' TK_PGEZ RIC TK_IMM | TK_PGEZ TK_LFTP TK_KPO JZ | 'Y' | TK_ON TK_DXSZ TK_ADRF RKUUX RF | AAA;

BY: TK_YUZOZ TK_AZ TOYPM | TK_DXSZ | NI BY BV TK_KPO TK_ARJ;

RKUUX: 'X' TK_LFTP RF 'X' | TOYPM RIC RKUUX GVTPU TK_BQEDS | TK_BQEDS | TK_PGEZ TK_LFTP GVTPU NI TK_DXSZ | 'X';

