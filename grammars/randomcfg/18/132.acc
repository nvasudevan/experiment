%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: RIC AAA;

NL: BV TK_IMM | TK_KPO 'K' BY BV;

NI: TOYPM NI RIC TK_ADRF V | RKUUX | GVTPU TK_YUZOZ BV TK_ARJ | 'Y' | ;

AAA: 'X' TK_ISIWH | 'X' NL TK_BQEDS RIC;

GVTPU: TK_OC | RF;

JZ: TK_SS 'K' TK_ISIWH TOYPM GVTPU | TK_BQEDS JZ QBP;

GSBBT: NI QBP BCSI RKUUX TK_PGEZ | TK_YUZOZ TK_YUZOZ | TK_OC;

LN: TOYPM TK_ISIWH V LN TK_IMM | TK_ISIWH 'X' NI TK_IMM TK_LFTP | RF BCSI TK_DXSZ BCSI | V TK_IMM NI GSBBT TK_PGEZ;

RF: TK_ADRF QBP NI | 'X' V NL | 'D' QBP TK_ISIWH TK_LFTP;

RIC: RIC TK_ISIWH TK_KPO 'Y' | QBP TK_ARJ | TK_KPO RKUUX NI V TK_AZ;

BV: TK_ISIWH NI TK_DXSZ | TK_ISIWH | TK_PGEZ | BCSI TK_OC TK_IMM | 'K' AAA BY;

RJ: TK_ADRF V BCSI RKUUX;

V: BV TK_ON RF TK_ON TK_OC | TK_ON QBP;

TOYPM: TK_ARJ | ;

BCSI: 'X' 'D' RF TK_DXSZ | RIC GVTPU TK_IMM | BCSI;

QBP: TK_PGEZ TK_PGEZ 'K' JZ | TK_ADRF 'K' TK_ADRF TK_AZ TK_ADRF;

BY: TK_DXSZ GVTPU AAA TK_LFTP NL;

RKUUX: BV TK_DXSZ 'K' LN TOYPM | TK_ON | RIC TOYPM 'D' | RJ TK_KPO BV;

