%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: TK_ADRF | BY BV RKUUX 'K' 'K';

NL: TK_AZ | 'X' | TK_SS BCSI | TK_OC NL RKUUX | NL;

NI: TK_YUZOZ TK_SS TK_ADRF | BY TK_BQEDS GSBBT GVTPU TK_LFTP | TK_AZ TK_LFTP AAA | ;

AAA: TK_KPO RF GVTPU TK_ADRF | GVTPU;

GVTPU: TK_LFTP BY | 'Y' LN TK_ON RIC | RKUUX BCSI;

JZ: NI TK_DXSZ RF AAA | TK_ISIWH NL RIC | RIC GSBBT V;

GSBBT: 'Y' BV 'Y' BCSI AAA | TK_OC TK_SS TK_ON TK_ARJ | BCSI BCSI BCSI | TK_PGEZ NL TK_LFTP 'D' QBP;

LN: TK_BQEDS TK_KPO | TK_SS V | TK_SS TK_ON TK_IMM;

RF: 'X' BCSI | NI NL TK_YUZOZ | JZ TK_ISIWH 'K' TK_ON TK_OC;

RIC: BY JZ GVTPU | ;

BV: TK_ADRF RKUUX TK_SS | TK_BQEDS;

RJ: V GVTPU | AAA RF RJ RIC;

V: 'K' 'X' TK_IMM AAA;

TOYPM: TK_ARJ;

BCSI: RIC;

QBP: TOYPM TK_DXSZ | QBP 'D' RKUUX V RF | TK_IMM TK_ARJ 'X' NL TK_PGEZ;

BY: TK_ON TK_AZ RKUUX TK_BQEDS TK_DXSZ;

RKUUX: 'X' TK_ARJ | GSBBT;

