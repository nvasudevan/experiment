%token TK_SS, TK_IMM, TK_BQEDS, TK_YUZOZ, TK_ON, TK_LFTP, TK_AZ, TK_ARJ, TK_ISIWH, TK_KPO, TK_PGEZ, TK_ADRF, TK_DXSZ, TK_OC;

%nodefault

root: TK_ARJ TK_LFTP | TK_SS NL | BV NL 'K';

NL: RKUUX GSBBT | GVTPU AAA | TK_ADRF TK_ADRF AAA | TK_OC TK_ADRF;

NI: 'X' TK_LFTP | RF V;

AAA: GSBBT RJ TK_IMM TK_ARJ BY;

GVTPU: RKUUX | BCSI | TK_KPO RJ V RKUUX | QBP GSBBT TK_AZ TK_DXSZ;

JZ: TK_IMM TK_LFTP GSBBT GSBBT;

GSBBT: TK_ON;

LN: 'Y' RJ GSBBT 'X' TK_ISIWH | TK_OC GVTPU NL BV;

RF: TK_KPO BV RIC BV TK_PGEZ | TK_BQEDS TK_ON | RKUUX JZ TK_ADRF | ;

RIC: TK_KPO BCSI TK_KPO 'X' | V V TK_ISIWH RKUUX | TK_YUZOZ NI GSBBT | TK_PGEZ TK_SS 'K' BV QBP | LN TK_KPO BV BCSI;

BV: TK_IMM TK_OC TK_LFTP | NL NL 'K' TK_ON;

RJ: BY BY V;

V: TK_DXSZ TK_ADRF NL NL GSBBT | JZ;

TOYPM: TK_OC 'Y' V | TK_BQEDS JZ 'K' V | GSBBT | AAA;

BCSI: BV TK_OC AAA BV | TK_YUZOZ TK_DXSZ TK_PGEZ V 'K' | ;

QBP: NL TK_ARJ GSBBT;

BY: BY BV TK_YUZOZ V | QBP QBP;

RKUUX: TK_OC TK_ARJ GSBBT | RJ TK_PGEZ RF BV LN | BCSI TK_OC GSBBT TK_ADRF;

