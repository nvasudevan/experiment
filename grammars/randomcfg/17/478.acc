%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_IEDA DNFF;

VYJU: TK_OM | SWRI I G VYJU TK_BBZ;

VZKQM: SWRI VYJU TK_PRGF TK_OM | UXESO TK_OM TK_RBS TK_KL | TK_RBS DNFF 'Q' TK_SRWU TK_BBZ | TK_RBS DNFF I SRVI TK_PRGF;

DNFF: MGOV 'H' TK_MVI TK_ERGO 'Q';

UXESO: OGR | TK_KL OGR SWRI 'K' | G C;

SWRI: UXESO 'Y' TK_RBS TK_TGKU | D D SRVI TRL I | 'K' VZKQM TK_TGKU SRVI | TK_BBZ TK_BBZ;

G: OGR | TK_BBZ TK_PRGF D TRL | D TK_SRWU LP 'K' TK_TGKU;

SRVI: TK_IEDA | AHRM | TK_BBZ D TK_KL | D DNFF 'K';

C: TK_RBS TK_ERGO | TK_KL LP TK_MVI VYJU I | VZKQM 'H' TK_OM VZKQM SRVI | ;

D: 'Q' | D DNFF | 'H' TK_BBZ;

OGR: TK_SRWU 'K' | C TK_KL UCKQB;

AHRM: 'Y' D AHRM SWRI TK_BBZ | TK_RBS SRVI UXESO TK_ERGO | OGR AHRM TK_KL TK_MVI | ;

I: MGOV MGOV OGR I | DNFF TK_BBZ TK_BBZ TK_MVI C;

UCKQB: VZKQM 'H' VYJU;

LP: TK_TGKU DNFF 'Q' TK_IEDA TK_DTRWR | VZKQM MGOV TK_SRWU | TRL TK_OM VZKQM TK_HLJ TK_TGKU | 'H';

MGOV: D TRL DNFF | 'W' AHRM LP | I SRVI C TRL DNFF | TK_OM TK_ERGO;

TRL: UCKQB 'H' AHRM | C I 'K' | TK_BBZ | OGR VZKQM;

