%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: 'K' 'Y' TK_DTRWR LP;

VYJU: AHRM SRVI TK_PRGF TK_MVI 'Y';

VZKQM: TK_SRWU TK_MVI TK_BBZ VZKQM TK_SRWU | 'W' TK_HLJ TK_KL;

DNFF: TK_SRWU;

UXESO: TRL 'W' I AHRM VYJU;

SWRI: SRVI VZKQM LP | G TK_SRWU | SWRI OGR TK_OM;

G: TK_SRWU 'W' G TK_TGKU | UCKQB | TK_PRGF;

SRVI: TK_RBS VZKQM 'Q' SRVI 'Y' | TK_RBS TK_SRWU TK_HLJ | DNFF VZKQM OGR TK_DTRWR TRL | 'K' 'W' TK_BBZ I | ;

C: TK_DTRWR 'H' UCKQB | TK_KL SWRI C TK_HLJ | SRVI I TK_PRGF TK_IEDA;

D: 'H' I TK_DTRWR | UXESO UCKQB VYJU VYJU AHRM | AHRM 'H' TK_SRWU I TRL | DNFF;

OGR: I DNFF;

AHRM: LP TK_HLJ | UXESO G TK_ERGO 'K' G | I SWRI VYJU;

I: I UCKQB | I OGR D TK_TGKU | SWRI | TK_RBS LP 'Y';

UCKQB: TK_MVI MGOV LP | ;

LP: 'K' TRL TK_BBZ | C LP;

MGOV: G D TK_ERGO | C TK_SRWU;

TRL: TK_SRWU AHRM TK_SRWU | 'Y' | TK_IEDA TK_MVI TK_OM C TK_ERGO | TK_KL UCKQB 'K' | TK_PRGF 'K';

