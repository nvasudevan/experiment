%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: MGOV TK_TGKU TK_HLJ LP;

VYJU: LP TK_HLJ 'Y' TK_DTRWR;

VZKQM: TK_TGKU TK_BBZ TK_IEDA G 'Y';

DNFF: OGR | C | SRVI TK_HLJ AHRM I I | SRVI | TK_PRGF;

UXESO: SRVI DNFF MGOV | LP I UXESO VZKQM 'Y' | TK_HLJ VZKQM SRVI | G VZKQM TK_MVI TK_SRWU TK_PRGF;

SWRI: VYJU TK_MVI | I LP TK_ERGO TK_PRGF TK_BBZ | 'Y' SRVI | TK_RBS TK_TGKU AHRM UCKQB | G;

G: UXESO TK_SRWU 'W' UCKQB | AHRM MGOV TK_RBS MGOV TK_OM | TK_BBZ 'H' TK_OM;

SRVI: TK_KL AHRM UCKQB TK_TGKU;

C: TK_TGKU TK_MVI I | D | 'W' LP;

D: OGR TK_BBZ TK_PRGF | LP 'W' | SRVI | TK_TGKU C TK_OM 'W';

OGR: TK_KL TK_KL UXESO | 'Y' SWRI 'W' TK_DTRWR C | ;

AHRM: 'Y' TK_BBZ TK_MVI SWRI TK_BBZ | I 'H' TK_MVI | SWRI AHRM TK_OM | SRVI;

I: TK_PRGF LP | TK_OM;

UCKQB: SWRI TK_OM DNFF G UXESO | 'W' | D SWRI TK_MVI DNFF MGOV | ;

LP: SRVI | TK_DTRWR 'K' SRVI | TK_TGKU I TK_ERGO TK_DTRWR | 'Y' TK_TGKU TK_SRWU OGR C | TK_BBZ SRVI G;

MGOV: TK_ERGO | LP DNFF | DNFF TK_HLJ AHRM | TK_RBS | LP;

TRL: 'W' TK_RBS;

