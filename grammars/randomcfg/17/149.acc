%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: I;

VYJU: C UXESO VYJU | TK_PRGF | TRL UXESO TK_OM TK_PRGF;

VZKQM: 'W' AHRM TK_SRWU UCKQB TK_KL;

DNFF: UCKQB SWRI TK_MVI G TRL | 'W' D | SRVI LP | UCKQB | D;

UXESO: I | TK_PRGF | 'H' VYJU 'K' | TK_PRGF 'W' DNFF;

SWRI: C | ;

G: TK_SRWU | D MGOV UXESO;

SRVI: AHRM G OGR TK_MVI;

C: 'K' D | SWRI 'Y' | 'Y' I D 'Q' | 'Y' 'H' TK_PRGF 'W';

D: TK_MVI I | SWRI LP SRVI TK_RBS | 'Y' 'Q' MGOV | VYJU C | TK_KL TK_MVI TK_PRGF;

OGR: C TK_MVI;

AHRM: TK_MVI TK_TGKU D | 'W' SRVI UXESO | MGOV UXESO | AHRM TK_IEDA VZKQM TK_RBS UXESO | TK_RBS TK_RBS UCKQB;

I: DNFF VYJU TK_SRWU G SRVI | 'Y' TK_DTRWR TK_SRWU TK_SRWU TK_RBS | TK_MVI TK_BBZ | ;

UCKQB: TRL | TK_OM SRVI | TK_HLJ TK_BBZ AHRM | TK_TGKU;

LP: I SWRI TK_DTRWR | TK_ERGO LP;

MGOV: UXESO TK_BBZ TK_DTRWR LP VZKQM | TK_TGKU TK_MVI | TK_IEDA | UXESO UCKQB TK_DTRWR TK_KL 'Q';

TRL: D 'Q' TK_HLJ C TK_DTRWR | TK_TGKU | UXESO 'Y' TK_OM TK_SRWU LP | TK_PRGF;

