%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: I VYJU | TK_KL | D TK_PRGF | MGOV TK_MVI I TRL;

VYJU: OGR TK_TGKU | TRL UXESO 'Q' | AHRM 'Q' TK_ERGO TK_DTRWR G | TK_KL D TK_HLJ TK_PRGF | TK_OM AHRM;

VZKQM: TK_DTRWR | TK_HLJ | TK_KL TK_KL | C;

DNFF: TK_DTRWR 'Q' TK_HLJ | SRVI TRL TK_OM | 'Q' | TRL TK_KL;

UXESO: TK_IEDA VZKQM | TK_MVI TK_SRWU AHRM D | TK_KL 'H' 'Q' AHRM TK_RBS | VZKQM VZKQM;

SWRI: I TK_OM MGOV 'Q' TK_BBZ | 'Q' TK_SRWU TK_OM | UXESO TK_RBS TK_KL DNFF TK_HLJ | C 'Q' TK_TGKU;

G: 'H' | TK_KL | SWRI G UXESO TK_BBZ | AHRM TK_PRGF;

SRVI: AHRM | TK_DTRWR TK_MVI | ;

C: TK_KL AHRM TRL D;

D: LP G TRL | TK_HLJ TK_BBZ TK_KL AHRM | TK_PRGF | 'K' TK_SRWU G SRVI | ;

OGR: TK_PRGF 'K' | TK_IEDA 'Y' | VZKQM UXESO;

AHRM: AHRM TK_OM TK_BBZ TK_ERGO | 'Y';

I: TK_MVI | 'W' UCKQB TK_MVI OGR MGOV | TK_OM SRVI LP TK_KL | TK_DTRWR UXESO TK_PRGF;

UCKQB: TK_SRWU LP MGOV TK_DTRWR | SRVI | TK_RBS UCKQB TK_IEDA 'Q' | I TK_OM 'W' | TRL;

LP: TK_BBZ VYJU TK_SRWU VZKQM | D D;

MGOV: D I UCKQB D | TK_TGKU MGOV;

TRL: 'Y' SWRI;

