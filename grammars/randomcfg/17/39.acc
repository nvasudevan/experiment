%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: UXESO OGR TK_RBS | MGOV SRVI TK_SRWU TK_TGKU;

VYJU: TK_KL TK_KL | TK_PRGF | TK_SRWU 'Y' VZKQM TRL C | 'H' MGOV 'H' | ;

VZKQM: TK_KL LP 'Y' TK_HLJ 'Q' | TK_RBS TK_TGKU SWRI;

DNFF: TK_RBS G 'K' 'K' | TK_DTRWR VZKQM | C VZKQM DNFF TK_IEDA | VYJU TK_OM TK_DTRWR C 'W';

UXESO: TK_MVI C | C 'K' | DNFF VZKQM | I | TK_TGKU 'Y' TK_KL;

SWRI: SWRI TK_OM TK_HLJ | TK_ERGO TK_TGKU DNFF;

G: TK_BBZ UCKQB TK_BBZ TK_HLJ G | TK_PRGF TK_PRGF | D VYJU | TK_RBS TK_DTRWR MGOV VZKQM;

SRVI: TK_SRWU TK_BBZ | ;

C: LP 'W' AHRM 'Q' | C DNFF TK_DTRWR TK_RBS;

D: 'Q' TK_DTRWR | 'W' TK_IEDA TK_MVI UCKQB TK_KL | 'Q' TK_IEDA TK_SRWU | TK_DTRWR UXESO;

OGR: 'K' DNFF 'Y' TK_OM 'K' | TK_RBS VZKQM TK_IEDA | TK_OM | OGR SWRI MGOV TK_SRWU;

AHRM: LP SWRI TK_MVI | I TK_BBZ SRVI DNFF 'H' | C;

I: AHRM TK_BBZ TK_MVI | I OGR D | 'H' 'K' | VZKQM 'K' UXESO TK_MVI UCKQB;

UCKQB: TK_OM | TK_IEDA DNFF | 'Y' AHRM UCKQB | 'W' TK_MVI TK_MVI DNFF SWRI | TK_SRWU TK_SRWU 'Y' TK_HLJ;

LP: TK_ERGO G SWRI I VYJU;

MGOV: LP SRVI DNFF | I 'K' | TK_OM TK_PRGF TK_BBZ VYJU TK_MVI;

TRL: VYJU | TK_HLJ TK_MVI LP VZKQM | TK_IEDA VZKQM;

