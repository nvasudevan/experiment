%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: MGOV TK_KL OGR;

VYJU: G TK_HLJ 'Y' SWRI;

VZKQM: TK_RBS C TK_RBS C | LP VYJU;

DNFF: D SWRI TRL G TK_BBZ | TK_BBZ 'Q' 'Q' 'W';

UXESO: TK_ERGO TK_ERGO 'Q' TK_KL TK_TGKU | TK_IEDA OGR TK_OM | DNFF | TK_KL TK_PRGF SRVI TK_PRGF | TK_KL VZKQM 'W' MGOV;

SWRI: TK_KL DNFF | TK_OM G | SRVI 'W' I I | TRL AHRM UXESO 'Y';

G: TRL UCKQB TK_RBS | TK_RBS TK_HLJ C TK_OM | AHRM 'K' OGR;

SRVI: TRL SWRI TK_IEDA TK_OM | VZKQM | TK_TGKU MGOV C 'W' TK_OM | LP TK_OM 'W';

C: AHRM | 'W' 'K' SWRI TK_BBZ 'K';

D: SWRI TK_PRGF TK_DTRWR TK_RBS;

OGR: OGR UXESO TK_ERGO | 'H';

AHRM: TRL | TK_TGKU TK_BBZ TK_ERGO MGOV | VZKQM 'K' UXESO 'H' OGR | SWRI SRVI MGOV | C TK_OM;

I: TK_KL DNFF TK_SRWU | TK_TGKU TK_KL | TK_RBS VZKQM SRVI TRL | AHRM | ;

UCKQB: 'W' | TK_RBS 'W' VYJU;

LP: TK_ERGO TK_MVI TK_BBZ TK_RBS 'Y' | LP 'H' D | VYJU UXESO UXESO SRVI 'H' | UCKQB TK_TGKU UCKQB TK_IEDA SWRI;

MGOV: G TK_BBZ OGR | ;

TRL: TK_ERGO | VZKQM C TK_IEDA | 'K' 'H' | TK_ERGO AHRM;

