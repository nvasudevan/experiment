%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: 'Y' TK_OM;

VYJU: LP VZKQM 'W' TK_MVI UXESO | TK_ERGO DNFF | 'W' TK_DTRWR OGR TK_DTRWR TK_KL | ;

VZKQM: TK_RBS TK_DTRWR TK_ERGO VZKQM | TK_ERGO TK_SRWU SWRI | UCKQB 'W' UXESO | OGR VYJU 'H' TK_OM TK_OM;

DNFF: AHRM DNFF TK_TGKU I | TK_PRGF;

UXESO: TK_HLJ TK_BBZ;

SWRI: 'K' TK_OM G TK_SRWU LP | TK_OM DNFF C;

G: AHRM UCKQB SWRI D | 'K' TK_SRWU | OGR 'K' TK_SRWU SWRI | TK_DTRWR;

SRVI: VYJU TK_SRWU | TK_OM | OGR;

C: TK_OM;

D: TK_IEDA TK_RBS TK_KL;

OGR: TK_KL 'K' TK_DTRWR DNFF | TK_PRGF DNFF DNFF 'Q' DNFF | C D SRVI TK_BBZ TK_PRGF;

AHRM: TK_SRWU;

I: DNFF 'W' LP | LP 'Y' TK_RBS | SRVI TK_KL TK_HLJ TRL AHRM | UXESO 'H' TK_PRGF 'H' 'Y' | TRL SRVI OGR VYJU SRVI;

UCKQB: TK_ERGO;

LP: 'K' | TRL 'K' | SWRI TK_MVI SWRI | TK_ERGO G TK_KL TK_IEDA 'K';

MGOV: I D UXESO VYJU | SWRI TK_HLJ UXESO I;

TRL: I SWRI G TK_PRGF;

