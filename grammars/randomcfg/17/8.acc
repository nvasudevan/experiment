%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: SRVI 'H' UCKQB MGOV | LP 'H' UCKQB | OGR | TRL G UCKQB;

VYJU: 'Y' SWRI TK_ERGO TK_OM VZKQM | SRVI TK_OM | SWRI TK_ERGO TK_BBZ;

VZKQM: TK_IEDA MGOV I LP DNFF;

DNFF: TK_RBS 'W' TRL I | OGR 'K' AHRM D | G MGOV TK_PRGF TRL;

UXESO: TK_PRGF DNFF | TK_OM MGOV AHRM TK_KL TK_MVI | TK_PRGF MGOV 'Q' 'H';

SWRI: VYJU SRVI OGR C 'H' | ;

G: UXESO I | 'H' LP;

SRVI: TK_IEDA TK_IEDA D TK_TGKU | VYJU | MGOV TK_PRGF OGR | 'W' TK_DTRWR DNFF TK_IEDA TK_PRGF | TK_HLJ TK_OM TK_BBZ;

C: MGOV | LP OGR LP OGR;

D: MGOV TK_OM TK_ERGO TK_IEDA | I AHRM TK_SRWU VZKQM D | TK_OM | TK_RBS | ;

OGR: TK_DTRWR TK_PRGF UXESO;

AHRM: C TK_HLJ G | G SWRI TK_PRGF TRL LP | TK_DTRWR;

I: G 'H' LP;

UCKQB: OGR TK_IEDA TRL 'Q' 'Q';

LP: 'Q' | TK_KL D TK_PRGF SRVI UCKQB | 'K' TK_HLJ | 'H';

MGOV: TK_DTRWR TK_TGKU 'H' TK_RBS | VYJU TK_SRWU TK_BBZ TK_SRWU | TK_BBZ D TK_DTRWR TRL MGOV | UCKQB TK_KL | LP TK_ERGO VZKQM;

TRL: SWRI 'K' | TK_KL 'H' 'W' UXESO | TRL TK_ERGO DNFF 'Y' | TK_HLJ 'Y' TK_ERGO AHRM DNFF;

