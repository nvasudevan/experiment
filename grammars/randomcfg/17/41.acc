%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: I SWRI TK_OM C SWRI | SWRI TK_MVI | VZKQM TK_BBZ;

VYJU: TK_BBZ TK_RBS TK_IEDA | TK_PRGF TK_DTRWR | D SWRI | TK_PRGF TK_MVI TK_BBZ VZKQM | TK_HLJ LP VZKQM AHRM;

VZKQM: TK_ERGO | TK_RBS | TRL C VZKQM SRVI | TK_TGKU TRL OGR TK_RBS AHRM;

DNFF: MGOV TK_PRGF TK_HLJ | C | TK_HLJ TK_OM 'H';

UXESO: UCKQB TK_DTRWR 'Q' 'Y' 'W' | AHRM TRL | VZKQM G TK_MVI UXESO | TK_HLJ;

SWRI: MGOV TK_HLJ UXESO VZKQM VYJU | 'Y';

G: TK_MVI MGOV TRL 'K' | TK_BBZ LP SRVI | TK_OM C;

SRVI: I 'W' 'Q' | TK_ERGO DNFF DNFF 'W' TK_SRWU | ;

C: LP LP MGOV TK_TGKU | 'Y' VYJU MGOV G;

D: OGR TK_RBS | VYJU MGOV | VZKQM TK_HLJ D;

OGR: 'H' 'Y' UXESO LP | 'K' | UCKQB 'W';

AHRM: DNFF | UXESO DNFF TK_HLJ | 'Y' TK_PRGF TK_TGKU | TK_ERGO | ;

I: G MGOV SWRI 'K' VYJU | TK_BBZ C TK_TGKU TK_RBS TK_KL | 'H' TK_KL;

UCKQB: UXESO TK_OM D | TK_OM TK_KL DNFF;

LP: 'H';

MGOV: SWRI TK_RBS TK_OM MGOV 'Q' | I | TK_MVI TK_OM 'H' TRL;

TRL: TK_SRWU SRVI | DNFF | D UCKQB VZKQM D TK_BBZ | SWRI;

