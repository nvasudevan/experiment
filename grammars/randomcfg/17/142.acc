MGOV: AHRM TK_DTRWR 'W' TRL | G | TK_DTRWR TK_PRGF 'H' 'K' UXESO | I MGOV | TK_OM G 'K' 'Q';

TRL: TK_BBZ TK_DTRWR G I TK_DTRWR;

