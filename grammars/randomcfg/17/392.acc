%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: SRVI I TK_HLJ;

VYJU: TRL TK_KL DNFF | TK_DTRWR | SWRI | 'Q' 'H' TK_MVI SRVI TK_PRGF | UXESO D G;

VZKQM: OGR TK_KL SRVI UXESO OGR | DNFF 'W' C TK_HLJ | 'K' 'K' OGR TK_MVI | TK_SRWU UXESO VZKQM 'W' SRVI | TK_RBS TK_OM D 'K' AHRM;

DNFF: C | TK_BBZ TRL | UCKQB AHRM TK_IEDA | ;

UXESO: DNFF | D SWRI UXESO;

SWRI: D VYJU TK_TGKU | 'W' | C D;

G: SWRI;

SRVI: SWRI TK_KL C TRL TK_HLJ | TK_DTRWR TK_BBZ;

C: 'Q' TK_IEDA VYJU C TK_PRGF | SWRI;

D: TK_PRGF TK_KL | TK_SRWU OGR | D D UCKQB TK_PRGF | UXESO | TK_PRGF D D UXESO;

OGR: 'Q' TK_TGKU | AHRM | 'Q' I 'W' TK_BBZ;

AHRM: VYJU LP TK_RBS 'Q' TK_KL | VZKQM TK_ERGO TK_RBS MGOV | TK_HLJ | G TK_TGKU TK_PRGF TRL 'W' | TK_IEDA TK_OM TK_MVI SRVI 'H';

I: TK_ERGO UCKQB I MGOV TK_RBS | TK_OM LP TK_TGKU UXESO TK_DTRWR | TK_ERGO UCKQB TK_HLJ | SRVI TK_HLJ TK_HLJ | ;

UCKQB: 'Y' LP | AHRM I TK_RBS;

LP: TK_BBZ UXESO C 'K';

MGOV: TK_RBS;

TRL: TK_RBS 'H' | 'Q' VYJU SRVI | LP SWRI SRVI TK_DTRWR TK_IEDA | TK_IEDA TK_RBS;

