%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: OGR | I TRL TK_HLJ 'W' 'Q' | 'Q' VZKQM | TK_OM D TK_TGKU;

VYJU: VZKQM | OGR 'Y' 'W' VYJU;

VZKQM: TRL | MGOV SRVI TRL UCKQB TK_BBZ | 'Y' SRVI AHRM I TK_MVI | 'H' TRL 'H' TK_SRWU;

DNFF: VYJU TK_OM | TK_SRWU | I DNFF;

UXESO: D TK_MVI DNFF VYJU | TK_ERGO G I 'Y' TK_MVI | ;

SWRI: TK_OM 'K' VZKQM | 'K' TK_MVI TRL;

G: 'Q' TK_KL TK_PRGF D | TK_ERGO UXESO TK_IEDA TK_OM | TK_MVI UCKQB TK_TGKU | 'Y' TK_RBS 'Q' TK_HLJ D;

SRVI: MGOV DNFF D UXESO | TK_DTRWR 'H' | I TK_DTRWR TK_SRWU;

C: TK_RBS LP TK_OM TK_IEDA | MGOV | TK_SRWU | TK_HLJ TK_HLJ | ;

D: TK_BBZ 'K' I SRVI 'K' | D MGOV | TK_ERGO SWRI;

OGR: VZKQM AHRM | VZKQM TK_KL | DNFF UXESO TK_KL;

AHRM: UCKQB 'Q' | MGOV VYJU UCKQB D OGR | SWRI C D | 'W' TK_PRGF VYJU;

I: TK_PRGF UCKQB TK_PRGF TK_BBZ | LP | UCKQB TRL VYJU G | TK_ERGO TK_IEDA TK_RBS LP | C D TK_MVI;

UCKQB: TK_ERGO TK_BBZ TK_TGKU | TK_BBZ 'Y' | 'Y' 'W' TK_PRGF TK_KL | G TK_ERGO MGOV MGOV;

LP: SWRI SWRI;

MGOV: 'H' 'Y' TK_BBZ AHRM | TK_IEDA TK_RBS TK_DTRWR UCKQB D | DNFF VZKQM DNFF 'Q' AHRM | TK_IEDA D TK_ERGO;

TRL: TK_SRWU TK_DTRWR | 'W' TK_HLJ AHRM VZKQM TK_ERGO | TK_OM UXESO TK_PRGF VYJU;

