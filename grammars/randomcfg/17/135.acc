%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: G OGR UCKQB C TK_OM;

VYJU: TK_TGKU SWRI MGOV | TK_IEDA;

VZKQM: AHRM | UXESO 'H' 'Q' I SWRI | TK_PRGF | VZKQM TK_ERGO;

DNFF: TK_DTRWR UCKQB G UXESO C | TK_TGKU TK_DTRWR | VZKQM VZKQM 'H';

UXESO: TK_SRWU TK_KL I TK_IEDA TK_DTRWR | D TK_HLJ | TK_TGKU 'Y' VYJU;

SWRI: TK_DTRWR | DNFF TK_TGKU | I 'Q' TK_RBS TK_PRGF SWRI;

G: 'Q';

SRVI: C | AHRM TK_IEDA;

C: TK_TGKU TK_IEDA 'Q' | TK_DTRWR 'K' OGR TRL TK_OM | 'K' TK_KL TK_SRWU MGOV 'Q';

D: SRVI | TRL 'H' DNFF | G MGOV MGOV | 'H' TK_PRGF VYJU;

OGR: VYJU LP | 'W' TK_HLJ 'W' TK_PRGF | LP OGR | G SRVI;

AHRM: TK_IEDA | TK_TGKU 'Y';

I: 'W' MGOV | ;

UCKQB: SWRI AHRM | 'Q' 'H' | TK_PRGF TK_TGKU 'W' 'H' TK_HLJ | TK_PRGF 'H' TK_MVI TK_OM TK_KL | VYJU MGOV TK_RBS C 'K';

LP: TK_HLJ TK_IEDA D G 'Q' | TK_BBZ TK_KL TK_SRWU TK_PRGF | 'W' TK_ERGO UCKQB | TK_RBS 'K' UCKQB VZKQM | ;

MGOV: SRVI 'H' MGOV VYJU | 'Q' | LP TK_DTRWR UCKQB;

TRL: D AHRM | SWRI TK_KL TK_KL G VYJU;

