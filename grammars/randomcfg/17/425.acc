%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_TGKU TK_MVI TK_KL;

VYJU: 'Q' TK_RBS TK_IEDA D I | TK_RBS OGR G | D VYJU | TK_SRWU 'Q' TK_TGKU;

VZKQM: LP VZKQM 'W' 'K' | ;

DNFF: 'K' VYJU TK_SRWU | TRL UCKQB | C TK_RBS TK_PRGF 'Q' | C I TK_KL TK_TGKU | OGR OGR;

UXESO: TK_PRGF AHRM | 'Q' TK_PRGF | TRL TK_HLJ D DNFF;

SWRI: DNFF VYJU | TK_HLJ TK_HLJ | MGOV VZKQM TK_RBS TK_PRGF | TK_DTRWR 'Y' VYJU TRL 'W' | TK_DTRWR 'Y' DNFF;

G: TK_KL TK_PRGF | VZKQM | DNFF TK_HLJ TRL UCKQB UXESO;

SRVI: SRVI VZKQM | UCKQB;

C: TK_RBS TRL TK_TGKU I | C TK_MVI | ;

D: TK_DTRWR;

OGR: 'Y' 'W' LP D TK_TGKU;

AHRM: TK_KL TK_HLJ TK_RBS TK_RBS | UCKQB 'K' TK_RBS TK_SRWU;

I: AHRM TK_SRWU 'Q' | OGR 'W' SRVI 'K' TK_IEDA | 'Y' | TK_OM | TK_ERGO;

UCKQB: AHRM TK_OM 'H' | 'Q' LP TK_MVI UXESO | TK_ERGO TK_PRGF SRVI D DNFF;

LP: TK_MVI C TK_ERGO TK_DTRWR TK_BBZ;

MGOV: UXESO UXESO TK_OM SWRI VYJU | TK_HLJ UXESO VYJU;

TRL: I VYJU 'K' TK_RBS | 'H' TK_RBS | TK_MVI 'H' TK_IEDA | I LP TK_TGKU TK_DTRWR TK_ERGO | TK_DTRWR 'W' OGR 'Y';

