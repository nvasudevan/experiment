%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_PRGF 'W' C TK_TGKU | 'Q' 'Y' I | TK_RBS 'Q' AHRM UXESO;

VYJU: TK_ERGO TK_DTRWR D VZKQM;

VZKQM: SRVI 'Q' TK_HLJ TK_BBZ;

DNFF: SRVI C TK_RBS D | 'K' TK_DTRWR SRVI TK_MVI AHRM | SRVI 'H' TK_ERGO | 'K' | 'K' TK_SRWU SWRI;

UXESO: TK_RBS TK_MVI C C | TK_HLJ 'Y' OGR | C | UXESO 'H';

SWRI: 'W' AHRM C;

G: LP | TK_SRWU | 'K' TK_BBZ VZKQM MGOV SRVI;

SRVI: LP TK_BBZ | DNFF D 'Q' | 'Y' MGOV 'Y' | OGR TK_OM TK_RBS C C | TK_ERGO 'W' MGOV;

C: TRL LP VYJU 'K' 'W' | TK_OM | TK_TGKU AHRM TK_IEDA | TRL DNFF LP 'Q' | SWRI;

D: DNFF VYJU | TK_RBS | UCKQB AHRM;

OGR: VYJU | UXESO 'K' | 'K' TK_ERGO | 'W' TK_MVI | SRVI UXESO VYJU UCKQB TK_DTRWR;

AHRM: 'K' DNFF | SRVI TK_KL TK_OM | ;

I: TK_SRWU 'K' 'Q' | TK_IEDA 'K' | TK_SRWU G TK_PRGF TK_HLJ 'Y' | SWRI | TK_RBS MGOV TK_BBZ SWRI;

UCKQB: DNFF VYJU VYJU OGR DNFF | DNFF | MGOV LP TK_KL | ;

LP: C TK_TGKU TK_MVI TK_PRGF | D | VYJU VZKQM | TK_PRGF;

MGOV: TK_RBS TK_HLJ 'Y' TK_BBZ AHRM | SWRI VZKQM G TK_PRGF | TK_KL D UXESO TK_HLJ | TK_SRWU 'W' TK_RBS G;

TRL: TK_DTRWR TK_KL | TK_DTRWR | TRL;

