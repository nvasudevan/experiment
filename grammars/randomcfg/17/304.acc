%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: MGOV TK_ERGO SWRI 'H' | TRL TK_ERGO;

VYJU: TK_TGKU 'K' TK_MVI 'Q' | TK_HLJ SRVI DNFF | TK_SRWU MGOV TK_MVI;

VZKQM: OGR TK_OM LP TK_RBS UCKQB | TK_KL TK_HLJ | ;

DNFF: TK_ERGO 'W' 'K' | TK_SRWU TK_KL | UCKQB TK_DTRWR TK_TGKU TK_RBS 'Q';

UXESO: I I | TK_KL 'K' TK_TGKU UXESO | VYJU;

SWRI: 'W' TK_IEDA 'Q' | D TK_HLJ TK_HLJ UCKQB TK_SRWU | TRL 'H' TK_DTRWR | TK_OM TK_IEDA 'Q' | TK_KL;

G: VYJU TK_OM TK_IEDA OGR | I 'K' C TK_IEDA | 'Y' D 'Q' VYJU LP | 'Q' G UXESO TK_OM;

SRVI: UXESO TK_SRWU UCKQB TK_DTRWR;

C: TK_HLJ TK_SRWU LP TK_ERGO | TK_RBS TK_SRWU VZKQM TK_MVI SRVI | D;

D: 'Q' TK_ERGO OGR TK_PRGF D | 'K' TK_OM 'Q';

OGR: OGR | 'Y' I | VZKQM SRVI 'K' 'K' 'H' | TK_TGKU;

AHRM: AHRM SWRI | TK_KL TK_IEDA TK_MVI | G | UXESO 'W' TK_TGKU SRVI | TK_DTRWR;

I: VYJU TK_MVI D TK_SRWU 'W' | TK_ERGO TRL VYJU D TK_RBS | TRL TRL | TK_PRGF | 'Q' SWRI I VYJU;

UCKQB: 'W' AHRM | ;

LP: TK_HLJ | VZKQM TK_HLJ TK_ERGO TK_ERGO | TK_ERGO D D;

MGOV: LP I MGOV TK_RBS G | TK_MVI | MGOV 'W' UXESO DNFF | 'Q' 'K' TK_KL TK_KL TRL;

TRL: MGOV | TK_RBS TK_SRWU | DNFF AHRM TK_DTRWR TK_ERGO | TK_SRWU | TK_RBS TK_MVI TK_ERGO;

