%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: AHRM I TK_PRGF | VZKQM TK_PRGF VYJU;

VYJU: OGR TK_MVI TK_DTRWR | VYJU 'H' TRL SWRI OGR;

VZKQM: TK_KL | 'Y' DNFF | SWRI;

DNFF: SRVI G UCKQB | TK_PRGF SWRI VYJU | 'W' 'K' TK_RBS 'Y';

UXESO: VZKQM 'Y';

SWRI: TRL TK_KL TK_BBZ TK_BBZ D | OGR;

G: OGR TK_DTRWR D TK_RBS | TK_KL C I D;

SRVI: VZKQM DNFF TK_RBS | LP D UCKQB 'W' | TK_HLJ TK_ERGO | TK_OM TK_ERGO 'Y' DNFF | TK_SRWU AHRM;

C: TK_IEDA TK_TGKU TK_PRGF DNFF | D TK_SRWU TK_PRGF D;

D: D 'K' TK_HLJ | TK_BBZ TK_DTRWR TRL TRL | TK_DTRWR TK_IEDA C C MGOV | TK_PRGF C TK_PRGF 'K' TK_TGKU | TK_RBS DNFF UXESO;

OGR: UXESO 'Q' 'W' TK_MVI | TK_PRGF UXESO C UXESO LP | G TK_TGKU | VZKQM SWRI SWRI 'Q' | TK_SRWU I TK_BBZ 'Q';

AHRM: 'Q' LP 'Q';

I: TK_DTRWR I UCKQB | VYJU | TK_OM UXESO;

UCKQB: VZKQM | LP VYJU UXESO TK_RBS;

LP: TRL UCKQB TRL TK_HLJ | OGR TK_TGKU TK_OM TK_MVI | D;

MGOV: 'W' VZKQM | OGR TK_SRWU TK_KL I G | G TK_BBZ 'Y' AHRM TK_DTRWR | TK_RBS TK_ERGO | ;

TRL: I | TK_OM | TK_DTRWR TK_SRWU TK_PRGF TRL | 'Q' MGOV | ;

