%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: MGOV 'H' | TK_IEDA TK_ERGO TK_PRGF;

VYJU: 'Q' D TK_SRWU TK_PRGF | TK_TGKU TK_KL | TK_TGKU UCKQB TK_ERGO AHRM 'Q';

VZKQM: TK_BBZ;

DNFF: TK_HLJ TK_PRGF TK_DTRWR TRL TK_KL | G UCKQB C TK_OM TK_OM | TK_BBZ;

UXESO: AHRM G | DNFF AHRM | UXESO TRL | ;

SWRI: 'Y' LP | TRL TK_KL VYJU D | 'Q';

G: TK_BBZ | TK_KL | TK_TGKU | 'Q' TK_SRWU SWRI TK_OM 'Y';

SRVI: AHRM SWRI;

C: UCKQB TK_ERGO | 'Y' | OGR TK_TGKU TK_SRWU | TK_OM VYJU TK_BBZ;

D: 'W' TK_DTRWR SRVI OGR | TK_HLJ | TK_KL MGOV | TK_KL VYJU | TK_SRWU;

OGR: VYJU C DNFF TK_SRWU | SWRI TK_OM 'Q' C | UXESO VZKQM | D TK_SRWU VZKQM | 'Q' OGR 'H' TK_PRGF;

AHRM: 'Q' G LP;

I: TK_MVI D TK_MVI;

UCKQB: TK_RBS TRL | TK_IEDA MGOV | 'W' 'Y' TK_TGKU G TK_SRWU | G AHRM;

LP: TK_HLJ 'H' UCKQB TK_RBS | TK_DTRWR TK_IEDA TK_DTRWR TK_MVI;

MGOV: TRL 'Q' VZKQM | C TK_SRWU UXESO 'Y' TK_ERGO | TK_DTRWR TK_ERGO UXESO MGOV UXESO | ;

TRL: UXESO TK_KL | I LP TK_PRGF | I UCKQB 'Q' TRL | I;

