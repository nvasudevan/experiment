%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: G | MGOV SWRI | MGOV LP VYJU;

VYJU: AHRM TRL TK_TGKU TK_SRWU;

VZKQM: TK_MVI AHRM TK_SRWU TK_SRWU;

DNFF: AHRM SWRI UCKQB;

UXESO: TK_BBZ SWRI | TK_RBS | G TK_PRGF | MGOV 'W' UXESO TK_TGKU | LP DNFF;

SWRI: TK_PRGF TK_OM TK_OM | TK_DTRWR TK_BBZ TK_DTRWR;

G: TK_RBS | VYJU TK_ERGO TK_IEDA | TRL | TK_BBZ | 'W' AHRM UXESO AHRM TK_DTRWR;

SRVI: 'Q' TK_ERGO;

C: C 'W' TK_OM | TK_IEDA | G TK_IEDA I 'Y' TK_SRWU | TK_SRWU LP TK_TGKU | OGR MGOV;

D: TK_OM TK_TGKU TK_OM OGR | TK_OM 'W' TK_DTRWR | C 'Y' TK_PRGF OGR 'Y' | AHRM LP | ;

OGR: DNFF TK_KL TK_OM TK_TGKU 'Q' | ;

AHRM: DNFF | C 'K' 'K' | TK_IEDA TRL D | TK_TGKU C 'Q' VYJU UXESO | 'W';

I: TK_PRGF SRVI | TK_TGKU UXESO | SRVI TK_MVI UCKQB | TK_IEDA 'Q';

UCKQB: D I TK_ERGO UXESO D | D | TK_IEDA;

LP: LP TK_ERGO G TK_BBZ | I | UXESO I 'K' 'K' LP | D;

MGOV: 'H' | C TK_BBZ TK_OM TK_OM SRVI | TK_SRWU TK_HLJ TK_KL TK_IEDA | UXESO UXESO TK_TGKU TK_DTRWR;

TRL: UXESO C | OGR 'Y' TK_HLJ | MGOV;

