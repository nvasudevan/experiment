%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: OGR TK_DTRWR TK_SRWU VYJU;

VYJU: UCKQB UXESO | UXESO | C | MGOV TK_OM UCKQB | TK_DTRWR I;

VZKQM: UXESO TK_OM TK_BBZ UCKQB | D UCKQB | VYJU TK_ERGO 'K';

DNFF: TK_TGKU C | LP TK_RBS DNFF;

UXESO: TK_BBZ TK_SRWU 'Q' | UCKQB | TK_DTRWR D DNFF | 'Q' G AHRM TK_KL;

SWRI: D 'K' SWRI TK_OM | D TK_IEDA | TK_DTRWR TK_PRGF | C;

G: TK_PRGF 'H' SRVI C | TK_PRGF | TK_ERGO TK_IEDA TK_IEDA TRL | VZKQM TK_KL LP | TK_BBZ TK_TGKU D;

SRVI: TK_KL SRVI SRVI | C UXESO D VYJU TK_KL | VZKQM SRVI TK_IEDA | VYJU C | MGOV;

C: SRVI TK_DTRWR LP TK_PRGF | OGR TK_ERGO TK_MVI TK_SRWU 'K' | TRL;

D: LP I OGR | UCKQB UCKQB TK_RBS TK_KL | TK_HLJ TK_ERGO TK_MVI | TK_OM OGR TK_ERGO C D;

OGR: 'W' 'Y' DNFF 'H' TK_SRWU | 'H' C | ;

AHRM: 'Q' G G UXESO 'K';

I: TK_BBZ VZKQM I | AHRM 'Q';

UCKQB: TK_KL TK_ERGO 'H' | TK_IEDA SWRI UXESO I | TK_HLJ TK_TGKU DNFF D;

LP: OGR OGR | DNFF TK_IEDA TK_OM LP | 'H' 'K' 'W' AHRM SRVI | ;

MGOV: TK_IEDA TK_HLJ I 'Y' | TK_DTRWR TK_RBS SRVI TK_ERGO | MGOV SWRI VZKQM VYJU TK_MVI | TK_DTRWR LP 'K';

TRL: 'Y' TK_PRGF | I TK_BBZ 'Y' AHRM | VYJU UXESO VYJU | AHRM;

