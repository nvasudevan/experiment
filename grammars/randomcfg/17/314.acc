%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: OGR C TK_TGKU | 'H' TK_BBZ TK_RBS TK_RBS | TK_HLJ | TK_MVI TK_SRWU TK_OM DNFF | TK_OM VYJU;

VYJU: 'Q' C TK_MVI D 'H';

VZKQM: UCKQB TK_PRGF 'W' | VZKQM AHRM TK_IEDA TK_MVI D | TK_PRGF I TK_ERGO 'K' | 'Q' MGOV SRVI VZKQM TK_PRGF;

DNFF: TK_MVI TK_DTRWR TK_DTRWR | VZKQM I TK_ERGO | C TK_DTRWR | SRVI | TK_OM TK_SRWU;

UXESO: TK_HLJ TK_IEDA | TK_SRWU TRL VZKQM;

SWRI: TRL | SRVI TK_KL | TK_SRWU UXESO TK_MVI | ;

G: 'Q' | TK_RBS 'W' TK_ERGO TK_TGKU | G LP UCKQB TK_BBZ | TK_DTRWR TK_SRWU TK_PRGF | ;

SRVI: VZKQM | SWRI TK_HLJ VYJU | SRVI TK_OM TK_MVI TK_RBS;

C: LP | TK_IEDA LP TK_TGKU TK_MVI;

D: TRL TK_MVI VYJU TK_RBS | C TK_TGKU C TK_PRGF TK_KL | TK_BBZ TRL DNFF | TK_SRWU TK_DTRWR;

OGR: G 'H' MGOV TK_HLJ | TRL | TK_PRGF 'Q' 'K' SRVI TK_OM;

AHRM: LP TK_HLJ | DNFF VZKQM C I TK_ERGO | SWRI 'K' 'W' 'K' | TK_IEDA TK_BBZ TK_RBS SWRI;

I: UCKQB 'Y' UCKQB 'K';

UCKQB: TK_PRGF UCKQB 'H' 'Q' | SWRI 'H';

LP: UCKQB TK_TGKU | TK_BBZ;

MGOV: TK_MVI | TK_DTRWR | G TK_IEDA TK_SRWU TK_RBS TK_KL | 'Q' TK_BBZ C | 'Q';

TRL: TK_SRWU UCKQB LP TK_SRWU | SRVI | SWRI MGOV 'Y' TK_OM TK_PRGF;

