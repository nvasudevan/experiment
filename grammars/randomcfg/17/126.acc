%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_SRWU SWRI SRVI | TK_KL TK_HLJ;

VYJU: VZKQM TK_DTRWR | I DNFF | MGOV TK_ERGO I TK_BBZ;

VZKQM: VYJU TK_MVI TK_BBZ DNFF TK_IEDA | TK_KL OGR 'Y' | DNFF D TK_DTRWR TRL D | 'W' C TRL TK_RBS 'W' | TK_RBS;

DNFF: VYJU 'Y' 'Q' | TK_PRGF SRVI G | TK_SRWU 'K';

UXESO: SRVI TRL I 'Y' TK_KL | TK_MVI TK_OM TK_TGKU TK_MVI VZKQM | 'W' SRVI UCKQB;

SWRI: VZKQM | D MGOV SWRI | AHRM TK_IEDA SRVI TRL TK_PRGF;

G: I TK_HLJ G D | OGR 'Y' C DNFF 'Q' | SWRI 'Q' MGOV SWRI TK_PRGF | TK_KL 'Q' AHRM I UCKQB | G LP;

SRVI: TK_DTRWR AHRM SRVI TK_ERGO | OGR TK_KL SWRI | 'Y' TK_RBS;

C: TK_SRWU MGOV OGR TK_ERGO TRL | TK_IEDA | OGR 'Q';

D: MGOV | I C 'H' 'W' | ;

OGR: TK_OM I UCKQB OGR D | VYJU TK_MVI TK_RBS DNFF 'Q';

AHRM: 'K' MGOV TK_OM | 'Q' LP | UXESO TK_DTRWR DNFF | 'Y' I TK_OM | TK_RBS 'H' D I TK_TGKU;

I: 'Q' SWRI G TK_PRGF;

UCKQB: TK_TGKU 'K' SRVI TK_MVI TK_TGKU | TK_OM TK_IEDA 'Y' TK_PRGF TK_SRWU | ;

LP: TK_MVI VYJU TK_PRGF TK_BBZ | TK_KL LP DNFF MGOV TK_MVI;

MGOV: 'W' SRVI | TK_SRWU VYJU TK_IEDA TK_SRWU | LP | C VZKQM TK_DTRWR TK_IEDA TK_MVI;

TRL: VZKQM TK_MVI AHRM;

