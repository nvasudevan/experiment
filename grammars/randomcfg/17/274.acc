%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: LP OGR;

VYJU: TK_PRGF I MGOV 'H' | DNFF DNFF I TK_HLJ | TK_PRGF UCKQB TK_SRWU UCKQB TK_DTRWR;

VZKQM: SRVI UCKQB TRL D TK_BBZ | TK_IEDA OGR I;

DNFF: SRVI TK_OM 'Q' | UCKQB TK_ERGO MGOV G | VYJU | TK_PRGF D LP | 'Y' TK_BBZ 'Y';

UXESO: I OGR TRL LP 'H' | UCKQB 'Y' MGOV 'K' | TK_MVI MGOV 'Q' UXESO;

SWRI: TK_RBS TRL 'Q' | TK_IEDA TRL;

G: DNFF TK_PRGF | 'H' TK_SRWU LP | MGOV 'K' TK_KL 'K' TK_MVI | TK_BBZ 'W' C | TK_DTRWR D UCKQB TK_IEDA TK_PRGF;

SRVI: SRVI TK_HLJ TK_BBZ | VZKQM | I UXESO | TK_HLJ TK_KL 'K';

C: D TK_HLJ | SRVI UCKQB UXESO TK_KL | 'W';

D: VYJU | 'H' | TK_MVI TK_BBZ | TK_SRWU;

OGR: 'H' TK_KL TK_BBZ | TK_DTRWR VZKQM C | D TK_BBZ I | ;

AHRM: TK_ERGO 'K' 'W' | G TK_BBZ | SWRI TK_DTRWR TK_HLJ OGR;

I: 'Q' 'K' | TK_DTRWR I 'Y' 'H' | C OGR C TK_RBS TK_IEDA | VZKQM TK_ERGO MGOV;

UCKQB: UCKQB | AHRM 'Q' LP | ;

LP: UCKQB TK_MVI 'Y' DNFF TK_IEDA | LP TK_DTRWR D TK_HLJ TK_KL | TK_MVI DNFF UXESO G | 'W' 'Y' TK_KL;

MGOV: OGR LP 'K' | TK_RBS TK_IEDA G 'H' TK_RBS;

TRL: 'W' | TK_ERGO VZKQM | VYJU TK_KL UCKQB AHRM | 'Y' TK_KL 'Y' DNFF TK_DTRWR;

