%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: VYJU TK_TGKU TK_ERGO MGOV;

VYJU: TK_OM TK_SRWU DNFF | TK_DTRWR | VYJU TK_HLJ VZKQM 'Q';

VZKQM: TK_OM TK_SRWU TK_MVI 'Q' MGOV | TK_MVI MGOV TK_SRWU TK_IEDA TRL | TK_IEDA TK_SRWU TK_PRGF 'W' | MGOV;

DNFF: TK_HLJ TK_OM TK_DTRWR | TK_KL | TK_OM I TK_DTRWR 'H' | ;

UXESO: 'Y' TK_DTRWR TK_RBS TK_KL TK_DTRWR | MGOV TK_TGKU I | 'Y' 'Y' TK_MVI OGR G;

SWRI: UCKQB | 'K' TK_DTRWR TRL TK_MVI | 'Y';

G: SRVI C MGOV D 'W';

SRVI: D OGR | DNFF;

C: 'Q' | I TK_RBS SWRI TK_SRWU;

D: TK_DTRWR | 'Y' TK_DTRWR DNFF;

OGR: SRVI VZKQM UXESO 'W' D;

AHRM: 'K' SWRI SRVI C | TK_KL 'Q' 'K' G TK_KL | OGR | MGOV TK_PRGF TK_ERGO TK_IEDA | TK_SRWU TK_HLJ;

I: LP G DNFF TK_ERGO | TRL | TK_DTRWR TK_OM VYJU TK_MVI SRVI | TRL TK_HLJ;

UCKQB: D I TK_MVI 'Q' UCKQB | TRL D | 'H' | ;

LP: TK_PRGF TK_IEDA G | LP UCKQB UCKQB TK_SRWU;

MGOV: D | TK_KL TK_TGKU AHRM D 'Y' | C SWRI TK_BBZ | TK_KL MGOV 'Q' LP TK_OM | TK_TGKU MGOV;

TRL: VYJU 'K' TK_HLJ 'Y' TK_TGKU | TK_KL UXESO VZKQM C OGR | 'K' VYJU | D AHRM 'W' MGOV TK_IEDA | I TK_PRGF;

