%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: 'K' | VYJU TRL 'H' | TK_DTRWR TK_PRGF VYJU D I | 'W' TK_SRWU DNFF G TK_HLJ;

VYJU: TK_IEDA TK_IEDA UXESO | SRVI AHRM | MGOV D 'W' TK_BBZ LP;

VZKQM: SWRI 'Q';

