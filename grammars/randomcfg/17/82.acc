%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: UXESO 'Y' DNFF | C 'Y' 'W' | TK_OM OGR TK_RBS UXESO;

VYJU: TK_PRGF TK_DTRWR SWRI SWRI 'Q' | I SRVI C 'Q' | TK_HLJ TRL TK_OM | 'W';

VZKQM: SWRI 'Q' TK_BBZ SWRI | TK_DTRWR | TK_ERGO;

DNFF: C TK_TGKU SRVI TK_ERGO 'Y' | ;

UXESO: AHRM SWRI TK_BBZ 'Q' TK_HLJ | UCKQB MGOV TK_IEDA | D TK_KL OGR 'H' | SWRI I TK_BBZ VYJU | SWRI TK_DTRWR;

SWRI: TK_DTRWR D DNFF TK_MVI | TK_SRWU D DNFF TK_ERGO D | TK_TGKU DNFF TK_OM 'H' TK_ERGO | 'W' G | AHRM D 'H' TK_ERGO TK_MVI;

G: SRVI UCKQB SRVI TK_IEDA C | 'Q';

SRVI: TK_HLJ SWRI TK_MVI TK_TGKU | VZKQM TK_TGKU 'Y' G | ;

C: 'K' | TK_KL | TRL TK_PRGF G AHRM | TK_HLJ TK_RBS SWRI 'H' 'K';

D: TK_TGKU TK_OM D VZKQM | TK_BBZ TK_RBS UCKQB I AHRM | TK_PRGF;

OGR: TK_MVI;

AHRM: 'W' TK_PRGF | 'K' TK_ERGO TK_RBS TK_SRWU TK_DTRWR | 'K' MGOV TK_PRGF 'W';

I: VYJU AHRM OGR | LP G D TK_OM | OGR TRL UXESO TK_RBS TK_HLJ;

UCKQB: 'H' | TK_SRWU TK_ERGO AHRM | D | VZKQM;

LP: C 'Q' G | TK_DTRWR | 'W' VZKQM OGR OGR 'W' | TK_IEDA | TK_OM TK_IEDA D UXESO VZKQM;

MGOV: TK_SRWU;

TRL: TK_DTRWR TK_ERGO TK_MVI | TK_BBZ TK_BBZ LP | TK_KL | TRL LP SWRI TK_ERGO;

