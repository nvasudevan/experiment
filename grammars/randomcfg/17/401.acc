%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_OM | VZKQM DNFF | TK_IEDA 'K' TK_BBZ;

VYJU: TRL MGOV TK_DTRWR TK_MVI TK_RBS | MGOV TK_BBZ D 'Q' VYJU;

VZKQM: G TK_KL TK_ERGO MGOV TK_IEDA | TRL UCKQB VZKQM TK_RBS TK_HLJ | DNFF VZKQM;

DNFF: 'W' DNFF DNFF TK_DTRWR TK_PRGF | MGOV 'W' SWRI;

UXESO: VYJU TK_SRWU MGOV 'K' C | TK_DTRWR UXESO OGR | G | 'W';

SWRI: TK_KL TK_MVI UCKQB TK_PRGF | UXESO TK_HLJ VZKQM UCKQB | C 'K' TK_IEDA;

G: UXESO TK_BBZ TK_BBZ TK_HLJ TK_BBZ | 'W' AHRM SRVI | ;

SRVI: TK_KL | TK_ERGO UXESO;

C: LP TK_SRWU | TK_DTRWR TK_RBS TK_BBZ SWRI I | TK_BBZ TK_RBS;

D: D TK_DTRWR | I 'Q';

OGR: TK_OM TK_IEDA TK_ERGO TRL DNFF | TK_BBZ TK_ERGO TK_RBS TK_ERGO | MGOV TK_PRGF 'W' TK_SRWU | TK_IEDA;

AHRM: 'W' TK_BBZ TK_BBZ 'Q' SWRI | ;

I: TK_TGKU | OGR TK_PRGF MGOV VYJU C | TK_RBS | AHRM MGOV 'Q';

UCKQB: TK_SRWU MGOV TK_IEDA TK_IEDA MGOV | MGOV TRL TK_OM C TK_MVI;

LP: TK_KL 'H' D 'H' TK_IEDA | UCKQB 'Y' VYJU VYJU | SWRI;

MGOV: LP AHRM TK_PRGF | C VYJU 'Q' TK_MVI I | 'K' TK_PRGF TRL OGR;

TRL: I VYJU TK_ERGO OGR DNFF | TK_MVI | G;

