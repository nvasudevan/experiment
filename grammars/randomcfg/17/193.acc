%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: C OGR TK_PRGF DNFF TK_OM | AHRM VZKQM;

VYJU: TK_HLJ UXESO TK_HLJ SWRI TK_MVI | UXESO TRL TK_ERGO | LP VYJU | TK_MVI DNFF TK_HLJ SRVI;

VZKQM: DNFF TK_DTRWR | TK_OM G 'H' TK_MVI | TK_DTRWR TK_BBZ SWRI TRL C;

DNFF: SRVI TK_TGKU I TRL | TK_KL D TK_SRWU TK_HLJ VZKQM | SWRI 'Q' | SRVI | MGOV VZKQM OGR TK_RBS;

UXESO: TK_RBS UXESO | 'Q' | TRL TK_KL TK_SRWU DNFF I | G TK_IEDA SWRI | UXESO TK_SRWU;

SWRI: MGOV TK_HLJ TRL 'Y' VYJU | TK_ERGO LP | 'Q' 'K' | ;

G: LP TK_OM | TK_RBS 'W' | TK_IEDA MGOV TK_OM D TK_BBZ | TK_ERGO SRVI TK_RBS;

SRVI: TK_BBZ TK_BBZ SRVI | TK_TGKU TK_RBS 'K' | UXESO;

C: SWRI G;

D: TK_BBZ TK_DTRWR TK_SRWU | SWRI TK_IEDA SWRI DNFF TRL | TK_HLJ TK_ERGO 'K' UCKQB SRVI | ;

OGR: 'W' | SRVI TK_MVI TK_HLJ TK_IEDA | OGR DNFF | OGR C TK_ERGO OGR;

AHRM: TK_ERGO TK_HLJ TK_RBS | SRVI UCKQB UCKQB 'H' TK_BBZ | TK_BBZ UXESO TRL TK_SRWU | TK_PRGF VYJU;

I: TK_OM 'H' | TK_DTRWR | 'Q' TK_MVI | TK_IEDA TK_IEDA TK_ERGO | TK_RBS TK_PRGF 'K';

UCKQB: SRVI TK_KL 'Y' TRL 'Y' | 'K' TK_IEDA TK_DTRWR C | 'Q' I SWRI TK_OM TK_DTRWR | TK_PRGF VZKQM | ;

LP: LP TK_RBS AHRM | TK_OM VYJU TK_HLJ 'Q' I | TK_OM;

MGOV: I MGOV SRVI | I SRVI MGOV TK_SRWU C | TK_SRWU VYJU TK_OM | 'W' TK_RBS | TK_OM UCKQB TK_OM;

TRL: TK_IEDA MGOV TK_MVI DNFF TK_KL | TK_MVI TK_MVI | TK_IEDA TK_OM 'Q' TK_TGKU | MGOV LP TK_OM SRVI 'H';

