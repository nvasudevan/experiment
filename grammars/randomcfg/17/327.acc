%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: SRVI C TK_ERGO SWRI;

VYJU: UXESO TK_MVI 'Y' 'Q' 'W' | 'K' | ;

VZKQM: SWRI 'K' 'K' C | TK_DTRWR D TK_TGKU TK_HLJ UXESO | DNFF TK_OM VYJU TK_BBZ | UXESO LP UCKQB TK_BBZ TK_MVI | D SRVI TK_SRWU;

DNFF: VYJU | 'Q' DNFF VYJU 'W' | 'Q' I TRL SWRI VYJU;

UXESO: I;

SWRI: TK_SRWU TK_RBS 'W' | TK_DTRWR TK_HLJ C I;

G: VYJU TK_RBS SWRI 'W' TK_DTRWR | TK_ERGO TK_RBS;

SRVI: TK_DTRWR | TK_RBS | TK_DTRWR UXESO C | 'H' SWRI D;

C: TRL AHRM VZKQM | 'K' TK_SRWU TK_OM OGR UXESO | SRVI 'W' G OGR | 'K' TK_RBS TK_PRGF VYJU 'Y' | TRL TK_RBS;

D: 'W' MGOV C | TK_DTRWR DNFF TK_PRGF UCKQB TK_RBS | 'K' VYJU | 'Y' TK_DTRWR I OGR TK_IEDA;

OGR: TK_BBZ | AHRM | TRL TK_KL SRVI TK_IEDA SRVI;

AHRM: VZKQM | TK_MVI VYJU TK_OM TRL | OGR VYJU 'H' LP | TK_SRWU 'Q' G TK_ERGO | TRL TK_MVI SRVI 'W' LP;

I: TK_HLJ TK_OM MGOV | TK_OM TK_TGKU | ;

UCKQB: SRVI;

LP: OGR TK_SRWU I 'H' | TK_MVI 'K' SRVI UXESO SWRI;

MGOV: LP | TK_RBS | AHRM TRL | SRVI 'K' 'Y' UCKQB | TK_OM VZKQM UCKQB;

TRL: TK_BBZ OGR | DNFF TK_TGKU | I | OGR 'K' TK_TGKU;

