%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: 'Y' TRL TK_KL VYJU 'Y';

VYJU: TK_PRGF C TK_TGKU TK_HLJ TK_RBS | UXESO DNFF;

VZKQM: LP 'K' | I C | SWRI VYJU TK_DTRWR | OGR TRL 'K' AHRM;

DNFF: MGOV C | TK_DTRWR UCKQB LP;

UXESO: 'H' TK_MVI 'H' TK_OM TK_OM;

SWRI: SWRI MGOV | ;

G: DNFF DNFF | LP TK_KL TK_BBZ | VZKQM OGR | VYJU UXESO 'Y' | TK_RBS 'K' TRL I 'H';

SRVI: TRL TK_SRWU | UCKQB OGR | TK_KL TRL;

C: SRVI | MGOV TK_IEDA TK_IEDA UCKQB;

D: OGR TK_TGKU TK_DTRWR MGOV TK_RBS;

OGR: MGOV TK_OM G;

AHRM: TK_ERGO TK_IEDA SWRI VYJU | 'W' TK_ERGO UXESO TK_MVI D | DNFF TK_PRGF | TK_MVI UCKQB C | TK_TGKU TK_ERGO TK_RBS TK_OM C;

I: TK_ERGO TK_OM TK_PRGF VYJU;

UCKQB: TK_IEDA MGOV | AHRM TK_ERGO VZKQM DNFF LP | SWRI TRL OGR 'K' TK_BBZ;

LP: TK_MVI;

MGOV: TK_KL TK_KL;

TRL: TK_PRGF | VYJU TRL TK_MVI TK_BBZ | TK_ERGO | G C | OGR;

