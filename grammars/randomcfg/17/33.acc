%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: G | TK_TGKU TK_OM 'K' 'W' OGR | TK_KL | 'W' | TK_IEDA 'K' 'K';

VYJU: SWRI | 'H' TK_DTRWR 'Q' | DNFF TK_SRWU;

VZKQM: TK_DTRWR | LP DNFF;

DNFF: TK_DTRWR TK_ERGO | OGR TK_BBZ I TK_RBS;

UXESO: I OGR TK_RBS 'K' | VYJU SRVI TK_BBZ D VZKQM | 'W' TK_MVI | TRL OGR 'K' SRVI 'Q' | VYJU;

SWRI: OGR AHRM OGR TK_TGKU AHRM | TK_OM 'K' | TK_KL UCKQB I TK_TGKU LP | MGOV TK_IEDA TK_PRGF TK_SRWU;

G: TK_IEDA UXESO I;

