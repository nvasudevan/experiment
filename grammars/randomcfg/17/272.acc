%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: VZKQM TK_KL TK_SRWU TK_MVI TK_RBS | UXESO | TK_IEDA G | 'H' I LP TK_HLJ | TK_PRGF 'Q';

VYJU: TK_OM | TK_TGKU TK_SRWU;

VZKQM: TRL LP 'K' DNFF UCKQB;

DNFF: TK_KL | TK_DTRWR SWRI | C | UXESO UXESO TK_PRGF;

UXESO: TK_TGKU TK_RBS | TK_BBZ | TK_KL | TK_MVI;

SWRI: DNFF;

G: TK_BBZ TK_PRGF 'Y' UCKQB UXESO | 'K' 'H' 'H' TK_KL | UXESO OGR TK_HLJ VZKQM | ;

SRVI: UCKQB TK_MVI TK_RBS G VZKQM | SRVI TK_PRGF 'Q' G 'Y' | 'K' TK_KL LP | MGOV TK_PRGF TK_RBS | AHRM TK_BBZ;

C: TK_RBS AHRM 'Y';

D: TK_SRWU TK_HLJ UCKQB 'Y' DNFF;

OGR: G | TK_TGKU DNFF TK_BBZ | VYJU;

AHRM: VZKQM;

I: TK_BBZ VZKQM | VZKQM UCKQB TK_DTRWR TK_RBS | TK_BBZ TK_BBZ | SWRI;

UCKQB: AHRM 'H' | OGR TK_PRGF | SWRI TK_IEDA;

LP: TK_DTRWR TK_IEDA 'Q' | 'K' LP TK_PRGF TK_DTRWR TK_OM;

MGOV: I TRL SRVI | 'H' 'H' | TK_MVI | TK_PRGF VZKQM LP SRVI TK_SRWU;

TRL: TK_TGKU VZKQM TK_RBS 'Y' SWRI | TRL I TK_RBS | TK_MVI | ;

