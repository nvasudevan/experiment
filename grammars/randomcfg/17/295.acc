%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_MVI TK_HLJ | 'H' UXESO TK_OM TK_ERGO LP;

VYJU: 'W' 'H' | DNFF TK_TGKU | TK_OM | TK_TGKU DNFF;

VZKQM: UXESO TK_MVI AHRM;

DNFF: I 'H' I TK_BBZ D | SWRI | 'H' AHRM D | SWRI 'K';

UXESO: SWRI | SRVI I TK_IEDA | TK_RBS | TK_IEDA VZKQM;

SWRI: SWRI | TK_HLJ TK_ERGO TK_KL SRVI TK_PRGF | 'K' AHRM 'H' TK_MVI | TK_IEDA TK_HLJ TK_RBS 'W' TK_SRWU;

G: MGOV TK_MVI | TK_DTRWR | TK_KL TK_ERGO LP I | TK_SRWU LP 'W';

SRVI: OGR VZKQM 'Y' | TK_ERGO TK_IEDA SRVI TK_HLJ | TK_BBZ | SRVI | TK_RBS G OGR LP TK_ERGO;

C: 'W' LP OGR TK_KL I | SRVI SRVI VYJU | 'W' UCKQB TK_IEDA VYJU SRVI;

D: 'Q';

OGR: 'K' | MGOV | UCKQB TK_RBS;

AHRM: 'W' DNFF OGR | OGR DNFF SWRI | TRL;

I: 'Q' 'K';

UCKQB: OGR 'H' TK_MVI | UCKQB UXESO DNFF | TK_OM TK_SRWU 'W' I TK_KL;

LP: TK_OM DNFF TK_MVI TK_MVI | ;

MGOV: LP LP | TK_HLJ DNFF TK_TGKU | SRVI TK_IEDA OGR 'Y' | ;

TRL: TK_RBS AHRM TK_KL C;

