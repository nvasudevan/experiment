%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: G 'W' MGOV | AHRM VYJU VYJU AHRM I | UCKQB | MGOV C TK_KL;

VYJU: TK_TGKU 'K' TK_KL TK_BBZ 'Q' | VYJU TK_MVI UCKQB VZKQM | C TK_KL UCKQB SRVI | TK_BBZ;

VZKQM: UXESO G OGR 'Y' | SWRI 'Y' | I C UXESO TK_RBS I | AHRM TK_IEDA DNFF TK_DTRWR | TK_BBZ TK_SRWU SWRI UXESO;

DNFF: 'H' | D | C DNFF 'W' VYJU VZKQM;

UXESO: OGR 'K' | VZKQM TRL D OGR;

SWRI: SRVI MGOV TK_KL VYJU | ;

G: SRVI LP | C TK_PRGF TK_BBZ | UCKQB 'Y';

SRVI: I | TK_BBZ | MGOV D | VYJU | 'H' TK_IEDA;

C: SWRI | TK_SRWU TK_SRWU | TK_OM;

D: TK_ERGO D TK_RBS | TK_TGKU | SWRI TK_KL VYJU | 'H' TK_MVI 'H' C | TK_BBZ SWRI SRVI;

OGR: TRL TK_OM TK_KL AHRM DNFF;

AHRM: TK_OM C | SRVI OGR 'H' | TK_ERGO TK_BBZ TK_DTRWR | LP | UXESO 'H' OGR TK_OM;

I: TK_SRWU SRVI 'Q' TK_ERGO OGR | AHRM TK_MVI TK_KL TK_PRGF;

UCKQB: VYJU SWRI AHRM 'Y' 'W' | TK_DTRWR TK_TGKU | I OGR 'H' TK_PRGF 'K' | C 'Y' | ;

LP: 'K' TK_ERGO G VYJU 'W' | TK_TGKU TK_PRGF TK_PRGF VZKQM | 'H' SRVI TK_IEDA AHRM;

MGOV: TK_KL TK_OM 'Q' 'W' OGR;

TRL: VZKQM TK_HLJ G TK_OM C;

