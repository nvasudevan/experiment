VZKQM: TK_BBZ TK_DTRWR G 'K' | 'Y';

DNFF: TK_DTRWR TK_KL TK_KL TK_DTRWR TK_TGKU | UCKQB | TK_TGKU TK_HLJ | TRL C 'H' VZKQM SWRI;

