%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_SRWU MGOV;

VYJU: TK_SRWU TK_RBS C;

VZKQM: DNFF TK_KL | SRVI TK_BBZ DNFF G TK_DTRWR | TK_IEDA | TK_DTRWR | TK_HLJ DNFF VZKQM TRL TK_BBZ;

DNFF: 'K' TK_DTRWR | TK_PRGF TK_PRGF TK_MVI 'Q' | UCKQB G 'Q' | TK_RBS;

UXESO: TK_SRWU D MGOV VYJU AHRM | TK_TGKU TK_SRWU | DNFF LP TK_MVI UCKQB TK_DTRWR;

SWRI: TK_PRGF D C AHRM C | ;

G: TK_RBS | TK_RBS UCKQB 'W' TK_SRWU;

SRVI: TK_OM TK_IEDA MGOV C | AHRM D VYJU TRL;

C: VZKQM TK_ERGO | 'H' VYJU SRVI | TK_TGKU UCKQB TK_KL | ;

D: TK_SRWU TK_OM TK_IEDA | UXESO SWRI | DNFF TRL C TK_IEDA;

OGR: TK_ERGO DNFF;

AHRM: 'Y' UCKQB 'K' | 'W' D | TK_ERGO SWRI SWRI | LP 'W' DNFF DNFF G;

I: C C SRVI | I | 'K' TK_BBZ SWRI TK_DTRWR;

UCKQB: 'K' VZKQM | I | SWRI AHRM | OGR;

LP: TK_RBS TK_SRWU;

MGOV: TRL TK_HLJ TK_IEDA TK_BBZ | TK_IEDA SWRI LP D UCKQB | OGR;

TRL: UXESO TK_TGKU 'W' TK_BBZ 'K' | VZKQM OGR UXESO | AHRM TK_ERGO | TK_HLJ LP MGOV;

