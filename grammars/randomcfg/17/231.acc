%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: SWRI TK_ERGO | 'H' SRVI AHRM D TK_RBS | SWRI TRL VYJU TK_SRWU;

VYJU: MGOV OGR D | ;

VZKQM: D 'Q' I TK_TGKU | C | 'Y';

DNFF: 'K' | SWRI LP LP 'Y' SWRI | UCKQB 'W' TK_KL | MGOV D 'K' TK_RBS 'K' | 'W' MGOV 'Y';

UXESO: I C TK_IEDA TK_HLJ TK_DTRWR | SWRI I | ;

SWRI: G TK_PRGF 'H' | MGOV VYJU DNFF MGOV | TK_BBZ TK_ERGO | TK_SRWU TK_MVI MGOV;

G: I LP SWRI | TK_ERGO | OGR | SRVI TK_SRWU OGR SWRI;

SRVI: UCKQB SWRI G | TK_KL TK_RBS TK_DTRWR TRL;

C: I 'H' TK_HLJ TK_HLJ TK_MVI | 'Q' TK_DTRWR 'Y' UCKQB SRVI | DNFF TK_BBZ TK_ERGO | I TK_KL TK_IEDA OGR C;

D: UXESO TK_ERGO G TK_MVI MGOV;

OGR: MGOV | TK_ERGO VYJU G MGOV TK_SRWU | TK_HLJ TK_HLJ | TK_RBS TK_IEDA;

AHRM: UXESO;

I: VYJU TK_IEDA LP D I | C | TK_SRWU UXESO TK_PRGF LP AHRM | TK_RBS UXESO G;

UCKQB: 'K' TK_SRWU TK_HLJ | SRVI TK_DTRWR SWRI | C TK_ERGO | TK_ERGO TK_SRWU TK_MVI | UCKQB;

LP: DNFF SWRI | LP UCKQB SRVI TK_OM | DNFF TK_DTRWR SRVI TK_TGKU SRVI;

MGOV: TK_SRWU UXESO SWRI | UXESO TK_OM | 'Q' SRVI I;

TRL: TK_ERGO | DNFF | MGOV VZKQM G;

