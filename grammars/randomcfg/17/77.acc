%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_MVI 'K';

VYJU: D TK_PRGF | I MGOV TK_DTRWR VYJU DNFF | MGOV;

VZKQM: UCKQB 'H' | D TK_MVI I | G 'Y' | 'K' TK_RBS 'Q' OGR;

DNFF: TK_RBS DNFF | G MGOV | TK_TGKU 'W' | TK_PRGF AHRM TK_IEDA I;

UXESO: MGOV;

SWRI: MGOV VYJU 'Q' 'Q' | TK_HLJ 'Y' TK_HLJ TK_IEDA | SRVI TK_KL TK_SRWU | TRL SWRI C TK_OM;

G: TK_HLJ TK_BBZ TK_BBZ TK_BBZ TK_HLJ | 'H' | 'Q' UXESO 'H' LP SRVI | UCKQB TK_HLJ TK_MVI TK_TGKU TK_MVI;

SRVI: SWRI TK_DTRWR | TK_MVI | ;

C: D I TK_SRWU TK_RBS | TK_RBS UXESO OGR | D TK_TGKU DNFF TRL | VZKQM C 'W';

D: VYJU AHRM TRL C D | SRVI;

OGR: 'W' SWRI TK_IEDA | TK_OM LP | 'Q' TK_MVI C OGR 'W' | SRVI;

AHRM: AHRM 'H' | 'H' TK_DTRWR 'W' | ;

I: TK_BBZ VYJU | TK_SRWU 'Q' | VZKQM TK_DTRWR;

UCKQB: 'Y' UCKQB TK_IEDA 'Y' TK_MVI | UXESO TK_SRWU 'Y' TK_RBS 'Q' | OGR DNFF;

LP: G | SRVI | TK_IEDA SWRI | AHRM TK_IEDA SRVI I C;

MGOV: TK_KL TK_HLJ 'Q' TK_IEDA UCKQB | TK_DTRWR I 'W' MGOV 'W';

TRL: UCKQB C TK_DTRWR TRL | SRVI D | G G | LP UXESO LP SRVI;

