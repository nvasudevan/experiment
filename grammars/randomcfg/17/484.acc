UCKQB: AHRM MGOV TK_HLJ TK_TGKU | DNFF 'Y' TK_PRGF 'H';

LP: 'W' OGR TK_OM TK_HLJ | TK_DTRWR TK_BBZ | TK_HLJ UXESO | G;

MGOV: TK_IEDA 'Y' TK_OM TK_BBZ G;

TRL: 'Y' | UXESO MGOV G TK_MVI D | AHRM 'Q' 'K' | MGOV TK_KL TK_OM LP TK_RBS | TK_RBS I;

