%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TRL | LP;

VYJU: 'H' 'Y' | TK_OM AHRM I;

VZKQM: DNFF 'W' | TK_DTRWR UXESO | G | C 'K';

DNFF: TK_KL 'Y' | TK_MVI 'K' VZKQM 'K';

UXESO: 'W' LP | 'Y' C UXESO TK_BBZ | 'K' 'H' TK_OM LP;

SWRI: D TK_TGKU OGR | TK_OM;

