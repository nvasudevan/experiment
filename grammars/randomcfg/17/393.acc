%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: UXESO MGOV | TK_TGKU TK_ERGO TK_HLJ;

VYJU: TK_HLJ | SRVI I D 'W';

VZKQM: TK_BBZ | C UCKQB DNFF | UCKQB I;

DNFF: UCKQB TK_KL LP | SRVI DNFF LP TK_TGKU TK_BBZ | D OGR 'Q' 'K' 'Q' | TK_BBZ DNFF TK_KL;

UXESO: TK_PRGF 'Y' TK_BBZ AHRM | 'Q' 'W' TK_IEDA | TK_SRWU C TK_RBS C TK_MVI | TK_RBS MGOV UXESO VZKQM | TK_ERGO UXESO TK_BBZ TK_ERGO;

SWRI: TRL | UCKQB TK_MVI G 'Q' | TK_PRGF UXESO TK_RBS MGOV | C UCKQB 'Y' TK_KL | TK_IEDA G TK_ERGO TK_BBZ;

G: TK_RBS | VZKQM D AHRM 'Q' DNFF | TK_KL TK_IEDA VZKQM;

SRVI: TK_IEDA I;

C: TK_SRWU | OGR UXESO TK_MVI UCKQB SWRI;

D: G | D UXESO | TK_HLJ | AHRM | UXESO TK_IEDA SWRI;

OGR: TRL 'W' UCKQB | VZKQM 'Y' 'W';

AHRM: 'H' 'K' C UCKQB | 'W' 'Y';

I: MGOV TK_ERGO | SWRI | TK_SRWU TK_OM I AHRM | ;

UCKQB: TK_IEDA 'Q' TK_DTRWR TK_DTRWR | TK_OM I TK_PRGF | ;

LP: VZKQM | VYJU LP TK_PRGF SRVI;

MGOV: 'Q' 'K' | TK_RBS TK_SRWU TK_TGKU TK_HLJ SRVI | 'Y' SRVI MGOV TK_BBZ | TK_BBZ TK_DTRWR TK_MVI;

TRL: TK_SRWU TK_RBS SWRI TK_TGKU | SWRI;

