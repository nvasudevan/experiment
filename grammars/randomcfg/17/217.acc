%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: VYJU SRVI;

VYJU: TK_DTRWR TK_ERGO UCKQB TK_MVI | TK_ERGO;

VZKQM: DNFF OGR | TK_ERGO TRL 'K' 'H' TK_HLJ | 'H' TK_TGKU | UXESO 'Y';

DNFF: TK_KL UXESO AHRM | TK_RBS TK_PRGF AHRM AHRM 'K';

UXESO: AHRM | 'Y' TK_TGKU TK_TGKU TK_OM TK_HLJ | I 'H' TRL TK_KL TK_MVI | 'K' | 'W' 'Q' TK_RBS VYJU VYJU;

SWRI: 'K' AHRM UCKQB TK_DTRWR | TK_MVI | TK_MVI VYJU UCKQB 'H';

G: TK_MVI | I DNFF AHRM;

SRVI: MGOV | TK_ERGO UCKQB | SWRI SWRI | TK_KL 'Y' SRVI | ;

C: LP DNFF UCKQB TRL | 'W' UCKQB TK_RBS | SWRI 'H' 'Y' TK_OM TK_DTRWR | TK_SRWU UCKQB TK_DTRWR 'Y' | 'Y';

D: UCKQB TK_DTRWR TK_KL TK_ERGO | TK_RBS TK_KL TK_OM TK_PRGF | ;

OGR: TK_DTRWR TK_KL TK_MVI SWRI TK_KL | VYJU | AHRM AHRM | MGOV 'W' TK_BBZ | TK_BBZ TK_SRWU OGR VYJU;

AHRM: G I OGR UCKQB TK_MVI;

I: TK_DTRWR 'Y' | TK_OM I | D TRL TK_KL 'Q' AHRM;

UCKQB: UXESO 'W' TK_IEDA TK_TGKU TK_MVI | G TRL TK_PRGF 'W' | OGR 'H' TK_TGKU | TK_RBS 'H' 'Q' 'H' TK_BBZ | UXESO 'K' G;

LP: VZKQM | TK_ERGO UCKQB TK_ERGO VYJU | SRVI TK_OM DNFF TK_OM TK_IEDA | 'K' | UXESO;

MGOV: TK_OM | MGOV AHRM | TK_KL 'H' | SWRI I;

TRL: TK_RBS DNFF;

