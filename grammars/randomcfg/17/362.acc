%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: SRVI C MGOV | VYJU | AHRM 'Q' TK_SRWU | TK_TGKU VZKQM 'K';

VYJU: TK_IEDA TK_MVI TK_HLJ 'Y' 'Y' | I | TK_SRWU TK_BBZ | VYJU 'K' 'Q';

VZKQM: MGOV TK_RBS TK_IEDA;

DNFF: MGOV 'Q' TK_ERGO | AHRM TK_KL 'K' | TK_TGKU 'W' TK_BBZ TRL;

UXESO: 'Y' MGOV 'K' | OGR VZKQM | TK_KL TK_OM | ;

SWRI: TK_KL UXESO | DNFF UXESO | 'W' I;

G: 'H' LP LP;

SRVI: I TK_IEDA VYJU TK_RBS | TK_PRGF LP;

C: TK_IEDA TK_RBS G TK_PRGF | G 'Y' C TK_PRGF TK_ERGO;

D: TK_BBZ VZKQM TK_TGKU | UCKQB AHRM UCKQB;

OGR: TK_BBZ LP TK_RBS 'W' | SWRI;

AHRM: TK_ERGO TK_RBS TK_TGKU | TK_ERGO G UCKQB TK_HLJ UCKQB;

I: DNFF TRL TK_IEDA MGOV 'W' | TK_IEDA SWRI C 'Y' | TK_MVI TK_BBZ MGOV UCKQB TRL | TK_PRGF | SWRI DNFF;

UCKQB: D I TK_MVI 'H';

LP: OGR UXESO 'W' 'Y' UXESO | 'H' TK_BBZ G AHRM | UXESO C TK_BBZ | ;

MGOV: TK_IEDA TK_DTRWR TK_ERGO TK_OM 'Y' | SRVI;

TRL: D UXESO D | SWRI C;

