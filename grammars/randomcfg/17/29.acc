%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: I | VYJU TK_KL | LP 'Y';

VYJU: C TK_OM | TK_OM;

VZKQM: TK_OM DNFF 'Q' LP | 'W' TK_RBS AHRM C TK_SRWU | LP 'Q' OGR | UXESO TK_KL DNFF MGOV TRL | VZKQM TK_OM DNFF TK_IEDA;

DNFF: UCKQB TK_OM TK_SRWU | 'H' 'K' | TK_RBS TK_SRWU | I UCKQB OGR G | DNFF TK_SRWU UCKQB VZKQM;

UXESO: TK_OM I | OGR VYJU TK_SRWU 'H' | TK_MVI DNFF SWRI 'Q';

SWRI: TK_DTRWR OGR TK_IEDA G AHRM | 'Q' | TK_DTRWR TK_KL I | G UCKQB TK_IEDA 'W' D;

G: TK_ERGO AHRM MGOV 'K' | TK_ERGO | 'Y' | UCKQB | TK_RBS VZKQM C TK_IEDA TK_RBS;

SRVI: SWRI | I VZKQM G TK_SRWU | ;

C: UCKQB TK_IEDA SWRI TK_RBS G;

D: 'K' TK_OM VZKQM I TK_MVI | TRL 'K' LP SRVI | 'K' | ;

OGR: D LP TK_BBZ;

AHRM: 'Y' TK_KL 'Y' | TK_DTRWR TK_BBZ;

I: DNFF MGOV OGR UCKQB | I TK_TGKU | TK_RBS TK_BBZ TK_ERGO | AHRM LP LP;

UCKQB: VZKQM 'K' | TK_HLJ;

LP: TK_MVI | C VYJU;

MGOV: TRL SRVI TRL TK_ERGO | TK_MVI 'K' 'Y' 'H' | TRL AHRM;

TRL: TK_HLJ 'Q' TK_BBZ | TK_TGKU TK_HLJ 'Q' | UXESO 'H' 'Y' TK_BBZ AHRM | 'Q' 'W' TK_KL TK_TGKU | TK_RBS TK_HLJ TK_HLJ LP;

