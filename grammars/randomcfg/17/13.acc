%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: D LP 'W' TK_ERGO | 'K' TK_PRGF TK_RBS | TK_ERGO SRVI 'Q' VZKQM 'H';

VYJU: DNFF TK_ERGO TK_MVI TK_SRWU TK_BBZ | SRVI 'Q' 'K' TK_BBZ SWRI | LP VYJU LP AHRM;

VZKQM: TK_BBZ | SWRI | UCKQB UCKQB TK_ERGO | UXESO VYJU AHRM I TK_OM;

DNFF: OGR TK_HLJ | C SWRI OGR | SRVI SRVI TK_ERGO | TK_KL TK_OM 'K' VYJU;

UXESO: OGR D DNFF | TK_RBS 'K';

SWRI: UCKQB TK_SRWU C D | D 'K' TK_PRGF OGR TK_MVI | VZKQM DNFF TK_ERGO SWRI G;

G: 'H' TK_TGKU 'W' 'Y' G | C TK_DTRWR UXESO TK_BBZ;

SRVI: MGOV UXESO D 'Y' | TK_DTRWR | TK_SRWU TK_DTRWR TK_PRGF TK_KL | C D | ;

C: 'K' TK_HLJ UCKQB TRL | TK_IEDA TK_BBZ TRL;

D: UXESO DNFF | 'Y' | C | VZKQM TK_HLJ | TRL TK_PRGF C VZKQM 'W';

OGR: 'H' DNFF TK_MVI | MGOV | TK_HLJ;

AHRM: AHRM TK_PRGF TK_ERGO 'K' | TK_HLJ TK_TGKU UXESO | ;

I: TK_TGKU 'W' 'H' | MGOV SWRI VZKQM UCKQB 'Q';

UCKQB: G TK_MVI 'H' | UCKQB I TK_SRWU | TK_DTRWR;

LP: 'W' SRVI TK_IEDA DNFF | D;

MGOV: VYJU 'Y' SWRI 'Q' SWRI;

TRL: 'H' G 'Q' | UCKQB TK_PRGF 'K';

