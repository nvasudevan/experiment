%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: G TRL C TK_KL MGOV | TK_MVI 'H';

VYJU: C I 'H' SRVI;

VZKQM: AHRM LP TRL AHRM | TK_DTRWR TK_PRGF TRL D 'K' | 'H' UCKQB SWRI;

DNFF: 'W' | G UCKQB MGOV | D TK_ERGO OGR | LP 'Q' TK_BBZ TK_IEDA | UXESO TK_TGKU TK_IEDA;

UXESO: TK_IEDA | TK_KL | ;

SWRI: TK_DTRWR;

G: TK_IEDA AHRM TK_SRWU UXESO TK_KL | ;

SRVI: D TK_DTRWR TK_SRWU;

C: VYJU VYJU G | 'Q' | TRL | LP I;

D: I TK_ERGO | TK_BBZ C | TK_HLJ D TK_SRWU 'Y' 'W';

OGR: D TK_ERGO 'Q';

AHRM: OGR TRL VZKQM TK_ERGO TK_ERGO | SWRI SRVI | TK_TGKU C;

I: 'Q' | DNFF C TK_ERGO | TK_DTRWR I VYJU;

UCKQB: TK_BBZ LP VZKQM | 'W' TK_SRWU DNFF | 'Q' I;

LP: I | DNFF UXESO | LP | SWRI AHRM I;

MGOV: 'W' | TK_HLJ TK_RBS 'Q' OGR | TK_IEDA TK_RBS 'Y' SWRI | TK_ERGO TK_SRWU | TK_OM C TK_MVI TK_MVI;

TRL: 'Y' 'Q' LP | TK_BBZ I SRVI | 'Q' TK_HLJ | VZKQM TK_DTRWR;

