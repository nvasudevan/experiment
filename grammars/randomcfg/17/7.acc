%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: SWRI 'W' D TK_KL TK_RBS;

VYJU: 'Y' TK_HLJ TK_HLJ SWRI | UCKQB TK_BBZ C UXESO TK_MVI | C | TK_TGKU | 'W' UCKQB TK_DTRWR;

VZKQM: LP TK_OM AHRM TK_SRWU LP | VYJU 'H' LP | TRL 'Q' TK_DTRWR TK_HLJ | VYJU;

DNFF: 'W' 'K' VZKQM | TK_MVI I 'H' LP DNFF | C | I;

UXESO: TK_ERGO SWRI 'Q';

SWRI: TK_ERGO TRL | 'Y' VYJU LP | SWRI SWRI SWRI 'Y' TK_MVI | UCKQB UXESO | VZKQM D D TK_KL SWRI;

G: TK_RBS 'H' MGOV 'H' UCKQB | TK_TGKU 'H' | LP | ;

SRVI: TK_KL 'Q' 'W' SWRI SWRI;

C: DNFF TK_OM MGOV DNFF TK_IEDA | TK_OM TK_ERGO 'H' | TK_TGKU DNFF TK_BBZ VZKQM SWRI | TK_ERGO;

D: TK_HLJ TRL;

OGR: 'K' TK_IEDA | 'Q' | TK_SRWU DNFF TK_HLJ;

AHRM: 'H' TK_ERGO TK_MVI TK_OM | SWRI VYJU TK_ERGO 'Y';

I: UXESO TK_SRWU TK_SRWU TK_MVI | TK_DTRWR DNFF 'K' TK_HLJ UXESO | AHRM TRL SRVI | ;

UCKQB: DNFF D I OGR | SRVI | AHRM DNFF 'Y' VYJU | TK_IEDA;

LP: G UXESO AHRM G | SRVI I | 'Q' TK_SRWU TK_TGKU TK_SRWU TK_BBZ | TK_PRGF TK_DTRWR;

MGOV: TK_TGKU | TK_PRGF | UCKQB 'H' OGR DNFF VZKQM | 'Y' AHRM TK_TGKU OGR TK_KL | TK_MVI TK_ERGO 'Y' C;

TRL: TK_TGKU VYJU 'H' TK_KL | 'H' | UXESO 'Y' C | TK_ERGO TK_DTRWR TK_PRGF SRVI VZKQM | UCKQB TK_ERGO TK_TGKU;

