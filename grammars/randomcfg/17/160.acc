%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: 'H' AHRM;

VYJU: TK_OM G C TK_IEDA | TK_HLJ;

VZKQM: TK_IEDA TK_TGKU TRL TK_OM MGOV | G | I | 'Q' 'K';

DNFF: UXESO TK_BBZ 'Q';

UXESO: UCKQB SWRI | TK_HLJ 'H' | TK_BBZ MGOV | C;

SWRI: 'Q' TK_RBS TK_BBZ TK_MVI TK_HLJ | C TK_OM C AHRM 'Y';

G: TK_HLJ TK_OM UXESO | MGOV SWRI SWRI TK_IEDA | VZKQM TK_TGKU MGOV OGR UXESO | C TK_PRGF | G G SWRI 'Q';

SRVI: 'H' TK_TGKU LP 'K' TK_PRGF | TK_ERGO | C C;

C: TK_SRWU UCKQB I | D TRL TK_ERGO UXESO | I TK_BBZ C D;

D: SRVI I TK_HLJ TK_OM 'Y' | SWRI AHRM TK_IEDA TK_MVI | 'Y' | TK_OM TK_OM | VYJU TK_PRGF TK_OM TK_PRGF TK_TGKU;

OGR: SRVI;

AHRM: C OGR TK_ERGO DNFF;

I: SRVI | C SRVI TK_TGKU | TK_HLJ D VZKQM SWRI OGR | 'W';

UCKQB: TK_SRWU 'H' 'Y';

LP: 'H' G VZKQM TK_RBS TK_SRWU | TK_RBS TK_DTRWR D TK_SRWU | SWRI 'W' TK_MVI 'K' 'W' | 'Y' 'W' TK_HLJ 'H';

MGOV: I VYJU TK_HLJ AHRM SRVI | D I TK_PRGF TK_KL | TK_TGKU SRVI TK_PRGF SRVI | ;

TRL: VYJU TK_ERGO TK_SRWU 'Q' | TK_MVI UCKQB TK_TGKU TK_MVI VYJU | ;

