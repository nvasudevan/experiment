%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: SWRI C TK_MVI;

VYJU: TK_OM 'Q' TK_SRWU UXESO;

VZKQM: TK_TGKU UCKQB LP TK_OM | G | TRL G LP I | TK_KL 'W';

DNFF: TK_KL TK_OM UXESO;

UXESO: TK_ERGO AHRM UCKQB VZKQM | ;

SWRI: 'H' TK_ERGO TRL TK_KL TK_HLJ | D TRL | I 'W' 'Y' TK_SRWU | AHRM TK_DTRWR C;

G: TK_RBS AHRM TK_ERGO TK_SRWU TK_OM;

SRVI: VYJU D 'K' | UCKQB TK_IEDA G TK_DTRWR | VYJU;

C: TK_ERGO UXESO 'Y' | ;

D: 'H' SRVI C UCKQB DNFF | UCKQB 'W' 'K' | TK_SRWU TK_HLJ 'W' C TRL | SRVI VYJU TK_RBS;

OGR: TRL TRL | TK_DTRWR TK_KL D AHRM | LP SWRI SWRI TK_PRGF UCKQB | TK_KL | G TK_TGKU;

AHRM: SRVI 'Y' | C TK_PRGF UXESO | 'Y' UCKQB TK_ERGO TK_PRGF 'Q' | TRL UCKQB 'Q' | SWRI;

I: TK_SRWU UXESO;

UCKQB: 'Y' AHRM TK_DTRWR | C VZKQM VYJU TK_PRGF | SRVI UCKQB VYJU 'Y' | TK_BBZ TK_IEDA I 'Y' 'H';

LP: TK_ERGO TK_PRGF UXESO | SWRI | TK_OM 'Y' SRVI;

MGOV: TK_SRWU DNFF;

TRL: OGR I D LP VZKQM;

