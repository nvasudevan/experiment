%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_TGKU UCKQB 'Q' DNFF;

VYJU: UCKQB UXESO 'Q' D;

VZKQM: SWRI SRVI UXESO | DNFF;

DNFF: TK_DTRWR TK_TGKU I | 'Q' | ;

UXESO: C TRL UCKQB D SRVI | TK_TGKU 'H';

SWRI: TK_KL TK_OM TK_BBZ UCKQB OGR | 'K' UCKQB | UXESO 'H' 'Q' TK_IEDA TK_OM | LP TK_BBZ AHRM UXESO;

G: TK_RBS TK_PRGF TK_HLJ UXESO | G;

SRVI: SRVI 'K' D TK_DTRWR | UXESO;

C: TK_PRGF | 'W' TK_RBS TK_DTRWR G VZKQM;

D: VYJU TK_PRGF TK_OM 'H' | TK_TGKU 'Q' TK_HLJ 'W' | D TK_RBS | TK_DTRWR 'H' TK_OM I VYJU | SWRI TRL UXESO;

OGR: TK_BBZ 'W' TK_ERGO TRL MGOV;

AHRM: TK_HLJ OGR TK_DTRWR TK_RBS 'Y' | TK_IEDA UXESO;

I: VYJU | TK_KL TK_HLJ | 'K' | TK_OM TK_PRGF | ;

UCKQB: TRL | 'K' TK_OM | TK_DTRWR 'H' DNFF | UCKQB TRL | VZKQM TK_HLJ;

LP: TK_PRGF TK_OM TK_TGKU;

MGOV: TK_BBZ C TK_IEDA | UXESO | OGR 'H' TK_KL;

TRL: 'Y' LP TK_ERGO | TK_ERGO | D SRVI | 'Y' | 'W' D TK_MVI;

