%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: SRVI TK_PRGF TK_IEDA | C UCKQB TK_IEDA;

VYJU: 'Y' I LP DNFF | VZKQM SRVI SRVI TK_BBZ TK_MVI | TRL TK_MVI | TK_SRWU TK_DTRWR TK_TGKU;

VZKQM: VYJU G OGR 'K' 'K' | TK_ERGO VYJU OGR | ;

DNFF: DNFF | G UXESO | SRVI I | VYJU TK_KL | 'Y' 'Y';

UXESO: TK_OM TRL SWRI D;

SWRI: DNFF | TK_HLJ | 'Y' VYJU AHRM 'H';

G: AHRM VYJU TK_SRWU SRVI 'K' | TRL | 'K' AHRM G DNFF | UCKQB | TK_OM TK_SRWU;

SRVI: TRL | VYJU TK_DTRWR AHRM TK_HLJ | AHRM UXESO;

C: TK_IEDA | OGR UCKQB LP UXESO MGOV | UXESO MGOV LP G;

D: 'Y' TK_DTRWR TK_MVI I | G | MGOV UCKQB TK_RBS VZKQM TK_SRWU | TK_TGKU SRVI MGOV 'H';

OGR: TK_OM TK_RBS | ;

AHRM: 'H' | SWRI LP;

I: DNFF VZKQM TK_OM | AHRM G | TK_SRWU TK_BBZ UXESO VYJU | 'Q' TK_RBS;

UCKQB: C DNFF SWRI VZKQM | 'K' | AHRM;

LP: DNFF TK_PRGF TK_SRWU | LP TK_KL MGOV | I OGR;

MGOV: SWRI C | TK_SRWU OGR C SWRI;

TRL: DNFF 'Q' UCKQB TK_HLJ TK_KL | TK_MVI UXESO 'Y';

