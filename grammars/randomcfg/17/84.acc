%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_TGKU | DNFF TK_RBS 'H' TK_DTRWR AHRM | TK_ERGO 'H' TK_RBS | TK_RBS AHRM 'H';

VYJU: TK_BBZ TK_TGKU TRL MGOV C | TK_OM VZKQM SWRI VYJU 'H' | ;

VZKQM: 'K' I OGR | ;

DNFF: MGOV G 'K' D;

UXESO: TK_HLJ AHRM TK_IEDA | I VZKQM | SWRI 'K' UXESO I | UXESO | TK_HLJ AHRM;

SWRI: TK_DTRWR TK_TGKU SRVI VZKQM TK_IEDA | LP AHRM;

G: 'K' TK_SRWU;

SRVI: TK_ERGO 'Q' | OGR | 'Y' TK_OM TK_SRWU | DNFF UXESO I 'H' | TK_RBS;

C: G TK_HLJ DNFF OGR D | UXESO TK_OM | MGOV TK_IEDA C D | 'Q' SRVI;

D: MGOV TK_OM OGR VYJU 'Q' | UXESO | TK_TGKU C TK_KL TK_TGKU | I SRVI 'W' | SRVI MGOV VZKQM TK_DTRWR;

OGR: UCKQB | 'Q' OGR LP TK_KL I | TK_PRGF AHRM TK_ERGO TK_OM | TK_KL SRVI 'W';

AHRM: G LP G DNFF | TK_KL TK_DTRWR TK_SRWU D;

I: TK_IEDA UXESO TK_KL | DNFF;

UCKQB: 'H' 'K' OGR TK_IEDA | TK_BBZ MGOV 'Y' 'W' 'Y';

LP: 'K' SRVI;

MGOV: TK_ERGO G TK_BBZ;

TRL: DNFF TK_ERGO AHRM | TK_MVI OGR D AHRM;

