%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_ERGO TK_TGKU TK_PRGF;

VYJU: 'Y' LP TK_KL | VZKQM;

VZKQM: TK_BBZ TK_ERGO 'Q' | TK_SRWU TK_SRWU TK_DTRWR 'W' SRVI | TK_PRGF 'Q' SWRI SRVI | 'Q' SRVI TK_SRWU;

DNFF: TK_BBZ 'H' TK_PRGF TK_OM | TK_SRWU TK_HLJ 'Y' | G TK_ERGO;

UXESO: C SRVI 'K' AHRM TK_BBZ | VYJU VYJU 'W' VZKQM 'Y' | 'Y' DNFF TK_SRWU | TK_ERGO TK_PRGF 'K';

SWRI: C AHRM | I VYJU SRVI SWRI DNFF | D TK_MVI TK_BBZ | TRL VZKQM TK_ERGO AHRM | ;

G: TK_ERGO TK_TGKU UXESO VZKQM | TK_RBS TK_OM I TK_DTRWR VZKQM | G 'W' | MGOV SRVI;

SRVI: TK_TGKU TK_KL TK_BBZ | TK_DTRWR AHRM 'H' TK_BBZ;

C: TK_MVI TK_KL UXESO TK_HLJ | I TK_ERGO SWRI | TK_SRWU;

D: VZKQM OGR 'W' VZKQM UXESO | UCKQB | UXESO 'H' UXESO LP;

OGR: G | LP TK_SRWU 'Q' OGR | TK_TGKU SWRI TK_BBZ AHRM TK_RBS | AHRM TK_MVI | TK_KL SRVI;

AHRM: TK_DTRWR 'Y' SWRI | ;

I: 'W' I | TK_TGKU TRL VZKQM VYJU 'Q' | 'Q' UXESO TK_PRGF AHRM | 'W' | TK_KL;

UCKQB: TK_ERGO TK_IEDA 'K' | LP UXESO | C LP 'W' MGOV TRL | VZKQM | 'K' 'W';

LP: UCKQB VZKQM TK_PRGF;

MGOV: AHRM 'Y' DNFF 'W' C | D TK_IEDA | TK_RBS DNFF UXESO AHRM C;

TRL: UCKQB;

