%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: 'Y' SRVI 'H' | MGOV | 'H' 'W' | G 'W' TRL;

VYJU: TK_IEDA | 'W' | DNFF TK_IEDA TK_DTRWR;

VZKQM: OGR MGOV 'W';

DNFF: TK_MVI 'H' 'Y' VZKQM C | G | 'K' | C DNFF TK_MVI TK_BBZ 'Q';

UXESO: 'H' TK_RBS | TK_KL TK_HLJ 'Q' | TK_OM VYJU;

SWRI: TK_ERGO LP | TK_SRWU | TK_TGKU 'Y' | C SWRI C TK_RBS | TK_BBZ TK_TGKU 'W' SWRI;

G: SRVI LP I | SWRI TK_OM VYJU DNFF | TK_SRWU TK_BBZ VZKQM 'Y' SWRI | TK_TGKU 'H' D AHRM TK_IEDA | ;

SRVI: TK_PRGF G | TK_DTRWR I 'K' D C | TK_OM 'W' TK_SRWU | DNFF VZKQM | SWRI UXESO 'W';

C: OGR | OGR 'H' | TK_IEDA G | TK_MVI UCKQB TK_KL;

D: UXESO UCKQB | 'K' TK_MVI | OGR SWRI | ;

OGR: OGR TK_HLJ D D | TRL I TK_BBZ TK_OM | D VYJU 'H' C 'W';

AHRM: TK_TGKU UXESO | TK_KL D TRL LP;

I: TK_IEDA | VZKQM D 'W' | TK_IEDA D TK_SRWU;

UCKQB: TK_BBZ 'W' | TK_MVI TK_PRGF | TK_BBZ TK_SRWU TK_DTRWR 'W';

LP: 'H' D TK_SRWU | TK_HLJ I;

MGOV: TK_SRWU TK_HLJ 'H' DNFF SRVI | TK_HLJ D SWRI;

TRL: TRL 'W' 'W' | TK_BBZ | TRL 'Q' C;

