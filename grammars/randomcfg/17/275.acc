%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: SRVI | LP | TK_RBS MGOV;

VYJU: VZKQM LP TK_HLJ TK_RBS TK_PRGF | UXESO | TK_PRGF VZKQM MGOV TK_ERGO | I 'Q';

VZKQM: TK_PRGF LP VZKQM SWRI I | TK_RBS TK_SRWU 'Q' MGOV TK_TGKU | DNFF MGOV TK_SRWU UXESO | ;

DNFF: TK_BBZ | I 'H' AHRM | 'Y' TK_KL 'W';

UXESO: TK_IEDA VYJU TK_SRWU VZKQM | I TK_DTRWR LP AHRM TK_KL | TK_PRGF SRVI TK_KL TK_SRWU D | MGOV 'H' 'W' 'W';

SWRI: 'Q' UXESO DNFF AHRM SRVI | TK_HLJ G | TK_DTRWR TK_IEDA TK_SRWU UXESO TK_KL | VZKQM TK_PRGF TK_TGKU;

G: 'W' 'Y' VYJU | TK_PRGF | UCKQB TK_KL TK_BBZ TK_SRWU | UCKQB I TRL D | ;

SRVI: TK_IEDA | TK_IEDA TK_BBZ TK_SRWU TK_BBZ | TK_RBS | ;

C: AHRM D TK_IEDA TK_SRWU G;

D: TK_KL TK_SRWU DNFF D | TK_DTRWR | VYJU TK_IEDA DNFF D;

OGR: LP TK_KL VYJU | TK_KL TK_DTRWR VYJU | TK_TGKU OGR D | TK_KL TK_IEDA TK_OM TRL;

AHRM: TK_HLJ 'K' SRVI 'Y' | 'W' UXESO MGOV 'Q' | C TK_IEDA TK_OM OGR VZKQM | TK_PRGF UCKQB TK_RBS;

I: SRVI TK_ERGO TK_OM DNFF TK_PRGF | MGOV TK_OM | TK_OM TK_KL I UXESO 'K' | TK_SRWU TK_IEDA;

UCKQB: SRVI TK_KL 'H' VYJU LP | SRVI TRL TK_SRWU | UXESO TK_KL G VYJU TK_BBZ;

LP: TK_MVI TK_SRWU OGR TK_RBS | TK_PRGF | VYJU TK_HLJ 'Y' 'Y' TK_ERGO | 'Y' UCKQB TK_RBS UXESO | 'Q' TK_PRGF DNFF;

MGOV: TK_BBZ LP TK_DTRWR TK_OM DNFF | 'Y' 'Y' MGOV 'W' | 'K' C TK_HLJ TK_MVI | 'H' AHRM;

TRL: TK_HLJ DNFF SRVI | TK_SRWU TK_PRGF | 'Y' TK_RBS | 'W' 'Q' UCKQB | TK_OM;

