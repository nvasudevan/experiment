root: TK_OM | C | VZKQM TK_IEDA TK_OM;

VYJU: MGOV C TK_SRWU UCKQB TK_IEDA | 'K' AHRM TK_OM TK_TGKU VZKQM | TK_TGKU | TK_BBZ C 'Y' VYJU 'W';

VZKQM: TK_ERGO;

DNFF: TK_SRWU | 'W' SRVI UCKQB 'W' | TK_MVI 'H' | TK_OM G I TK_TGKU | I 'W' 'Y';

UXESO: UCKQB UCKQB | TRL I DNFF D TK_SRWU | TK_SRWU | SWRI SRVI VZKQM VYJU 'K';

SWRI: TK_ERGO VYJU 'K' 'Y';

G: TK_IEDA TK_KL TK_SRWU | 'W' 'Y' | D VZKQM TK_KL 'H' | C 'Q' SWRI | VYJU OGR;

