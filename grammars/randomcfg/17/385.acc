%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_PRGF 'Y' | VYJU TRL UCKQB | TK_DTRWR TK_BBZ VYJU 'Y' LP;

VYJU: TK_SRWU TK_OM I | G OGR UCKQB | AHRM 'Q' 'H' LP;

VZKQM: TK_DTRWR DNFF | UCKQB OGR | TK_SRWU G | TK_KL TK_HLJ DNFF;

DNFF: TK_OM UXESO TK_ERGO 'Y' 'H' | SWRI D TRL | TK_SRWU TK_RBS D | LP | ;

UXESO: TK_OM | UXESO SWRI | I | G SWRI | TK_PRGF TK_RBS UCKQB TK_TGKU;

SWRI: TK_HLJ TK_SRWU C | 'H';

G: TK_DTRWR TK_KL 'W' TK_ERGO;

SRVI: VZKQM 'H' 'Q';

C: VYJU UXESO I | TK_ERGO MGOV DNFF | DNFF I TK_IEDA;

D: 'Q' SWRI UCKQB 'Y' | TK_MVI | TK_SRWU UCKQB | AHRM DNFF 'Y' 'H';

OGR: G 'K' 'H' | ;

AHRM: UXESO SRVI G MGOV TK_BBZ | TK_HLJ TK_TGKU TK_ERGO TK_DTRWR;

I: UCKQB TK_RBS TK_HLJ | OGR D TK_IEDA TK_DTRWR 'Y' | UXESO VYJU TK_OM TK_DTRWR UXESO | I TK_ERGO TK_SRWU AHRM | OGR 'K' DNFF;

UCKQB: UCKQB | TRL C LP;

LP: UCKQB | D TK_BBZ SRVI | 'H' TK_KL TK_ERGO SWRI TK_BBZ | DNFF 'K' MGOV;

MGOV: TK_SRWU VYJU AHRM TRL 'W' | TK_IEDA C LP | UCKQB TK_OM 'W' | TK_HLJ | 'W';

TRL: LP C | TK_RBS VYJU TK_RBS DNFF TK_BBZ | G TK_MVI TK_OM TRL 'Y' | 'Q' UCKQB TK_PRGF | TK_IEDA TK_IEDA MGOV TRL;

