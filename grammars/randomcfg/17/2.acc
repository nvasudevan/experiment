%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: MGOV | TK_TGKU TK_DTRWR TK_ERGO TK_SRWU TK_SRWU | MGOV I;

VYJU: 'H' | AHRM | SRVI OGR AHRM | TK_OM OGR TRL TK_KL TK_SRWU;

VZKQM: OGR TK_DTRWR LP UCKQB TK_DTRWR | TK_ERGO SRVI VZKQM VYJU | TK_DTRWR TK_HLJ TK_DTRWR LP | SRVI UXESO TK_DTRWR I | TK_OM D 'W';

DNFF: TK_TGKU I TK_BBZ TK_OM | 'Q' 'K';

UXESO: TK_RBS | VYJU OGR AHRM TK_DTRWR SWRI;

SWRI: VZKQM 'W' TK_ERGO | D;

G: TK_MVI 'K' 'H' | LP DNFF TRL D OGR | TK_RBS TK_PRGF | TK_MVI TK_SRWU TK_PRGF OGR UXESO | I I TK_BBZ;

SRVI: TK_KL TK_HLJ | TK_IEDA TK_BBZ AHRM | 'Q' | G 'W' LP MGOV TK_ERGO | ;

C: TK_ERGO UXESO D | TK_OM TK_DTRWR TK_BBZ 'K' AHRM;

D: 'H' TK_RBS | OGR SWRI 'H' 'Q' | MGOV OGR 'W' VZKQM | 'H' SRVI | TK_KL;

OGR: 'H' TK_BBZ;

AHRM: 'K' TK_PRGF SRVI SRVI 'W' | I UCKQB TK_KL G SWRI | TK_IEDA 'Q' I;

I: DNFF | I UXESO OGR | UCKQB TK_KL SRVI;

UCKQB: VYJU TK_IEDA TK_KL;

LP: TK_SRWU DNFF OGR TK_SRWU | LP TK_RBS | TK_HLJ DNFF MGOV TK_OM | SRVI C | C TK_OM C VZKQM;

MGOV: UCKQB 'H' | TK_HLJ | ;

TRL: TK_BBZ TK_BBZ G | 'Q' UCKQB SRVI | 'Q' TK_MVI TK_DTRWR 'Q';

