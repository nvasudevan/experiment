AHRM: TK_DTRWR TK_TGKU | SRVI 'H' | OGR;

I: LP | TK_OM | LP TK_TGKU | TRL DNFF G TK_PRGF | TK_BBZ TK_DTRWR OGR VZKQM TK_OM;

UCKQB: 'H' | VZKQM TK_DTRWR 'K' TK_ERGO 'K' | 'Y' C UCKQB | TK_ERGO 'Q';

LP: DNFF TK_SRWU;

MGOV: TK_SRWU UCKQB TK_KL TK_ERGO VZKQM | VYJU | D TK_ERGO 'Y' | 'Q' TK_ERGO TK_MVI 'K' TK_IEDA;

TRL: UXESO TK_PRGF UCKQB 'W' I;

