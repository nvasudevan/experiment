%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_MVI D 'H' 'K';

VYJU: TK_MVI 'H' | TK_OM;

VZKQM: TRL TK_BBZ 'K' TK_OM | D I | C | TK_ERGO TK_ERGO G TK_SRWU AHRM | LP SWRI TRL TK_SRWU;

DNFF: 'Q' 'Y' SWRI UCKQB | TRL TK_KL TK_BBZ | I TK_SRWU | TK_DTRWR C;

UXESO: TK_KL;

SWRI: SWRI 'Y' TK_ERGO C | TK_ERGO VZKQM I TK_MVI UXESO | C TK_KL TRL;

G: TK_BBZ TK_DTRWR C | LP TK_PRGF TK_ERGO MGOV | ;

SRVI: LP TK_ERGO | ;

C: TRL SRVI TK_BBZ | TK_PRGF TRL TK_IEDA SRVI | VZKQM C VYJU 'W' | D;

D: 'Y' TK_BBZ | UCKQB LP 'Y' C OGR;

OGR: TK_PRGF D D | TK_BBZ TK_RBS TK_ERGO 'Y' | DNFF 'Y' 'Y' TK_PRGF | TK_TGKU VZKQM G | TK_HLJ UXESO TK_MVI SWRI;

AHRM: 'K' TK_KL | 'W' TK_BBZ | VZKQM;

I: OGR TK_KL | TK_MVI TK_KL TK_KL 'W' 'H' | 'Y' VZKQM TK_SRWU | D;

UCKQB: C TK_SRWU G | 'W' | G VZKQM 'W' UXESO SWRI | C C TRL SRVI D;

LP: 'Q' 'H' UXESO TK_OM TK_OM | OGR TK_RBS TK_PRGF | LP SRVI UXESO | TK_BBZ DNFF I | OGR 'H' VZKQM 'Y';

MGOV: TK_OM VZKQM | SWRI VYJU C UCKQB TK_BBZ | LP TK_HLJ MGOV | TK_TGKU TRL;

TRL: 'Y' TK_DTRWR;

