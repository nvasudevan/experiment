D: C TK_TGKU TRL OGR DNFF | I C | AHRM VZKQM | C VZKQM;

OGR: TK_RBS 'H' SRVI | TK_HLJ;

AHRM: DNFF | MGOV;

I: DNFF TK_PRGF C | OGR OGR TK_IEDA UCKQB | TK_BBZ TK_OM VYJU TK_RBS | 'H' SRVI TK_RBS TK_DTRWR | VZKQM TK_MVI;

UCKQB: I | TRL DNFF G G | OGR | TK_HLJ | VYJU 'Q' I LP;

LP: C SRVI OGR | TRL DNFF VYJU | I SWRI VZKQM | TK_SRWU TK_OM TK_PRGF LP | ;

MGOV: LP TK_IEDA LP 'K' | TK_KL | SRVI | D TRL 'K' C TK_PRGF;

TRL: TK_RBS TK_OM | LP TK_SRWU 'H' I SWRI | TK_OM MGOV | VYJU SRVI | VYJU;

