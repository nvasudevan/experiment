%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: I | I LP | TK_KL VZKQM | I TK_PRGF TK_PRGF 'Q';

VYJU: DNFF TK_ERGO SWRI | VZKQM TK_DTRWR TK_KL | 'Q' TK_KL TK_SRWU SWRI | SRVI | 'H' TK_OM TK_OM TK_TGKU TK_KL;

VZKQM: TK_KL 'W' 'Q' | I TK_PRGF VYJU TK_MVI LP;

DNFF: TK_IEDA 'Y' TK_TGKU | DNFF TK_TGKU;

UXESO: 'Y' 'W' VZKQM;

SWRI: G AHRM 'Q' UCKQB | TK_SRWU I G | C TK_PRGF;

G: 'Q' TRL VZKQM SRVI | 'K' VYJU TRL TK_IEDA 'H' | 'W' UXESO | TK_TGKU TK_HLJ;

SRVI: TK_HLJ TK_IEDA TK_MVI I | 'W' DNFF UCKQB TK_DTRWR SWRI | VYJU TK_SRWU DNFF | G VZKQM TK_KL;

C: MGOV UCKQB TK_BBZ | TRL OGR D UCKQB | SWRI | LP TK_PRGF C;

D: 'H' 'Y';

OGR: TRL 'H' TK_MVI TK_BBZ TK_ERGO | UXESO LP VYJU TK_KL TK_DTRWR | TK_OM TK_KL UXESO 'H' | TK_SRWU 'H' D;

AHRM: TK_BBZ | TK_ERGO 'H' | TK_KL 'Q' TK_KL | TK_MVI TK_IEDA DNFF TK_SRWU UXESO | ;

I: I 'Q' TK_RBS G TK_ERGO | 'Y' | UXESO TK_TGKU OGR OGR;

UCKQB: TK_PRGF | UXESO VZKQM DNFF | TK_IEDA TK_SRWU UXESO | TK_ERGO MGOV 'H' MGOV TK_SRWU;

LP: TK_HLJ TK_SRWU TK_KL 'H' | TK_BBZ UCKQB TK_PRGF | TK_PRGF TRL C;

MGOV: G TK_HLJ | LP G | D UXESO | ;

TRL: TK_ERGO D | SWRI G TRL D TK_KL;

