%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: UXESO VZKQM;

VYJU: MGOV TK_SRWU | LP TK_SRWU | TK_SRWU;

VZKQM: AHRM | LP 'K' | TK_RBS TRL | VZKQM;

DNFF: AHRM 'H' I | ;

UXESO: 'W' D | TK_IEDA TK_BBZ I;

SWRI: TK_PRGF I | TK_RBS;

G: TK_OM OGR UXESO | TK_ERGO SWRI TK_MVI | TK_MVI LP TK_MVI TK_RBS TK_PRGF | I AHRM TK_TGKU | ;

SRVI: VZKQM AHRM TK_IEDA | TK_IEDA LP TK_MVI D 'K' | AHRM OGR TK_DTRWR OGR | SWRI UXESO SWRI 'W' TK_BBZ;

C: 'Q' | UXESO TK_TGKU;

D: 'H' 'W' D TK_KL | TK_HLJ | TK_TGKU UCKQB;

OGR: MGOV TK_RBS | TK_KL TK_HLJ;

AHRM: TK_KL;

I: D | TK_BBZ TK_HLJ TK_RBS UXESO | I LP TK_OM 'Y' 'Q';

UCKQB: 'Y';

LP: TK_ERGO 'Q' 'K' | TK_IEDA TK_BBZ | TK_ERGO 'H' | LP LP DNFF;

MGOV: VYJU TK_SRWU TRL;

TRL: TK_IEDA TK_RBS VYJU MGOV 'Q' | G 'W' | 'Q' 'Q' AHRM VZKQM TRL | SRVI 'W' TK_HLJ 'H' MGOV;

