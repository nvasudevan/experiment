%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: OGR UXESO TK_DTRWR SRVI 'K' | UCKQB SWRI C TK_RBS;

VYJU: 'K' I AHRM 'K' SRVI | TK_MVI UXESO TK_PRGF D | TK_HLJ | ;

VZKQM: TK_MVI;

