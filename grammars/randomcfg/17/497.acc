%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: SRVI | SWRI TK_RBS TK_BBZ TK_KL G | 'H' | TK_TGKU MGOV VZKQM | 'Q';

VYJU: AHRM D TK_MVI C TK_HLJ | DNFF TK_DTRWR 'K' MGOV | TK_RBS 'H' | C TK_IEDA DNFF | LP C I VYJU OGR;

VZKQM: AHRM TK_PRGF VYJU MGOV | TK_IEDA TK_IEDA | TK_IEDA 'H' | SRVI | UXESO TK_DTRWR SWRI 'Y';

DNFF: G 'W' TK_SRWU VZKQM | 'Y' TK_DTRWR | TK_OM | TK_MVI VZKQM G D | TK_MVI D TRL;

UXESO: UXESO TRL AHRM AHRM UXESO | DNFF TK_SRWU I SWRI | C 'Q' | 'Y' UCKQB | DNFF UXESO TRL AHRM;

SWRI: TK_TGKU DNFF SWRI | SWRI TK_ERGO | G 'K' | 'Y';

G: 'H' TK_DTRWR UXESO 'W' | 'W' TK_OM TRL UCKQB TK_TGKU | AHRM D SWRI UCKQB D | G VZKQM TK_TGKU DNFF TK_KL | ;

SRVI: TK_SRWU | TK_TGKU | UXESO VYJU 'H' MGOV TK_KL | TRL;

C: TK_SRWU | SWRI OGR LP | LP AHRM 'K' TK_PRGF D | I SWRI VZKQM C TRL;

D: TK_KL TRL 'Q' | TK_ERGO | 'K' | SWRI TK_TGKU MGOV;

OGR: MGOV | TK_KL TK_SRWU UXESO 'H' | 'Y' LP 'W' TK_OM | TK_KL TK_HLJ TK_DTRWR;

AHRM: TK_PRGF | TK_OM TK_DTRWR AHRM TK_RBS | 'H' DNFF | LP OGR UXESO VYJU AHRM | ;

I: G D SRVI | TK_OM | 'Q';

UCKQB: 'Y' TK_PRGF I | 'K' DNFF 'H' TK_SRWU MGOV | G UCKQB | TK_OM TRL VZKQM SRVI 'H' | C DNFF TK_MVI MGOV;

LP: LP 'Q' | 'Y' | ;

MGOV: TK_KL SRVI TK_BBZ C | D AHRM TK_PRGF TRL 'W' | 'Y' SRVI TK_BBZ TK_OM VZKQM | TK_BBZ 'H';

TRL: TK_KL TK_MVI UXESO 'W' TK_MVI | TK_HLJ TK_DTRWR TK_DTRWR TK_IEDA | AHRM LP TK_ERGO;

