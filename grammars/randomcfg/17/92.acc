%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: AHRM | AHRM I TK_KL 'Y' | TK_TGKU VZKQM | LP MGOV DNFF TK_TGKU TK_DTRWR;

VYJU: TK_KL | TK_IEDA D;

VZKQM: C 'H';

DNFF: TK_IEDA 'W' TK_PRGF TK_TGKU UXESO | SRVI;

UXESO: TK_HLJ UXESO TK_RBS | UCKQB TK_OM UXESO G | TK_BBZ TK_PRGF 'W' | SWRI;

SWRI: VYJU TK_HLJ MGOV | MGOV TRL D TK_IEDA | C TK_DTRWR OGR | MGOV 'K';

G: TK_HLJ TK_DTRWR TK_ERGO | TK_OM I TK_BBZ | ;

SRVI: TK_DTRWR G MGOV TK_RBS G | 'Q' DNFF SRVI TK_KL I | VYJU SWRI OGR TK_MVI;

C: OGR 'Q' MGOV | 'Q' AHRM VYJU D TK_BBZ;

D: I | 'W' TK_ERGO;

OGR: VYJU 'Y' SWRI TK_HLJ TK_RBS | TK_KL 'K';

AHRM: 'W' | ;

I: MGOV SRVI C LP DNFF;

UCKQB: TK_RBS | TK_IEDA TK_HLJ AHRM 'W' VYJU;

LP: TK_OM 'Q' | UXESO I LP TK_PRGF TK_OM | 'K' 'K' 'Q' SWRI 'H';

MGOV: D D SWRI I | TK_DTRWR VYJU AHRM SWRI TK_SRWU | TK_OM;

TRL: TK_PRGF | TK_IEDA 'Q' | TK_IEDA | TK_PRGF TK_MVI 'W' | TK_OM G C;

