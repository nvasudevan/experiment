%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: D TK_DTRWR 'Q' | 'W' TK_TGKU | 'H' TK_ERGO | TK_HLJ | SRVI C DNFF;

VYJU: VZKQM | TK_ERGO D 'W' 'K' | 'Q' G TK_DTRWR TK_MVI TK_TGKU;

VZKQM: 'Y' | D VYJU LP | TK_PRGF DNFF DNFF C | UXESO G TK_TGKU DNFF 'H' | I I SRVI TK_OM OGR;

DNFF: LP TK_RBS OGR TRL;

UXESO: TK_OM I TK_TGKU TK_IEDA;

SWRI: G TK_PRGF TK_IEDA | TK_ERGO TK_IEDA TK_ERGO;

G: C MGOV I | TK_TGKU SWRI;

SRVI: TRL OGR C G 'K' | ;

C: SRVI 'Y' VYJU MGOV DNFF;

D: TK_DTRWR UCKQB TK_ERGO TK_DTRWR | 'W' MGOV | TK_OM UCKQB | C;

OGR: UXESO TK_HLJ 'Q' | C;

AHRM: AHRM 'Q' 'K' LP TRL | TRL | TK_MVI TK_TGKU VYJU LP;

I: SRVI MGOV G 'Y' | G | TK_OM;

UCKQB: VZKQM D I SRVI | TK_MVI TK_SRWU TK_DTRWR | TK_HLJ | G | TK_TGKU TK_TGKU MGOV 'W';

LP: G LP TK_KL TK_DTRWR D | MGOV LP TK_MVI TK_KL | AHRM OGR TK_DTRWR;

MGOV: 'K' | C SRVI TK_TGKU | 'K' LP | ;

TRL: 'K' TK_TGKU D TRL | SRVI TK_RBS TK_TGKU;

