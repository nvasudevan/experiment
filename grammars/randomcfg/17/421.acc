MGOV: 'K' MGOV VZKQM 'Q' | MGOV VYJU 'W' 'Y' 'Y' | TK_TGKU TK_KL 'W' TRL | LP TK_MVI 'W' 'K' OGR;

TRL: TK_HLJ D | 'Y' | TK_BBZ TK_BBZ TK_OM | DNFF VZKQM TK_ERGO TK_MVI TK_ERGO;

