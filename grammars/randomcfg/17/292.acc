%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: I G D SWRI TK_ERGO | TK_BBZ | TK_DTRWR TK_DTRWR TK_HLJ;

VYJU: TK_TGKU D I UXESO | TK_DTRWR | UCKQB G UCKQB | 'W' | TK_ERGO;

VZKQM: DNFF;

DNFF: TK_RBS VYJU OGR UCKQB AHRM | OGR 'H' TK_MVI TK_ERGO OGR | ;

UXESO: SWRI C 'Q' | TRL | TK_OM TK_ERGO OGR | ;

SWRI: TK_TGKU VYJU | 'Q' | LP TK_BBZ 'Y' AHRM TK_BBZ | TK_OM TK_PRGF | OGR;

G: SRVI TK_RBS LP MGOV MGOV | C DNFF UXESO | TK_BBZ TK_SRWU TK_ERGO TK_BBZ;

SRVI: 'W' TK_TGKU D TK_OM TK_PRGF | D C | TK_OM TK_TGKU | UXESO;

C: SWRI TK_OM LP TK_HLJ | 'Y' TK_IEDA TK_TGKU;

D: 'H' TK_TGKU 'H' | MGOV TK_RBS TRL | AHRM 'Y' | AHRM VYJU | TK_TGKU UXESO;

OGR: TK_PRGF VYJU | TK_HLJ I | SRVI TK_KL DNFF;

AHRM: SWRI 'W' | 'H' | TK_SRWU | 'Y' DNFF TK_DTRWR TK_DTRWR TK_HLJ;

I: TK_IEDA UXESO TK_OM | DNFF TK_PRGF TK_HLJ AHRM;

UCKQB: 'Q' TK_RBS LP TRL G | 'Y' D TRL D | SRVI TK_MVI 'K' 'Y' | TRL TK_IEDA | MGOV SWRI TK_TGKU TK_DTRWR;

LP: AHRM TK_MVI 'K' G | AHRM TK_DTRWR I TK_HLJ | D DNFF TK_SRWU SRVI D;

MGOV: G TK_HLJ | UCKQB SRVI TK_PRGF | 'Q';

TRL: TK_HLJ | TK_OM D TK_OM;

