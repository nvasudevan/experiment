%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: I TK_MVI AHRM TK_PRGF G | TK_SRWU DNFF I | G 'K' TK_RBS TK_SRWU TK_TGKU | G;

VYJU: SRVI TK_RBS TK_RBS TK_IEDA OGR | TK_KL MGOV I TK_PRGF | TK_IEDA SRVI | TK_PRGF SWRI TRL TK_OM TK_SRWU;

VZKQM: 'H' AHRM VYJU I;

DNFF: TK_DTRWR TK_BBZ SRVI | TK_RBS LP SRVI | 'H' | ;

UXESO: TK_IEDA TK_IEDA TRL TK_SRWU | TK_DTRWR TK_MVI 'Y' AHRM;

SWRI: VYJU 'Q' 'W' 'Y' OGR | OGR | 'H' C MGOV TRL | TK_DTRWR TRL 'W' I OGR | ;

G: DNFF TK_SRWU TK_PRGF TK_OM LP | D D VZKQM TK_HLJ 'Q' | OGR | UXESO LP AHRM OGR | 'H';

SRVI: TK_MVI UXESO 'W';

C: TK_BBZ VZKQM TK_RBS UXESO D | TK_IEDA;

D: TK_RBS C;

OGR: LP TK_MVI 'H' TK_HLJ 'K' | SWRI VZKQM TK_ERGO VZKQM 'K' | TK_ERGO;

AHRM: TRL 'Y' TK_BBZ TK_PRGF TK_HLJ | TK_TGKU 'W' TK_IEDA C | TK_ERGO;

I: AHRM TK_HLJ SRVI | UCKQB TK_RBS 'W' TK_MVI;

UCKQB: TK_SRWU MGOV;

LP: AHRM | TK_TGKU D;

MGOV: 'Q' | AHRM;

TRL: DNFF | G D G OGR 'Y' | TK_IEDA | UXESO VZKQM DNFF;

