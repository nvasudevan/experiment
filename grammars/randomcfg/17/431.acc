%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: UCKQB TK_MVI TK_HLJ | TK_MVI TK_BBZ TRL 'K' TK_OM;

VYJU: OGR SRVI 'H';

VZKQM: TK_TGKU | ;

DNFF: TK_MVI TK_PRGF TK_OM | OGR TK_OM VYJU 'Y' TK_MVI | TK_DTRWR G;

UXESO: OGR 'Q' SWRI | TRL 'H' TK_HLJ MGOV | TK_PRGF TK_ERGO;

SWRI: I VYJU | SRVI TK_KL | LP TK_TGKU TK_HLJ TK_MVI | LP OGR TK_BBZ TK_SRWU | DNFF SRVI SWRI TK_PRGF;

G: TK_SRWU 'W' SWRI | AHRM UCKQB 'Y' | VZKQM G SWRI;

SRVI: 'H' TK_PRGF 'Q' TRL | TK_PRGF TK_IEDA TRL 'K' | SRVI DNFF | UXESO TK_IEDA G | SRVI TK_TGKU;

C: UCKQB SRVI | SWRI TK_HLJ TK_IEDA TK_OM | 'K' DNFF | TK_KL 'H' 'W' TK_ERGO;

D: TK_MVI UCKQB TK_KL | TK_DTRWR 'Q' | 'Y' TRL VYJU MGOV VZKQM;

OGR: D TK_MVI 'K' | TRL TK_RBS TK_ERGO TK_DTRWR UCKQB | LP TK_OM OGR TK_SRWU TK_BBZ | VZKQM DNFF;

AHRM: TK_DTRWR TK_PRGF C VZKQM I | ;

I: TK_OM TK_BBZ TK_DTRWR | TK_SRWU TK_SRWU 'Q' 'Q' | TK_SRWU UCKQB TK_TGKU | 'W' DNFF LP TK_PRGF;

UCKQB: OGR 'K' | UCKQB TK_TGKU 'H' 'Y' AHRM | TRL UCKQB OGR | OGR;

LP: SRVI TK_MVI I;

MGOV: TK_OM | TK_DTRWR TK_DTRWR | D 'K';

TRL: VYJU DNFF | TK_TGKU 'K' | D | 'K';

