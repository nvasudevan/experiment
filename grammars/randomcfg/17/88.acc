SRVI: TK_BBZ | TK_RBS TK_MVI 'Y';

C: MGOV;

D: 'H' TK_OM TK_SRWU | VZKQM TK_DTRWR TK_IEDA UCKQB | UCKQB D TK_PRGF LP | TK_TGKU LP;

OGR: VZKQM | LP UXESO | DNFF TK_ERGO VYJU TK_RBS | C SRVI TK_OM | ;

AHRM: G TK_BBZ | DNFF TK_TGKU SWRI TK_SRWU UXESO | 'Q' TK_MVI VYJU | MGOV UXESO | TRL C;

I: TK_TGKU UXESO | TK_HLJ D;

UCKQB: SWRI;

LP: TK_HLJ 'Q' 'K' TK_SRWU TK_KL | TK_HLJ TK_HLJ | SRVI TK_PRGF MGOV 'Y' | ;

MGOV: 'Q' TK_MVI TK_SRWU | SWRI TRL MGOV TK_MVI | UXESO C C;

TRL: SWRI G TK_TGKU TK_BBZ | TK_DTRWR | 'Q' 'Y' G G UXESO;

