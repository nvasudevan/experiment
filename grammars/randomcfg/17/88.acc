%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: I | 'K' TK_MVI | 'Q' D UXESO VYJU | TK_SRWU LP;

VYJU: TK_DTRWR D | DNFF AHRM TK_RBS UXESO | TK_SRWU 'Y';

VZKQM: TK_SRWU | DNFF VYJU TK_SRWU | LP TK_IEDA C | D 'K' TK_OM;

DNFF: DNFF 'W' TK_OM C | TK_SRWU | UCKQB 'Q' 'Q' | TRL TK_RBS 'W' | SWRI;

UXESO: TK_ERGO SRVI SWRI | 'Y' AHRM TK_TGKU TRL | TK_BBZ TK_ERGO C TK_MVI | 'Y' C VZKQM TRL TK_BBZ | SRVI TRL UCKQB VZKQM D;

SWRI: TK_RBS DNFF TK_KL;

