%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: UCKQB 'K' SRVI TK_ERGO | UXESO UXESO TK_TGKU TK_KL;

VYJU: TK_HLJ UXESO | TK_IEDA TK_OM SWRI | TK_KL 'Q' TK_MVI C LP | UCKQB VZKQM TK_RBS 'Y';

VZKQM: 'H' TK_KL 'Q' VYJU | TK_BBZ TK_SRWU | ;

DNFF: TK_DTRWR | ;

UXESO: TK_DTRWR AHRM 'K' | VZKQM TK_ERGO TK_RBS | I TRL SWRI TK_TGKU TK_BBZ;

SWRI: UXESO TK_IEDA TK_RBS 'Y' | TK_KL | TK_PRGF TK_RBS TK_KL TK_HLJ TRL | D MGOV TK_ERGO AHRM TK_TGKU | TK_OM;

G: 'Y' | 'W';

SRVI: TK_OM 'W' TK_SRWU | TK_KL TK_TGKU I 'Y' TK_SRWU | SRVI TK_PRGF DNFF;

C: TK_BBZ SRVI | TK_IEDA | TK_BBZ AHRM 'Q' TK_TGKU UCKQB | TK_DTRWR AHRM D TRL OGR;

D: OGR TK_BBZ TK_IEDA | 'Y' TK_HLJ;

OGR: VYJU TK_DTRWR | SWRI TK_TGKU | C G;

AHRM: TK_SRWU 'K' | TK_IEDA LP | AHRM | TK_ERGO SWRI VYJU TK_DTRWR TK_KL;

I: VZKQM D 'Q' TK_OM G | VYJU TK_IEDA I MGOV 'Q' | TK_RBS TK_RBS AHRM TK_HLJ;

UCKQB: TK_HLJ TK_KL 'Q' | MGOV AHRM | SRVI TRL C | OGR 'Y' 'Q';

LP: MGOV D TK_TGKU D 'W';

MGOV: 'Q' 'H' UXESO | MGOV | 'W' SRVI;

TRL: UXESO 'K' I TRL | TK_HLJ TRL TK_RBS | TK_BBZ;

