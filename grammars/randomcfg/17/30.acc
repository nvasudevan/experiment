%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: MGOV TK_OM 'H' TRL | TK_HLJ VZKQM MGOV 'W' LP | TK_MVI;

VYJU: I | 'Q' SRVI C 'K' 'K' | TK_HLJ | TK_IEDA UCKQB DNFF LP MGOV;

VZKQM: TK_SRWU TK_DTRWR | TK_KL 'W' LP | TK_DTRWR VYJU | SRVI TK_RBS TK_PRGF TK_HLJ DNFF;

DNFF: VZKQM 'W' AHRM | UXESO 'Q';

UXESO: I 'Q' VZKQM | TK_KL I D | 'H' TRL D | ;

SWRI: 'Y';

G: TK_KL C TRL TK_DTRWR TRL | TK_SRWU 'Y' 'Y' AHRM C;

SRVI: VYJU LP | LP 'K' 'W' | D UCKQB TK_DTRWR | D LP UCKQB 'H' VYJU | VZKQM;

C: TRL C 'Y' TK_ERGO TK_MVI | TK_SRWU SWRI TK_TGKU | TK_KL | 'W' 'Q' 'Y';

D: AHRM | SRVI 'K';

OGR: TK_OM LP MGOV | UXESO SRVI TK_DTRWR TK_MVI TK_ERGO;

AHRM: LP 'Y' OGR | C MGOV | TK_HLJ TK_IEDA TK_MVI;

I: SRVI UCKQB 'K' UCKQB | TK_PRGF TRL UCKQB;

UCKQB: 'K' MGOV OGR SRVI | OGR | DNFF 'Q' C OGR SRVI;

LP: UXESO | DNFF TK_RBS | 'W' TK_SRWU | C OGR 'W';

MGOV: TK_HLJ TK_SRWU OGR LP | I SWRI TK_TGKU TK_MVI | TK_OM OGR SWRI | UXESO UXESO TRL D;

TRL: TRL TK_IEDA TK_BBZ TK_KL | ;

