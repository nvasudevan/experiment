%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_BBZ | C 'H';

VYJU: AHRM TK_HLJ 'Y' VZKQM G | VZKQM TK_PRGF 'K' SRVI TK_MVI | G UXESO LP | ;

VZKQM: 'H' SWRI | VZKQM D TK_PRGF VYJU | I TK_RBS 'K' TK_DTRWR 'K' | OGR | VZKQM TK_IEDA 'W' 'H';

DNFF: TK_DTRWR TK_PRGF | 'W' SRVI TK_IEDA MGOV TK_DTRWR;

UXESO: TK_BBZ VZKQM 'W' TRL TK_SRWU | 'Q' 'Y';

SWRI: TK_OM SWRI TK_MVI UCKQB | 'Y' SRVI | ;

G: 'K' | G | D LP C TK_DTRWR | UCKQB 'K' 'H' | I TK_DTRWR TK_TGKU 'K' TK_ERGO;

SRVI: I 'W' DNFF TK_MVI TK_DTRWR | MGOV G | TK_MVI TK_TGKU SRVI UXESO VZKQM | DNFF TK_TGKU TRL TK_DTRWR;

C: TK_RBS TK_TGKU D TK_OM TK_IEDA | OGR 'W' SRVI;

D: TK_HLJ | G TK_IEDA TRL 'K' SRVI;

OGR: D | TK_DTRWR MGOV SRVI MGOV UCKQB | 'W' C UXESO TK_BBZ TK_MVI | 'Q' TK_SRWU SWRI UXESO SWRI;

AHRM: TK_PRGF TK_KL | G TK_TGKU TRL SRVI AHRM | 'W' | OGR | OGR 'Q' TK_HLJ;

I: TK_TGKU I TK_OM | UXESO 'H' SWRI | VYJU AHRM TK_HLJ TRL;

UCKQB: AHRM TK_DTRWR | 'Q';

LP: TK_PRGF UXESO TRL TK_KL | AHRM 'Q' TK_TGKU G | SRVI SRVI;

MGOV: G TK_DTRWR TK_OM | 'H' TRL OGR 'W' | 'Y' TRL TK_HLJ | TK_MVI C;

TRL: DNFF TK_TGKU | VZKQM;

