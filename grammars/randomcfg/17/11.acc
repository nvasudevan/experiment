C: UCKQB TK_TGKU TRL UCKQB | TRL TK_HLJ;

D: TK_TGKU TK_HLJ VYJU | TK_IEDA I TK_HLJ OGR | 'W' TK_OM MGOV OGR DNFF | ;

OGR: TK_IEDA | G 'H' TK_SRWU TK_HLJ | DNFF TK_HLJ C TK_KL | 'Q' TK_KL | TK_PRGF 'Y' TK_KL;

AHRM: 'H' TK_RBS TK_OM | TK_SRWU UXESO TK_RBS DNFF | TK_OM TK_ERGO TK_TGKU I | VYJU TK_ERGO 'Y';

I: TK_SRWU TK_RBS | 'K' SWRI | TK_TGKU TK_TGKU TK_BBZ 'Q' | TK_HLJ UCKQB VYJU;

UCKQB: TK_TGKU TK_PRGF TK_MVI | DNFF TK_TGKU 'Y' TK_TGKU;

LP: TK_ERGO SWRI;

MGOV: TK_SRWU TK_BBZ 'Q' | UCKQB TK_HLJ | TK_TGKU TK_SRWU AHRM TK_SRWU G | OGR TK_TGKU | I TK_HLJ 'W' LP;

TRL: 'H' | AHRM 'K' TK_RBS;

