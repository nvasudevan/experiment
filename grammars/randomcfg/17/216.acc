%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: 'H' OGR DNFF AHRM | 'W' | G TK_IEDA DNFF VZKQM VYJU;

VYJU: G | G D 'K' TK_KL | ;

VZKQM: TK_PRGF UCKQB DNFF;

DNFF: I UCKQB;

UXESO: TK_TGKU VZKQM TK_IEDA | TK_SRWU | TK_KL TK_KL | 'Q' TRL AHRM;

SWRI: TK_ERGO TK_PRGF UCKQB | TK_IEDA TK_MVI TK_OM | TK_BBZ TK_KL OGR;

G: TK_OM TK_HLJ VYJU TK_TGKU | SWRI | C UXESO LP 'H' AHRM | TK_IEDA I | TK_RBS TK_DTRWR TK_KL;

SRVI: UXESO G TK_TGKU 'Q' DNFF | MGOV UXESO 'W' DNFF TK_PRGF | C TK_BBZ | OGR I MGOV OGR VYJU;

C: LP TK_RBS | D 'Y' TK_IEDA TK_RBS UCKQB;

