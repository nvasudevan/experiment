OGR: TK_OM UCKQB | TK_BBZ MGOV TK_TGKU UXESO TK_HLJ;

AHRM: TK_TGKU 'K' 'Y' | 'Y' | SRVI UXESO | MGOV AHRM VYJU TK_ERGO DNFF;

I: TK_IEDA D SWRI TK_BBZ TK_SRWU;

UCKQB: D TK_SRWU TK_ERGO OGR | 'Y' AHRM G TK_DTRWR TK_HLJ | VYJU | SRVI TK_ERGO MGOV DNFF 'K';

LP: TK_ERGO TK_MVI TK_OM TK_DTRWR TK_KL | 'H' LP C UCKQB | ;

MGOV: 'K' DNFF | 'W' SRVI I SWRI | SWRI LP | TK_MVI D 'K' | TK_IEDA TK_KL TK_KL D DNFF;

TRL: TK_PRGF TK_MVI TK_HLJ | TK_TGKU 'K' | G | 'H' TRL TK_SRWU | UXESO TK_RBS;

