%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_TGKU SWRI TK_ERGO TK_IEDA | TK_ERGO VZKQM TK_RBS;

VYJU: TK_MVI TK_ERGO | TK_KL LP TK_RBS SWRI | TK_SRWU SWRI | TK_PRGF I TK_DTRWR TK_HLJ I | ;

VZKQM: TK_HLJ C UCKQB TK_ERGO UCKQB | 'W' OGR | OGR SRVI | AHRM VZKQM | ;

DNFF: 'K';

UXESO: MGOV TK_BBZ | LP SRVI 'W' | DNFF D TK_PRGF;

SWRI: TRL TK_HLJ G | DNFF AHRM 'Q' | D;

G: OGR 'Q' | 'Y' 'W' TK_BBZ SRVI MGOV;

SRVI: TK_SRWU TK_DTRWR TK_PRGF UXESO LP | TK_PRGF D 'W' VZKQM VYJU | I TK_HLJ UXESO LP | DNFF C TK_SRWU TK_BBZ;

C: VYJU I G SRVI TK_KL;

D: MGOV SRVI SRVI | TK_TGKU 'W' VZKQM AHRM C | TK_IEDA | VYJU LP UXESO I | TK_PRGF TK_TGKU DNFF;

OGR: 'K' | TK_IEDA 'W' SWRI TK_DTRWR OGR | OGR TK_MVI D DNFF | VZKQM TK_BBZ;

AHRM: TK_SRWU | TRL 'H' SRVI TK_KL;

I: SWRI G | TK_KL AHRM TK_BBZ 'K' G | TK_IEDA;

UCKQB: 'W' TK_PRGF TK_IEDA SWRI TK_KL | TK_SRWU | 'H' TK_BBZ UCKQB TK_HLJ;

LP: TK_RBS I TK_SRWU | MGOV LP TK_SRWU;

MGOV: 'Y' DNFF TK_RBS TK_PRGF;

TRL: VZKQM TK_TGKU DNFF TK_OM;

