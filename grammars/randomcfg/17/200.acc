%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: DNFF TK_OM | 'W' AHRM TK_PRGF | TK_BBZ | AHRM TK_HLJ;

VYJU: TK_KL 'K' OGR TK_SRWU TK_MVI | 'H' OGR | DNFF;

VZKQM: 'K' MGOV 'K' UXESO | AHRM TK_TGKU VYJU I | VZKQM DNFF OGR TK_SRWU SWRI | SRVI;

DNFF: UXESO TK_KL UCKQB VZKQM AHRM;

UXESO: DNFF TK_MVI UXESO | 'K' TK_DTRWR TK_RBS | TRL I VZKQM OGR | ;

SWRI: D C | TK_RBS 'Q' TK_MVI | DNFF DNFF TK_ERGO C | TK_SRWU LP SRVI SWRI | MGOV VYJU SRVI;

G: TRL MGOV TK_SRWU | TK_PRGF TK_HLJ TK_OM | UCKQB TRL;

SRVI: MGOV | VZKQM TK_BBZ I G | VZKQM VZKQM 'Q' TK_ERGO | TK_KL C;

C: TK_SRWU MGOV VYJU SRVI 'H' | 'W' TK_DTRWR DNFF AHRM TK_IEDA;

D: 'Q' TK_IEDA 'H' TK_SRWU C | MGOV 'Q' TK_TGKU TK_SRWU;

OGR: G MGOV LP;

AHRM: SRVI C | TK_DTRWR TK_ERGO | ;

I: C TK_TGKU DNFF;

UCKQB: G 'H' | TK_SRWU 'K' | AHRM OGR | UXESO TK_ERGO 'Y';

LP: VYJU G AHRM I;

MGOV: TK_RBS;

TRL: TK_KL TK_BBZ | TK_DTRWR VZKQM TK_ERGO | AHRM D C | AHRM TK_MVI | 'Q' TK_SRWU TK_HLJ 'H';

