%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: 'Y' TK_PRGF TK_IEDA SWRI;

VYJU: 'W' TK_DTRWR DNFF | LP D | TK_ERGO AHRM TK_PRGF UXESO TK_ERGO | OGR DNFF I;

VZKQM: LP UCKQB | AHRM VYJU;

DNFF: MGOV TK_DTRWR TK_TGKU D | TK_ERGO LP TK_KL TK_PRGF MGOV | LP TK_OM TK_HLJ SWRI C;

UXESO: LP TK_KL SWRI 'W' | TRL TK_TGKU AHRM | TK_ERGO 'Q' OGR;

SWRI: C VZKQM TK_HLJ TK_HLJ TK_RBS | TK_DTRWR TK_ERGO TK_HLJ TK_TGKU SRVI;

G: TK_PRGF | TK_SRWU UCKQB AHRM TK_KL C;

SRVI: TK_HLJ | UCKQB TK_TGKU | SRVI TK_MVI MGOV TK_DTRWR | TK_RBS TK_BBZ C VYJU 'W';

C: I TK_TGKU TRL | AHRM TK_MVI TK_BBZ OGR;

D: C SRVI TK_RBS TK_IEDA G | TK_DTRWR TK_ERGO I TRL | TK_IEDA SRVI TK_TGKU D | SRVI 'Q' TK_OM VYJU TK_IEDA | SRVI TK_MVI;

OGR: 'Q' 'Y' 'W' C UCKQB | TK_DTRWR AHRM SWRI 'W' TK_SRWU | TK_PRGF TK_BBZ G | TK_OM 'H' | DNFF TK_BBZ TK_RBS C MGOV;

AHRM: VZKQM C | LP DNFF TK_PRGF DNFF TK_DTRWR | TK_ERGO | 'H' I 'W' | AHRM UXESO C D;

I: OGR TK_RBS DNFF TK_SRWU TK_TGKU | TK_KL VZKQM | G TK_SRWU 'W' | ;

UCKQB: TK_KL TK_OM UXESO | VZKQM UCKQB | TK_ERGO TK_HLJ 'Q' 'Y' I;

LP: DNFF LP TK_TGKU TRL | MGOV DNFF 'K';

MGOV: 'Q' TK_ERGO VZKQM TK_IEDA | 'K' TRL;

TRL: UCKQB | 'K' VYJU TK_DTRWR TK_IEDA TK_KL | 'W' 'K' | ;

