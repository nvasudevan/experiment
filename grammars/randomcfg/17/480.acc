%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: MGOV D MGOV TK_TGKU | TK_HLJ SRVI UCKQB | TRL;

VYJU: TK_IEDA C | C SRVI | 'H';

VZKQM: TK_SRWU | DNFF TK_MVI | TK_RBS 'H' G TK_KL TK_SRWU;

DNFF: C | C TK_MVI;

UXESO: LP TK_PRGF TK_DTRWR UXESO TK_OM | TK_TGKU 'Q' 'Q' | D D D TK_HLJ | OGR UXESO | ;

SWRI: 'Y' 'H' VZKQM 'W' | ;

G: DNFF TK_DTRWR TK_OM SWRI UCKQB | TK_KL TK_ERGO | TK_KL;

SRVI: TK_TGKU | TK_IEDA TK_RBS;

C: TK_RBS OGR 'Y' OGR TK_ERGO | TK_BBZ TK_KL UXESO VYJU;

D: 'Y' DNFF G | LP 'Y';

OGR: DNFF | SWRI I | TK_PRGF TK_DTRWR TRL TK_OM | C DNFF TK_OM TK_DTRWR | TK_OM TK_ERGO MGOV;

AHRM: D VZKQM 'H' TRL TK_OM | SWRI;

I: 'W' SWRI | VZKQM;

UCKQB: TK_KL D TK_ERGO DNFF;

LP: G TK_ERGO TK_DTRWR;

MGOV: TK_HLJ TRL TK_TGKU 'H' | TK_TGKU UXESO 'Y' TK_DTRWR TK_PRGF | DNFF TK_BBZ | AHRM VYJU 'Q' TK_TGKU TRL;

TRL: TK_HLJ | 'K' UXESO AHRM AHRM TK_BBZ | 'Q' VZKQM D TK_OM;

