%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_RBS TK_HLJ VZKQM | TK_BBZ 'Q' TK_SRWU MGOV | TK_IEDA SRVI DNFF 'H' SRVI | 'H';

VYJU: TK_MVI C 'W' | MGOV TK_OM UCKQB | TK_BBZ 'W' OGR;

VZKQM: DNFF MGOV | C | 'Q' TK_ERGO | LP OGR | DNFF TK_DTRWR;

DNFF: TK_ERGO TK_DTRWR TK_RBS VZKQM | AHRM AHRM 'H';

UXESO: G | TK_OM VZKQM D;

SWRI: VZKQM | TK_IEDA | OGR TK_DTRWR 'K' LP;

G: TK_ERGO 'H' TK_OM | TK_IEDA TK_TGKU TK_ERGO 'Q' LP | OGR TK_PRGF TK_KL | TK_OM TK_PRGF;

SRVI: TK_ERGO TK_PRGF | TK_ERGO D | DNFF TK_PRGF TK_DTRWR | 'K' 'H' D;

C: SRVI TK_SRWU TK_MVI | TK_MVI C 'Y' VZKQM TK_MVI | VYJU;

D: UCKQB TK_SRWU 'K' | TK_TGKU DNFF UXESO UCKQB | 'Y' VZKQM VZKQM | C LP 'Y';

OGR: AHRM G SWRI SRVI TK_OM | 'Y' TK_PRGF UCKQB | TK_HLJ LP | TK_IEDA TK_ERGO D | ;

AHRM: DNFF MGOV C TK_RBS | DNFF UCKQB UCKQB SRVI TK_BBZ;

I: 'Y' C | MGOV | G | TK_KL UXESO | ;

UCKQB: TK_ERGO TRL | MGOV C TK_KL VYJU | TK_MVI TK_IEDA TK_DTRWR;

LP: TK_OM | AHRM OGR 'K' TK_OM VZKQM | TK_IEDA DNFF C UXESO | TK_OM TK_KL TRL | TK_DTRWR;

MGOV: TK_DTRWR 'W' I SWRI TK_IEDA;

TRL: TK_TGKU | D DNFF TK_DTRWR | 'W' TK_BBZ;

