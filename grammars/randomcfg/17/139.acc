%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: OGR TRL;

VYJU: VZKQM;

VZKQM: 'W';

DNFF: TK_IEDA TRL TK_BBZ | 'K' TK_TGKU TK_KL TK_SRWU TRL | C G 'K' 'K' | G TK_OM TK_DTRWR | VZKQM 'Q' I UCKQB;

UXESO: TK_HLJ AHRM | LP VZKQM LP VYJU VZKQM | TK_KL MGOV TK_HLJ | UXESO TK_IEDA TK_IEDA LP TK_OM | G;

SWRI: TK_OM 'Y' D G | TK_BBZ TK_SRWU UXESO | TK_IEDA;

G: 'W' 'W' LP UXESO 'Q';

