D: TK_IEDA TK_SRWU TK_HLJ | TK_ERGO VZKQM TK_SRWU | AHRM TK_TGKU 'Q' | MGOV TK_TGKU DNFF TK_OM MGOV;

OGR: TK_DTRWR;

AHRM: TK_SRWU | TK_TGKU TK_IEDA UXESO;

I: UCKQB TK_TGKU TK_DTRWR AHRM | I TK_ERGO | TK_RBS | OGR I DNFF VYJU TK_MVI;

UCKQB: 'H' TK_IEDA | VZKQM 'Q' VYJU TK_OM TK_DTRWR | TK_DTRWR DNFF C;

LP: MGOV I TK_IEDA SWRI | TRL SRVI LP TK_BBZ | TK_ERGO TK_TGKU AHRM | UXESO D 'Y' TK_IEDA D;

MGOV: TK_RBS OGR G LP D;

TRL: 'W' 'H' | 'Y' TK_DTRWR UXESO DNFF | TK_IEDA TK_IEDA | 'Q' TK_MVI TK_PRGF;

