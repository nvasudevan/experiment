%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: C VYJU TK_MVI SRVI UXESO | TK_OM SWRI 'K' | VYJU AHRM MGOV 'Y' DNFF | TK_BBZ UXESO TK_MVI;

VYJU: MGOV VZKQM DNFF | VYJU LP SRVI TK_ERGO;

VZKQM: C I 'W' | UCKQB LP VZKQM TK_MVI | TK_MVI | TK_RBS DNFF TK_DTRWR 'K';

DNFF: TK_IEDA | UCKQB UXESO TK_BBZ | MGOV AHRM TRL VYJU | UCKQB TK_ERGO AHRM | UCKQB TK_DTRWR VZKQM TK_SRWU;

UXESO: 'K' TK_SRWU SWRI UXESO | TK_DTRWR OGR | SRVI TK_SRWU TRL | OGR | TK_MVI VZKQM TK_ERGO SRVI;

SWRI: G C 'H' | LP | SWRI TK_BBZ TK_MVI TK_ERGO MGOV | 'K' TRL | TK_RBS 'K' D;

G: TK_IEDA VYJU TK_IEDA TK_OM TK_KL | C TK_SRWU VZKQM | G LP | OGR MGOV VZKQM TK_IEDA 'K' | ;

SRVI: 'K' UCKQB VZKQM UXESO TK_TGKU | TK_IEDA UXESO TK_BBZ | TRL TK_RBS LP TK_OM AHRM | AHRM UXESO 'W' UXESO | ;

C: UCKQB TK_PRGF;

D: TK_TGKU VZKQM I | C G 'K' | TK_RBS TK_KL TK_DTRWR VYJU DNFF | 'K' LP UXESO;

OGR: TK_OM | UCKQB AHRM | SRVI TK_HLJ TK_DTRWR TK_MVI C | SRVI TK_RBS AHRM TK_IEDA I | DNFF;

AHRM: AHRM D AHRM I UCKQB | TK_RBS MGOV I TK_KL | TK_BBZ TK_HLJ | SWRI 'Y' | ;

I: LP TK_SRWU AHRM TK_HLJ LP | OGR D UXESO | TK_MVI | TK_ERGO;

UCKQB: MGOV TK_DTRWR G | SRVI TK_PRGF | TK_HLJ TK_DTRWR TK_ERGO | LP OGR UCKQB TK_OM TRL | LP 'Q' TK_RBS;

LP: UXESO TK_TGKU G SWRI | C LP TK_BBZ 'Q' TK_ERGO | MGOV SWRI TK_PRGF TK_DTRWR;

MGOV: SRVI TK_BBZ MGOV | OGR TK_PRGF | I I TRL TK_OM UCKQB | 'W' OGR TK_KL 'Q' VYJU;

TRL: TK_HLJ TK_IEDA TRL | SWRI;

