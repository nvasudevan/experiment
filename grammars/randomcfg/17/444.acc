%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_OM MGOV VYJU;

VYJU: TK_PRGF SWRI SWRI TK_ERGO TK_OM;

VZKQM: 'H' TK_IEDA TK_ERGO | G TK_HLJ TK_ERGO | MGOV | D TK_BBZ LP 'H' 'W' | TK_SRWU D TK_SRWU MGOV;

DNFF: AHRM LP SWRI;

UXESO: TK_SRWU | SRVI VYJU 'Y' | 'H';

SWRI: TK_PRGF TK_DTRWR | OGR DNFF TK_ERGO I I | SRVI SWRI C | TRL;

G: TK_BBZ MGOV TK_KL LP TRL | TK_TGKU DNFF TK_BBZ AHRM | 'W' LP OGR C LP | TK_IEDA VZKQM;

SRVI: LP UXESO D TK_HLJ | TK_SRWU 'K' TK_SRWU G I | SRVI TK_TGKU 'H' TK_IEDA 'Y' | TRL UCKQB | ;

C: I G G TK_KL TK_MVI;

D: MGOV TK_PRGF OGR | G | OGR | C | AHRM OGR G TK_MVI;

OGR: 'Q' | TK_TGKU TK_PRGF 'H' TK_OM | TRL G SWRI TK_RBS;

AHRM: C VYJU | VZKQM TK_ERGO C;

I: TRL TK_PRGF D SRVI TK_HLJ;

UCKQB: TK_SRWU TK_SRWU 'K' TK_RBS SWRI;

LP: SRVI 'Y' SRVI MGOV | AHRM AHRM TK_KL | D TK_ERGO TRL TK_MVI TK_DTRWR | ;

MGOV: AHRM TK_PRGF UCKQB SRVI SWRI | TK_ERGO TK_KL VZKQM | D | 'Q' TK_IEDA MGOV;

TRL: DNFF TK_OM | UCKQB 'K';

