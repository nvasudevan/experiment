%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: VYJU TK_DTRWR AHRM SWRI | LP C;

VYJU: 'Y' C;

VZKQM: TK_BBZ SRVI | DNFF TK_BBZ SWRI 'Y' | TK_DTRWR TRL 'K' 'Y' | OGR;

DNFF: TK_OM VZKQM C 'K' | UCKQB MGOV G;

UXESO: VYJU OGR TRL SRVI SRVI;

SWRI: TK_HLJ 'H' | D OGR AHRM | VYJU C TRL UCKQB TK_BBZ | 'K' TK_OM TK_KL UXESO | G;

G: TK_HLJ 'Q' | TK_IEDA | D | MGOV AHRM OGR;

SRVI: AHRM TK_KL TK_PRGF;

C: VYJU 'K' | TK_RBS | MGOV 'H' VZKQM | 'K' SWRI TK_SRWU TK_ERGO TK_SRWU | ;

D: TK_DTRWR TK_KL TK_BBZ LP TK_TGKU | 'Q' TK_HLJ DNFF | TK_SRWU | TK_BBZ C TK_DTRWR SRVI | ;

OGR: TK_IEDA SWRI VYJU TK_BBZ | VZKQM;

AHRM: TK_SRWU | SRVI VZKQM TK_OM 'W' UXESO;

I: 'Q';

UCKQB: TK_HLJ VZKQM VZKQM | G;

LP: TRL | G | TK_HLJ | AHRM TK_SRWU;

MGOV: LP D | SRVI D TK_OM MGOV C;

TRL: VZKQM TK_SRWU TK_TGKU | TK_RBS | TK_DTRWR LP;

