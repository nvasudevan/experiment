%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TRL UXESO 'Q' | TK_ERGO 'K' G TK_OM | TK_BBZ TK_OM D TK_KL OGR | TK_KL 'W' DNFF | C AHRM;

VYJU: C TK_PRGF DNFF I;

VZKQM: DNFF SWRI SWRI TK_BBZ | SWRI D TK_HLJ TK_RBS | TK_PRGF TK_DTRWR;

DNFF: TK_MVI 'H' | UXESO UXESO TK_SRWU 'Q' | UXESO;

