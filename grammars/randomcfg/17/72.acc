%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: 'K' 'H' TRL 'Y' | UCKQB 'K' TK_IEDA TK_BBZ | D TK_BBZ VZKQM;

VYJU: DNFF | TRL TK_ERGO | SWRI 'Y' TK_PRGF | UCKQB | ;

VZKQM: 'H' 'W' OGR TK_RBS | TK_HLJ 'W' UCKQB TK_KL TK_KL | TK_HLJ | TRL G SWRI;

DNFF: I LP D | VZKQM TK_IEDA | SWRI | TK_BBZ TK_OM | 'Y' 'W' TK_TGKU;

UXESO: LP | SWRI | C 'W' G SWRI 'H' | TK_HLJ TK_RBS TK_RBS TK_BBZ TK_BBZ | ;

SWRI: TK_OM TK_BBZ | UXESO TK_PRGF TK_IEDA TK_ERGO;

