C: MGOV TK_TGKU TK_ERGO TK_HLJ MGOV | TK_RBS TK_HLJ AHRM TK_SRWU TK_BBZ | UCKQB SRVI;

D: AHRM TK_TGKU VZKQM TK_ERGO | TK_DTRWR | TRL | TK_DTRWR TK_MVI MGOV DNFF;

OGR: TK_RBS 'H' TK_ERGO LP | VZKQM TK_TGKU OGR SWRI 'H' | D AHRM TK_DTRWR TK_PRGF | TK_KL TK_ERGO;

AHRM: TK_ERGO SRVI TK_PRGF D C;

I: LP | C AHRM TK_PRGF LP UXESO | TK_KL TK_DTRWR;

UCKQB: TK_OM TK_BBZ TK_SRWU OGR 'K' | TK_KL TK_TGKU MGOV;

LP: VYJU;

MGOV: TK_PRGF UXESO TK_KL DNFF TK_TGKU | TK_ERGO TK_TGKU MGOV | TK_RBS TK_PRGF D TK_TGKU | VYJU | TK_SRWU TK_TGKU TK_ERGO;

TRL: SWRI TK_MVI UXESO TK_KL;

