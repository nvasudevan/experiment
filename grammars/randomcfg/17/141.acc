%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: AHRM OGR TK_PRGF TK_OM | VYJU | AHRM;

VYJU: I LP TK_BBZ UCKQB;

VZKQM: TK_MVI;

DNFF: TK_RBS TK_IEDA 'H' AHRM 'K' | VYJU | MGOV AHRM | AHRM SRVI | SWRI TK_RBS TK_TGKU;

UXESO: OGR | TK_PRGF DNFF VYJU SWRI 'Q';

SWRI: AHRM | UXESO TK_KL | TK_IEDA | TK_OM OGR TK_PRGF TK_MVI TK_OM;

G: UXESO 'W' TK_SRWU | TK_RBS SWRI OGR TK_BBZ 'Y' | TK_OM | TK_KL TK_IEDA;

SRVI: TK_IEDA TK_PRGF | SRVI MGOV 'Y' SWRI I | 'K' OGR 'Y' | TRL TK_OM DNFF SWRI UCKQB | 'W' TK_RBS TK_TGKU TK_DTRWR;

C: SRVI D TRL TK_MVI | ;

D: TK_DTRWR DNFF VZKQM TK_BBZ | TK_DTRWR UCKQB TK_KL TK_DTRWR 'K';

OGR: C 'W' | D UCKQB | OGR AHRM C;

AHRM: AHRM LP 'Q' | SRVI | TK_ERGO TRL | SRVI TK_RBS 'K' TK_KL I;

I: SRVI | ;

UCKQB: SRVI TK_PRGF | D UXESO | TK_SRWU TK_SRWU TK_TGKU LP;

LP: MGOV SRVI VZKQM AHRM | UXESO | DNFF TK_SRWU TK_IEDA | UCKQB C TK_ERGO | TK_PRGF;

MGOV: TK_HLJ TK_SRWU | TK_DTRWR TK_ERGO;

TRL: SRVI | TK_MVI UXESO AHRM D;

