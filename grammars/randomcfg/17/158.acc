%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: LP TK_OM I;

VYJU: TK_MVI TRL G TK_DTRWR AHRM | MGOV | UXESO TK_IEDA TK_KL | TK_MVI G OGR | D;

VZKQM: 'K' TK_KL SRVI 'K' | VZKQM TK_KL TK_OM 'H' I | LP SWRI TK_DTRWR | TK_PRGF 'Y' UXESO;

DNFF: TK_DTRWR | MGOV | 'Q' SRVI TK_ERGO TK_KL TK_OM | 'Q' | ;

UXESO: VYJU TK_PRGF | TK_TGKU TK_DTRWR;

SWRI: OGR TK_HLJ TK_BBZ TK_DTRWR LP;

G: 'Q' AHRM | 'W' TK_SRWU SRVI 'W' | TK_PRGF TK_BBZ | LP | SWRI TK_KL;

SRVI: UXESO VZKQM C;

C: TK_RBS VYJU TK_BBZ SRVI VYJU | TK_SRWU LP | 'W' 'H' TK_RBS OGR TK_ERGO | 'W' TK_MVI;

D: UCKQB | D C;

OGR: TK_HLJ TK_KL TK_OM TK_TGKU TK_DTRWR | DNFF 'Q' 'K' | UXESO G 'W';

AHRM: TK_SRWU TK_TGKU VYJU SWRI;

I: G TK_HLJ G TK_HLJ MGOV | SWRI SRVI | UCKQB TK_MVI | D 'W' TK_SRWU TK_ERGO MGOV | TK_MVI;

UCKQB: VYJU | ;

LP: UXESO TK_DTRWR TK_SRWU 'W' TK_ERGO | 'W' | VYJU UXESO | 'Y' TK_OM;

MGOV: TK_BBZ 'K' OGR | TK_IEDA SWRI TK_MVI AHRM | TK_MVI G TK_RBS;

TRL: 'Q' | OGR 'W' 'Q' TK_ERGO | TK_MVI D DNFF TK_HLJ LP;

