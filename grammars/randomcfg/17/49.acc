%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: TK_HLJ LP 'W' UXESO C | AHRM | MGOV TK_MVI | UCKQB TK_BBZ;

VYJU: D I AHRM | 'W';

VZKQM: TK_KL G AHRM TK_IEDA | UXESO TK_MVI AHRM 'W' 'Y' | TK_MVI TK_IEDA UXESO VYJU | SRVI C OGR DNFF;

DNFF: TK_TGKU TK_RBS 'K' | TK_RBS | TK_ERGO D AHRM TK_DTRWR G | 'H' VYJU TK_OM G VYJU | 'Y' TK_BBZ 'K';

UXESO: TRL MGOV 'Q' TK_TGKU TK_IEDA | OGR D TK_RBS TK_PRGF | UCKQB D VYJU LP I | TK_IEDA I TK_MVI 'Y' | 'K' 'W' UXESO 'K';

SWRI: LP MGOV G SWRI I | 'H' TK_MVI 'W' 'W' | UXESO SWRI LP TK_PRGF TK_KL | TK_ERGO;

G: TK_ERGO AHRM VZKQM SWRI | 'H' | TK_PRGF 'Y' TK_KL MGOV VZKQM | TK_SRWU VZKQM TK_ERGO TK_OM | TK_IEDA TK_MVI;

SRVI: SWRI 'K' TK_IEDA | SWRI | ;

C: 'W' | TK_KL MGOV 'Q' 'W' | MGOV 'Q' | VYJU TK_HLJ;

D: C;

OGR: VZKQM 'Y';

AHRM: G TK_ERGO I UCKQB | TK_KL TK_MVI TK_DTRWR | TK_MVI VYJU | ;

I: D C TK_SRWU TK_KL OGR | TK_RBS;

UCKQB: SWRI LP TK_SRWU VZKQM TK_DTRWR | I I 'W' VZKQM I;

LP: VYJU TK_KL | C AHRM TK_IEDA;

MGOV: TK_BBZ;

TRL: OGR TK_RBS | TK_BBZ MGOV | 'Y' DNFF;

