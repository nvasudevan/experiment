%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: G TK_DTRWR C I C;

VYJU: C DNFF TK_OM | SRVI SWRI UXESO 'Q' TK_PRGF | SRVI TK_ERGO UCKQB 'K' | 'W' TK_IEDA 'Q' UXESO | TK_RBS;

VZKQM: TK_DTRWR D UXESO 'H' | I TRL UXESO | TK_OM 'H' TK_TGKU 'Q';

DNFF: TK_PRGF TK_MVI TK_IEDA 'H';

UXESO: G I | TK_SRWU TK_PRGF | 'Q' 'Y' 'Y' | TK_DTRWR D | C;

SWRI: TK_RBS | TK_IEDA C UCKQB VZKQM | VZKQM TK_BBZ 'W' TK_KL | C C;

G: TK_ERGO TK_OM | G | VZKQM TK_PRGF | 'W' TK_PRGF TK_KL 'W' | ;

SRVI: G 'K' TK_DTRWR TK_TGKU TRL | TK_KL 'H';

C: 'Q' TK_OM | 'H' TK_SRWU SWRI;

D: TK_HLJ TK_KL TK_ERGO | G G UXESO | TK_OM TK_ERGO 'W' TK_RBS SRVI;

OGR: VYJU TK_ERGO 'K' | UCKQB TK_OM | TK_MVI G 'H' SRVI TK_IEDA;

AHRM: TK_ERGO 'Y' TK_TGKU TK_KL | TK_MVI | TK_DTRWR LP 'K' G;

I: TK_DTRWR TRL TK_DTRWR | TK_KL TRL AHRM;

UCKQB: UXESO | TK_SRWU;

LP: SRVI UCKQB | UCKQB TRL SRVI 'H' TK_HLJ | OGR;

MGOV: DNFF G;

TRL: VYJU TK_DTRWR 'K' | 'Q' AHRM OGR TK_RBS | VYJU | MGOV VYJU TK_DTRWR D | ;

