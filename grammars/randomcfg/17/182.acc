%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: UXESO TK_OM | TRL SRVI I | D AHRM;

VYJU: TK_TGKU 'K' | 'Q' UCKQB TK_BBZ | LP 'K' 'W' TK_ERGO TK_SRWU;

VZKQM: TK_IEDA 'Q' | UCKQB DNFF TK_RBS;

DNFF: TK_MVI 'W' TK_BBZ SRVI;

UXESO: OGR UCKQB 'H' VZKQM | VZKQM;

SWRI: G 'H' I OGR 'Y' | TK_HLJ TK_TGKU OGR VZKQM;

G: 'W' 'K' I I | ;

SRVI: 'H' D TK_OM;

C: UXESO TK_RBS;

D: TK_OM SRVI UXESO | OGR G | TK_SRWU;

OGR: 'W' TK_TGKU TK_RBS UXESO TK_MVI | TK_SRWU TRL TK_SRWU TK_BBZ;

AHRM: VYJU TK_DTRWR TK_HLJ MGOV | LP | TK_DTRWR TK_ERGO TK_OM MGOV | 'H' G TK_BBZ | AHRM TRL TK_RBS TRL;

I: TK_IEDA TK_IEDA;

UCKQB: TK_RBS VYJU AHRM | 'Q' TK_MVI 'H' G | UCKQB TK_OM | UXESO TK_MVI TK_DTRWR AHRM TK_HLJ;

LP: DNFF I AHRM AHRM | TK_PRGF TRL;

MGOV: UXESO VYJU TK_OM | SWRI TK_KL DNFF 'W' | TK_SRWU G TK_SRWU DNFF | TK_DTRWR TK_BBZ | ;

TRL: TRL | AHRM | 'W' 'Y' DNFF | G VYJU | SWRI TK_RBS;

