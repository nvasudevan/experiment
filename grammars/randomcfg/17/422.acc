%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: SWRI LP D | 'H' OGR | VZKQM | VYJU UXESO TK_KL LP AHRM;

VYJU: VYJU TK_MVI D 'H' | TK_HLJ | 'W' TK_DTRWR MGOV SWRI;

VZKQM: TK_PRGF SRVI TK_HLJ | TK_RBS;

DNFF: TK_HLJ 'W' VYJU | TK_BBZ TK_MVI | SRVI 'K' | 'W' OGR TK_BBZ TK_SRWU TK_OM;

UXESO: LP D UXESO TK_TGKU | TK_OM SWRI | VZKQM TRL OGR VYJU SWRI | SWRI TK_OM VZKQM TRL 'Y';

SWRI: 'Y' C VZKQM TK_MVI TK_SRWU | TK_SRWU TK_MVI LP 'K';

G: TRL TK_MVI 'H' D TK_KL;

SRVI: TK_BBZ C TK_MVI TK_OM TK_HLJ | 'Y' TK_MVI TK_SRWU | TK_ERGO TRL VYJU;

C: VZKQM TK_MVI MGOV UCKQB 'W' | LP TK_IEDA 'K' TRL TK_BBZ | TK_ERGO TK_BBZ UXESO TK_BBZ | DNFF 'Y' C MGOV;

D: 'H' G TRL TK_IEDA | MGOV TK_ERGO 'H' TK_IEDA;

OGR: D;

AHRM: VYJU | TK_DTRWR TK_ERGO TK_PRGF | TK_BBZ DNFF TK_IEDA VYJU | TK_BBZ TK_OM TK_SRWU | AHRM C TK_ERGO;

I: D VYJU SWRI | TK_HLJ 'Q';

UCKQB: TK_TGKU TK_TGKU LP TK_MVI | MGOV | TK_MVI 'W' | TK_KL UCKQB C | ;

LP: OGR 'K' TRL VZKQM TK_OM | 'Y' UCKQB TK_MVI | TK_DTRWR;

MGOV: TK_BBZ OGR | TK_DTRWR TK_SRWU 'H' TK_ERGO | TK_MVI TRL;

TRL: TK_TGKU TK_KL VYJU G TK_HLJ | LP TK_DTRWR SRVI UXESO 'Q' | TK_MVI OGR | ;

