%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: SRVI C | TRL LP TK_OM TK_TGKU | G TK_PRGF OGR DNFF MGOV | LP;

VYJU: UCKQB TK_IEDA G MGOV MGOV;

VZKQM: I 'Y' I TK_BBZ | TK_ERGO AHRM TRL | TK_KL DNFF OGR TK_HLJ 'W' | TK_MVI TK_IEDA LP | TK_IEDA TK_OM OGR TRL TK_TGKU;

DNFF: TK_DTRWR TRL I TK_OM | TK_OM | TRL UCKQB 'K' | 'W' TK_DTRWR | 'K' TK_ERGO 'K';

UXESO: VZKQM LP | I | ;

SWRI: TK_IEDA 'Q' TK_MVI TK_MVI | LP 'Q' | MGOV MGOV | MGOV SRVI D TK_RBS SWRI | MGOV AHRM VZKQM D 'K';

G: TK_OM TK_ERGO VZKQM | ;

SRVI: TK_SRWU TK_IEDA | TK_SRWU 'Q' | MGOV TK_DTRWR TK_PRGF | 'H' TK_TGKU TK_RBS VZKQM TK_IEDA;

C: I SRVI TK_MVI 'K';

D: G;

OGR: 'W' | MGOV TK_ERGO VYJU TK_OM | VYJU | G TK_ERGO;

AHRM: VYJU | TK_SRWU SRVI 'Y' TK_BBZ | TK_IEDA VZKQM SRVI TK_KL C | 'Y' DNFF D VZKQM;

I: 'W' UXESO UCKQB MGOV | SRVI TK_PRGF TRL;

UCKQB: TK_SRWU TK_TGKU TK_KL;

LP: SWRI C D TK_DTRWR | I | TK_BBZ D TK_RBS | 'W' TK_DTRWR UXESO | VZKQM D 'Y' TK_RBS;

MGOV: 'W' D TK_IEDA 'K' | TK_MVI TK_IEDA | AHRM VZKQM TRL TK_ERGO VYJU;

TRL: TK_DTRWR TK_BBZ | UCKQB VYJU | 'K' SWRI;

