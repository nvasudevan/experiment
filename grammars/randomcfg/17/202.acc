%token TK_TGKU, TK_DTRWR, TK_KL, TK_OM, TK_ERGO, TK_BBZ, TK_MVI, TK_HLJ, TK_RBS, TK_IEDA, TK_PRGF, TK_SRWU;

%nodefault

root: MGOV TK_PRGF;

VYJU: TK_SRWU UXESO TK_RBS | I | 'Y' TK_OM TK_KL UXESO | TK_TGKU SRVI AHRM TK_RBS 'H' | SWRI TK_TGKU;

VZKQM: UXESO TK_DTRWR TK_SRWU TK_HLJ SWRI | TK_OM 'Q' | UXESO TK_KL MGOV;

DNFF: D 'W' | MGOV | OGR G TK_BBZ TRL TK_OM;

UXESO: TK_MVI | TK_PRGF | TK_KL MGOV D LP TK_SRWU | ;

SWRI: TK_OM | TRL C | D SWRI TK_RBS UXESO TK_PRGF;

G: MGOV AHRM VYJU 'K' | C SRVI VYJU | MGOV TK_PRGF;

SRVI: UCKQB TK_ERGO;

C: UXESO TK_PRGF MGOV | TK_IEDA | TK_BBZ | TK_HLJ TK_RBS | 'Q' OGR 'W' AHRM TK_RBS;

D: SWRI | 'Q' C UXESO TK_OM | 'W' | 'Q' TK_TGKU TK_ERGO | LP TK_SRWU UCKQB;

OGR: AHRM TK_OM TRL | TK_KL TK_DTRWR SRVI D 'Y';

AHRM: OGR TK_OM | TK_RBS 'H' TK_HLJ D | UCKQB TRL 'Q' | LP VYJU;

I: TK_IEDA TK_HLJ UCKQB TK_KL MGOV | G 'Q' TK_SRWU;

UCKQB: 'H' TK_IEDA | D | UXESO VZKQM | MGOV C TK_RBS VZKQM TK_TGKU | TK_DTRWR 'H' TK_DTRWR AHRM;

LP: LP TK_KL 'W' D | 'W' 'K' UXESO | ;

MGOV: TRL TK_KL C | MGOV | TK_RBS | 'Y';

TRL: UCKQB TK_BBZ TK_HLJ TK_TGKU VZKQM | UXESO 'Q' TK_ERGO C 'Y' | TK_RBS TK_DTRWR SRVI;

