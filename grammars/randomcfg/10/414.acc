%token TK_ZY, TK_DY, TK_QI, TK_FSS, TK_APH, TK_QWXN, TK_HSY;

%nodefault

root: V TK_FSS | V TK_QI A EK TE | TK_FSS 'T';

A: 'Y' TK_QI TE TE | TK_ZY V TK_FSS LG;

LG: TE TK_QWXN A 'X' TK_HSY | EK TK_QI EK SCLJ | ;

EI: TK_DY TK_ZY 'Y' | V TE SCLJ TK_ZY | V DHOLL TK_ZY | TK_DY TK_QWXN EK;

DHOLL: TK_FSS TK_APH | LG;

EK: TK_APH TK_ZY TK_DY | TK_FSS TK_QWXN | 'T' EI TK_DY | SCLJ KSKAL EI 'X' 'Y';

SCLJ: TK_QWXN DHOLL A TK_HSY TK_ZY | 'X' 'T' 'Y' 'T' TK_QI;

KSKAL: TK_DY V | 'T' | A;

V: DHOLL V TK_FSS DHOLL | TK_QI | 'X' TK_QI DHOLL | TK_DY TK_QWXN 'T' | TK_QWXN A KSKAL TK_HSY 'T';

TE: EI 'T' 'Y' TE SCLJ | EI TK_DY 'T' TK_FSS TK_QWXN | LG TK_QI A | SCLJ TE SCLJ | TK_DY;

