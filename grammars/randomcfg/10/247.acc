%token TK_ZY, TK_DY, TK_QI, TK_FSS, TK_APH, TK_QWXN, TK_HSY;

%nodefault

root: LG A TK_FSS LG TK_APH | TK_APH V TK_ZY | 'Y';

A: TK_FSS V | TK_QI KSKAL 'T' TK_HSY TK_QWXN | TK_HSY TK_APH A DHOLL LG | 'X' EI TK_APH | EI DHOLL TK_HSY KSKAL LG;

LG: TK_QWXN V 'Y' TK_QI | 'X' | TK_FSS TK_ZY TK_QWXN;

EI: V TK_APH | 'T' LG EI 'X' | ;

DHOLL: TE A | 'T' 'X' TK_FSS | EK EK EI | TK_FSS | TK_APH;

EK: TK_DY A TE EK | 'X' LG | TK_HSY TK_APH 'X' | 'Y' TK_HSY TK_FSS 'T' TK_HSY | EK;

SCLJ: TK_FSS EK | SCLJ TK_DY EK 'T';

KSKAL: EI 'Y';

V: DHOLL V EI DHOLL | SCLJ EI TK_DY | TK_QWXN TK_ZY TK_ZY TK_APH;

TE: TE EI TK_ZY | EI DHOLL 'T' EI 'Y' | TK_QI SCLJ | TK_QWXN V EI TK_APH;

