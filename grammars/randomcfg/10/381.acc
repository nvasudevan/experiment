%token TK_ZY, TK_DY, TK_QI, TK_FSS, TK_APH, TK_QWXN, TK_HSY;

%nodefault

root: TK_ZY EK 'Y' 'X' TK_QWXN | TK_APH TK_QWXN LG A 'Y';

A: DHOLL 'X' TK_APH | TK_FSS V TE | TK_FSS;

LG: DHOLL DHOLL | 'T' EI TK_HSY TK_ZY EI;

EI: LG A TK_DY EI 'X' | TK_HSY 'T' EK | V V | DHOLL V TK_DY EI | EK TK_ZY TK_QWXN A EI;

DHOLL: SCLJ 'Y' | DHOLL TK_HSY | SCLJ 'T' TK_QWXN TK_DY A;

EK: TK_APH TK_QWXN | TK_FSS A 'T' | 'Y' 'X' EI SCLJ TK_APH;

SCLJ: KSKAL 'X' V SCLJ 'T' | SCLJ TK_ZY KSKAL SCLJ TK_QI | ;

KSKAL: TK_DY LG EI TK_HSY DHOLL;

V: TK_HSY TK_DY 'T' TK_QI 'T' | TK_QWXN TK_DY | TK_FSS | TK_QWXN TK_ZY V 'Y' 'T';

TE: DHOLL SCLJ | 'T' 'X' 'Y' KSKAL A | TK_HSY LG TE TK_ZY A;

