%token TK_ZY, TK_DY, TK_QI, TK_FSS, TK_APH, TK_QWXN, TK_HSY;

%nodefault

root: KSKAL A | TK_HSY A | TK_DY | TK_ZY TK_DY 'Y';

A: TK_QI DHOLL TK_DY | V | LG | DHOLL EK LG V | V EI LG 'X';

LG: TK_ZY TK_QWXN TK_QI EI;

EI: 'X' | TK_HSY 'X' EI TK_HSY | TK_QI TK_QWXN TK_QWXN TE;

DHOLL: DHOLL LG | TK_QWXN TE TK_FSS TK_QI | EI KSKAL V TK_HSY TK_APH;

EK: TK_DY KSKAL | TK_QWXN | ;

SCLJ: TK_DY TK_APH | TK_ZY TE V | 'Y' DHOLL A 'X' TK_DY | 'X' | TK_HSY DHOLL 'Y';

KSKAL: EK TK_HSY TK_FSS DHOLL | LG;

V: TK_HSY | TK_HSY TK_FSS 'Y' TK_QWXN EI | 'T' TK_QI | 'T' EI TK_HSY TK_DY | TK_FSS LG;

TE: A 'X' 'Y' TK_FSS TK_QWXN;

