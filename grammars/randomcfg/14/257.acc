%token TK_SMUQH, TK_MMH, TK_MRT, TK_SITJ, TK_OL, TK_HTW, TK_KTPXJ, TK_TSEG, TK_ZOD, TK_QE, TK_XY;

%nodefault

root: TK_ZOD KNCQN TK_XY | 'T' | CK DK TK_SMUQH LA;

CK: YZU | FKSHU JDAC TK_XY LA GWUI;

ETKEF: GWUI TK_SMUQH TK_SITJ TK_OL TK_QE | TK_MRT | TK_SMUQH ETKEF GFCX;

DK: TK_TSEG GWUI XGAY | DK | TK_XY RMSFP | 'B' TK_KTPXJ ETKEF;

LA: TK_MMH ETKEF TK_QE | LA CK | TK_ZOD ETKEF TK_TSEG RMSFP LA | ;

JDAC: TK_KTPXJ TK_ZOD TK_MMH | TK_TSEG JDAC DK 'K' 'T' | KNCQN CK ETKEF RMSFP TK_SMUQH;

FKSHU: KNCQN TK_XY DK TK_KTPXJ | JDAC;

GFCX: TK_MMH TK_XY TK_SMUQH TK_ZOD 'T' | TK_HTW | TK_TSEG 'T' RMSFP 'T' | 'T' TK_MMH TK_TSEG 'T';

GWUI: JDAC KNCQN YZU GFCX | JDAC TK_QE | TK_QE FKSHU TK_SITJ GWUI TK_SMUQH | TK_TSEG;

RMSFP: RMSFP ETKEF | ETKEF | TK_MMH TK_ZOD TK_XY TK_SITJ GWUI;

XGAY: GWUI UNBTN DK TK_MRT;

KNCQN: DK;

UNBTN: ETKEF TK_TSEG FKSHU TK_OL TK_SITJ;

YZU: XGAY DK JDAC | 'K' 'B';

