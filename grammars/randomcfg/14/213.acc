%token TK_SMUQH, TK_MMH, TK_MRT, TK_SITJ, TK_OL, TK_HTW, TK_KTPXJ, TK_TSEG, TK_ZOD, TK_QE, TK_XY;

%nodefault

root: YZU 'T' LA TK_HTW 'T' | TK_TSEG YZU 'K';

CK: TK_MRT RMSFP TK_KTPXJ KNCQN | CK TK_HTW DK;

ETKEF: CK XGAY YZU | FKSHU TK_XY 'T' TK_MMH UNBTN | KNCQN;

DK: YZU TK_MRT JDAC 'B' ETKEF | TK_SMUQH;

LA: CK TK_ZOD 'B' | CK LA TK_MRT | XGAY TK_KTPXJ KNCQN | JDAC;

JDAC: TK_QE TK_TSEG TK_XY TK_TSEG CK;

FKSHU: 'K' ETKEF TK_XY RMSFP | TK_SMUQH | CK | ;

GFCX: GFCX KNCQN LA TK_ZOD CK | CK GWUI | TK_MRT FKSHU KNCQN TK_XY | TK_SMUQH FKSHU GFCX JDAC;

GWUI: 'K' JDAC TK_HTW TK_ZOD TK_ZOD | TK_OL TK_MMH;

RMSFP: CK FKSHU TK_MMH RMSFP XGAY | TK_ZOD TK_TSEG | JDAC;

XGAY: TK_HTW LA KNCQN | DK TK_XY UNBTN TK_TSEG | RMSFP;

KNCQN: XGAY TK_XY GWUI DK GWUI | TK_SITJ TK_OL | JDAC TK_SITJ FKSHU TK_TSEG | RMSFP;

UNBTN: TK_MMH | FKSHU TK_QE | DK DK TK_SITJ | FKSHU TK_XY CK | TK_QE;

YZU: TK_XY GFCX XGAY | 'B' KNCQN TK_MMH | TK_SITJ | KNCQN GFCX XGAY 'B' | ;

