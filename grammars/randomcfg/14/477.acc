%token TK_SMUQH, TK_MMH, TK_MRT, TK_SITJ, TK_OL, TK_HTW, TK_KTPXJ, TK_TSEG, TK_ZOD, TK_QE, TK_XY;

%nodefault

root: ETKEF DK TK_ZOD | UNBTN 'K' CK TK_QE DK | UNBTN;

CK: TK_HTW TK_MMH | TK_SMUQH | TK_TSEG LA UNBTN | LA;

ETKEF: TK_MMH | FKSHU TK_HTW 'K' TK_MRT UNBTN | ;

DK: JDAC TK_TSEG FKSHU RMSFP | XGAY TK_QE | TK_SMUQH GWUI CK | KNCQN TK_MRT TK_KTPXJ TK_MMH TK_HTW | FKSHU RMSFP XGAY DK JDAC;

LA: DK XGAY ETKEF | 'T' YZU;

JDAC: TK_OL 'B' TK_MMH | KNCQN GFCX XGAY TK_MRT | TK_MRT | FKSHU UNBTN UNBTN TK_QE | FKSHU;

FKSHU: TK_SMUQH TK_SITJ ETKEF | XGAY TK_OL LA TK_SITJ | TK_XY CK TK_QE JDAC;

GFCX: 'B' | XGAY TK_OL KNCQN | TK_MMH TK_OL | ;

GWUI: TK_KTPXJ | TK_QE DK 'K' | XGAY TK_SITJ | TK_SMUQH ETKEF | 'T' GFCX;

RMSFP: CK TK_OL TK_KTPXJ;

XGAY: YZU CK KNCQN | DK GWUI YZU;

KNCQN: FKSHU TK_XY TK_SMUQH GWUI;

UNBTN: CK GWUI UNBTN | TK_SMUQH YZU TK_KTPXJ XGAY TK_SITJ | KNCQN TK_ZOD 'K' TK_HTW TK_MMH;

YZU: GFCX TK_MMH | CK TK_MMH TK_SMUQH TK_OL TK_OL | ETKEF ETKEF TK_XY 'B' | CK YZU XGAY TK_OL | YZU TK_XY LA XGAY GFCX;

