%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: TK_ZP 'O' YS YS TK_ZP | EUQ IEHS TK_ACLW | TK_XQ TK_XQ EUQ IEHS CGYXH;

IEHS: UNV TK_UDRGI | TK_SHR TK_WYRUM TK_ZCJFW CGYXH 'O' | TK_SHR DMS | KYF IEHS 'N' TK_ZP;

BOLFN: TK_ZCJFW CGYXH TK_ZP IEHS | YS TK_ZP RX 'H' | 'H' KYF | TK_SHR 'O' TK_ACLW TK_SHR IEHS | TK_WYRUM 'N' 'H';

UNV: CGYXH TK_UDRGI CGYXH;

EUQ: DMS IEHS 'N' TK_ZCJFW EUQ | CGYXH RX TK_UDRGI 'N';

RX: BOLFN 'O' | YS TK_UDRGI EUQ BU TK_ACLW;

KYF: 'N' | UNV TK_VLAOS | YS TK_ACLW TK_FBQZZ 'N' | TK_UDRGI DMS 'O' TK_ZCJFW IEHS | 'O';

CGYXH: KYF IEHS KYF | KYF CGYXH TK_UDRGI DMS RX | TK_XQ EUQ;

BU: DMS BU KYF BU | YHWA | YS 'O' TK_ZP BU TK_WYRUM;

DMS: TK_ACLW CGYXH BOLFN UNV TK_WYRUM | ;

YS: 'H' 'H' TK_UDRGI TK_ZP TK_ACLW;

YHWA: TK_ACLW TK_UDRGI UNV CGYXH | TK_ACLW | 'H' DMS BOLFN TK_ACLW | EUQ TK_UDRGI KYF TK_XQ TK_XQ | BOLFN TK_ZP;

