%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: UNV TK_ZP TK_FBQZZ TK_SHR TK_VLAOS;

IEHS: TK_ZCJFW | TK_FBQZZ;

BOLFN: YHWA DMS 'O' CGYXH | EUQ TK_ACLW CGYXH | DMS 'H' TK_ZP | 'H' BU YHWA UNV IEHS | KYF TK_VLAOS 'N';

UNV: TK_ZP YS DMS TK_SHR UNV | TK_ACLW UNV | TK_VLAOS TK_FBQZZ TK_VLAOS EUQ | TK_UDRGI;

EUQ: TK_XQ 'N' | TK_XQ YHWA | TK_SHR CGYXH 'N' RX | TK_ZCJFW TK_FBQZZ DMS;

RX: TK_ZP TK_ZCJFW 'O' | 'N' TK_SHR 'H' DMS | 'H' TK_XQ EUQ TK_UDRGI | TK_SHR CGYXH UNV 'N' 'H' | 'O' TK_VLAOS;

KYF: TK_FBQZZ IEHS 'O' TK_UDRGI TK_VLAOS | IEHS UNV YHWA TK_ZCJFW TK_SHR;

CGYXH: TK_VLAOS TK_ZCJFW BOLFN BOLFN | YS RX;

BU: BU UNV TK_WYRUM BOLFN | UNV YS KYF | UNV BOLFN TK_ZCJFW DMS BOLFN | TK_ZP TK_ACLW KYF | BOLFN TK_FBQZZ 'N' TK_ACLW;

DMS: IEHS YS | IEHS EUQ;

YS: BOLFN IEHS TK_SHR | KYF | TK_ZCJFW BU TK_VLAOS RX | YS | ;

YHWA: BOLFN BOLFN TK_UDRGI IEHS EUQ | EUQ BU TK_WYRUM;

