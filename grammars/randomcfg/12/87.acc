%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: UNV BOLFN 'N' | DMS TK_XQ | 'H';

IEHS: RX IEHS YHWA | DMS | TK_VLAOS IEHS;

BOLFN: UNV YS BOLFN | UNV 'N' BOLFN | 'N' KYF TK_VLAOS TK_VLAOS RX;

UNV: BOLFN YHWA | TK_FBQZZ 'N' 'O' IEHS YS | TK_ACLW | TK_ZCJFW YHWA | TK_WYRUM TK_WYRUM BU TK_VLAOS 'H';

EUQ: DMS TK_ZP | YS YHWA TK_WYRUM 'N' | CGYXH | TK_XQ;

RX: 'O' DMS | TK_ZP IEHS | IEHS TK_ACLW YS | KYF;

KYF: IEHS YS UNV TK_FBQZZ YHWA;

CGYXH: TK_ZCJFW BU | YHWA TK_UDRGI EUQ 'H' | 'N' BU IEHS KYF | TK_ACLW 'O' KYF IEHS;

BU: TK_ZCJFW RX UNV TK_ZP | BOLFN DMS TK_XQ UNV;

DMS: TK_VLAOS RX TK_UDRGI UNV 'H' | TK_WYRUM TK_WYRUM | YS YS 'N' TK_XQ TK_VLAOS | TK_FBQZZ YS TK_XQ | 'O' TK_WYRUM EUQ YHWA TK_UDRGI;

YS: TK_XQ BOLFN 'N' TK_ZP UNV | TK_XQ TK_VLAOS CGYXH;

YHWA: TK_VLAOS TK_SHR 'N' | ;

