%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: 'N' TK_ZP TK_WYRUM;

IEHS: YS BU TK_UDRGI | YS TK_XQ CGYXH TK_UDRGI 'N' | TK_UDRGI IEHS 'O' 'N' TK_ZP;

BOLFN: BOLFN YS EUQ | TK_ACLW TK_VLAOS YHWA | DMS TK_SHR | TK_VLAOS | BOLFN TK_VLAOS TK_ACLW 'N' TK_ZP;

UNV: IEHS KYF YHWA TK_UDRGI | TK_WYRUM RX DMS | ;

EUQ: UNV TK_ZP 'H' YS TK_XQ | 'O' | 'H' IEHS 'O' TK_VLAOS IEHS | TK_ACLW EUQ;

RX: 'O' RX | TK_ZP RX YS | BU;

KYF: TK_ACLW 'H' 'N' BU | KYF CGYXH | 'H' DMS 'O';

CGYXH: YS 'N' | RX 'N';

BU: YHWA TK_FBQZZ UNV BOLFN TK_ACLW;

DMS: TK_UDRGI | CGYXH | TK_UDRGI TK_SHR UNV;

YS: DMS KYF TK_UDRGI TK_ZCJFW TK_VLAOS | CGYXH TK_WYRUM TK_ACLW | IEHS | BOLFN YS;

YHWA: 'N' TK_ZP;

