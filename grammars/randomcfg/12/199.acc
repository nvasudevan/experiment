%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: IEHS TK_VLAOS | 'N' CGYXH TK_ZP TK_ACLW;

IEHS: BU BU TK_SHR | 'O' TK_ZP BOLFN RX;

BOLFN: TK_WYRUM IEHS CGYXH YS | EUQ 'H' RX | YS TK_SHR TK_XQ TK_FBQZZ | YS 'H' CGYXH UNV | EUQ KYF TK_VLAOS 'H';

UNV: RX | TK_ZCJFW KYF TK_WYRUM | IEHS CGYXH TK_SHR 'H';

EUQ: TK_FBQZZ 'N';

RX: TK_FBQZZ YS TK_WYRUM TK_UDRGI 'H' | BU TK_SHR | TK_XQ TK_XQ TK_VLAOS | 'H' TK_WYRUM EUQ | KYF YS TK_SHR YHWA 'H';

KYF: YS TK_UDRGI TK_ACLW YS | TK_UDRGI 'H' | ;

CGYXH: TK_ACLW YS TK_UDRGI | IEHS RX BOLFN | 'H' RX | TK_WYRUM;

BU: DMS BU TK_VLAOS TK_SHR | 'O' | KYF YHWA;

DMS: KYF TK_ACLW BOLFN 'N' TK_UDRGI | 'O' TK_ACLW TK_FBQZZ TK_XQ 'N';

YS: BU TK_FBQZZ | TK_VLAOS IEHS KYF;

YHWA: BOLFN | IEHS CGYXH TK_FBQZZ TK_FBQZZ CGYXH;

