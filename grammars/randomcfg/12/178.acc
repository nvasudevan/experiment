%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: IEHS DMS | 'H' | TK_XQ TK_UDRGI | TK_ZCJFW RX TK_UDRGI DMS YS;

IEHS: TK_VLAOS TK_WYRUM EUQ | TK_ZCJFW BU 'O' KYF DMS | DMS BOLFN BU TK_XQ | BU YS DMS 'O' | 'O' YHWA TK_ACLW 'O' TK_UDRGI;

BOLFN: TK_SHR TK_FBQZZ;

UNV: EUQ BOLFN TK_XQ TK_ZP TK_XQ | TK_ZCJFW TK_SHR TK_SHR BOLFN | TK_ZP | 'N' 'H' TK_UDRGI;

EUQ: TK_ZP UNV TK_VLAOS;

RX: UNV TK_WYRUM BOLFN BOLFN | TK_WYRUM BU TK_ZP | TK_SHR CGYXH IEHS | TK_ACLW TK_WYRUM 'H' TK_ZCJFW UNV;

KYF: TK_WYRUM TK_ACLW RX | ;

CGYXH: UNV TK_ZP IEHS EUQ KYF | UNV TK_UDRGI TK_SHR | TK_FBQZZ | RX DMS TK_ACLW 'N' YHWA;

BU: KYF EUQ IEHS TK_SHR | YHWA TK_WYRUM TK_ACLW;

DMS: TK_WYRUM BU BU | RX DMS UNV 'H' EUQ | TK_XQ TK_WYRUM YHWA;

YS: UNV TK_ZCJFW 'N' KYF | TK_XQ YHWA | TK_VLAOS RX TK_ZCJFW DMS;

YHWA: EUQ TK_WYRUM KYF YHWA | CGYXH TK_ACLW TK_XQ | RX YHWA | UNV EUQ TK_ACLW TK_ACLW | TK_WYRUM BU RX;

