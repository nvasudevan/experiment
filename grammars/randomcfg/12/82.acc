%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: IEHS DMS TK_WYRUM TK_UDRGI 'N';

IEHS: BU | BOLFN CGYXH DMS TK_XQ 'O' | TK_FBQZZ BU | 'O' IEHS | TK_FBQZZ TK_WYRUM TK_XQ;

BOLFN: KYF TK_WYRUM 'H' EUQ 'O' | TK_WYRUM | TK_VLAOS BOLFN CGYXH;

UNV: TK_FBQZZ TK_FBQZZ EUQ EUQ | YHWA BOLFN BOLFN TK_UDRGI RX | TK_VLAOS TK_XQ KYF RX | BOLFN YS BOLFN | ;

EUQ: TK_FBQZZ BOLFN BOLFN | TK_WYRUM TK_XQ TK_XQ RX TK_ACLW | TK_ZP | RX TK_FBQZZ TK_ZCJFW;

RX: YHWA | TK_XQ 'H' TK_UDRGI TK_WYRUM | EUQ 'N';

KYF: TK_ZCJFW | 'H' TK_ZP DMS BU 'N' | TK_SHR;

CGYXH: TK_SHR RX TK_VLAOS YHWA YS | BU | DMS TK_XQ BU 'O' TK_ZCJFW | TK_ZP CGYXH | RX;

BU: TK_VLAOS BOLFN TK_ZCJFW | BOLFN | YS UNV | YS | TK_VLAOS DMS TK_UDRGI;

DMS: 'N' UNV BOLFN BU | 'N' YHWA | TK_ZCJFW TK_SHR | TK_UDRGI RX RX | ;

YS: BOLFN IEHS YHWA RX TK_WYRUM;

YHWA: UNV | TK_SHR TK_WYRUM 'N' 'O' | TK_UDRGI DMS TK_WYRUM BOLFN | YHWA CGYXH UNV EUQ;

