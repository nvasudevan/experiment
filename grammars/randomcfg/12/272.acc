%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: UNV IEHS | UNV TK_FBQZZ RX;

IEHS: TK_SHR TK_XQ YS BU | YS YHWA TK_ZP | 'H' YS | 'H' YS TK_ZCJFW CGYXH TK_SHR;

BOLFN: TK_WYRUM TK_FBQZZ BU BU EUQ | TK_VLAOS BOLFN | TK_ACLW 'H' 'O' DMS KYF;

UNV: 'H' DMS TK_SHR YHWA | KYF TK_XQ DMS TK_ACLW RX | TK_UDRGI | 'N' TK_SHR CGYXH BOLFN TK_ZCJFW | ;

EUQ: BOLFN TK_UDRGI DMS | DMS YHWA TK_UDRGI TK_ZCJFW | RX TK_VLAOS | YS RX IEHS | TK_ACLW UNV RX 'O' 'H';

RX: TK_ZP CGYXH DMS TK_WYRUM | TK_VLAOS YHWA TK_ACLW TK_WYRUM UNV | BOLFN YHWA YHWA;

KYF: YHWA IEHS | TK_WYRUM BU | BOLFN;

CGYXH: TK_XQ TK_WYRUM 'H' CGYXH | 'N' BOLFN UNV | TK_FBQZZ 'H' TK_XQ;

BU: CGYXH TK_UDRGI | TK_ACLW TK_UDRGI TK_UDRGI IEHS IEHS | KYF TK_UDRGI;

DMS: BU | DMS KYF DMS YS;

YS: YHWA CGYXH EUQ RX | TK_SHR BOLFN 'H' IEHS IEHS;

YHWA: TK_UDRGI TK_UDRGI EUQ | TK_SHR 'H' KYF DMS UNV;

