%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: IEHS RX IEHS TK_SHR IEHS | TK_SHR TK_UDRGI EUQ;

IEHS: TK_UDRGI KYF | IEHS KYF 'N' DMS YS | EUQ BOLFN | EUQ DMS TK_UDRGI TK_ZCJFW 'N' | ;

BOLFN: TK_ZP TK_ACLW | TK_FBQZZ TK_UDRGI TK_ACLW | TK_VLAOS;

UNV: TK_XQ TK_FBQZZ BU 'H' YS;

EUQ: TK_VLAOS 'N' RX | 'H' 'N' YS TK_FBQZZ | 'H' YS TK_VLAOS EUQ | EUQ TK_SHR TK_UDRGI IEHS RX;

RX: 'N' YS YS TK_VLAOS YS | TK_ACLW | 'N' UNV TK_XQ | IEHS TK_ZP 'O';

KYF: TK_SHR | TK_ZCJFW TK_FBQZZ IEHS | TK_ZCJFW RX DMS 'N' | KYF TK_XQ TK_VLAOS 'N' 'N';

CGYXH: KYF TK_ZCJFW TK_SHR | TK_XQ TK_ACLW TK_FBQZZ | TK_WYRUM TK_SHR;

BU: TK_ACLW TK_UDRGI TK_XQ YS | TK_ZCJFW UNV KYF CGYXH DMS | 'O' TK_FBQZZ EUQ TK_ACLW | RX RX TK_FBQZZ 'H';

DMS: 'H' | KYF UNV TK_ZCJFW BOLFN | TK_ZCJFW 'H' TK_ZP | BOLFN TK_WYRUM;

YS: TK_VLAOS YS | BOLFN TK_VLAOS | YHWA TK_ACLW 'N' IEHS YHWA;

YHWA: EUQ BOLFN CGYXH TK_ACLW;

