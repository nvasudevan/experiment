%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: YHWA 'H' 'H' KYF | TK_ZP | TK_UDRGI TK_ACLW | TK_ZCJFW IEHS TK_VLAOS;

IEHS: BOLFN BU | DMS 'H' TK_WYRUM | ;

BOLFN: RX TK_XQ TK_WYRUM TK_FBQZZ;

UNV: 'H' TK_ZP YHWA | KYF TK_FBQZZ | TK_ACLW TK_ACLW YHWA | TK_FBQZZ YHWA | TK_ACLW BOLFN;

EUQ: BU YS TK_ZP DMS BOLFN | 'H' | TK_SHR TK_WYRUM 'O' | ;

RX: IEHS TK_UDRGI KYF EUQ | 'N' UNV TK_WYRUM BU | RX TK_ACLW BOLFN DMS YS | TK_SHR IEHS RX 'O';

KYF: RX IEHS 'N' 'O' IEHS | KYF | YS TK_SHR IEHS UNV;

CGYXH: YS TK_SHR TK_FBQZZ | 'H' | BU IEHS DMS TK_VLAOS;

BU: BU TK_ACLW CGYXH YHWA | RX | UNV | UNV KYF BOLFN;

DMS: TK_SHR 'O' DMS | TK_XQ YS;

YS: TK_UDRGI EUQ UNV TK_VLAOS | TK_UDRGI BU | BOLFN UNV | TK_VLAOS 'O' RX 'H' BU | KYF TK_XQ;

YHWA: BOLFN | EUQ RX | IEHS 'N' IEHS TK_ACLW BU | CGYXH | IEHS 'O' TK_ZP UNV YHWA;

