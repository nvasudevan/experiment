%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: 'H' TK_ZP 'H';

IEHS: TK_ACLW TK_WYRUM IEHS EUQ | YS | DMS RX 'H' EUQ KYF;

BOLFN: 'O' CGYXH | TK_XQ TK_WYRUM RX BOLFN | 'H' IEHS RX TK_ZP TK_WYRUM;

UNV: TK_ZCJFW TK_UDRGI | TK_WYRUM EUQ TK_WYRUM | EUQ KYF;

EUQ: 'N' | 'O' KYF RX IEHS | TK_ZP TK_ACLW | RX BU EUQ BOLFN BU;

