%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: 'H' YS DMS UNV BU | TK_VLAOS KYF IEHS BOLFN;

IEHS: TK_XQ | YS;

BOLFN: BU DMS TK_ZCJFW YS;

UNV: TK_WYRUM | RX 'H' TK_ZP IEHS TK_XQ | 'O' BOLFN TK_UDRGI DMS DMS | BU TK_ZCJFW;

EUQ: CGYXH IEHS CGYXH | BOLFN DMS | CGYXH YHWA UNV TK_ACLW | UNV UNV YHWA TK_ACLW | YS;

RX: TK_WYRUM DMS BU YHWA TK_ACLW | TK_SHR;

KYF: CGYXH 'N' | TK_ACLW YHWA | YS YS 'O' KYF TK_UDRGI | 'O' IEHS TK_UDRGI BU BOLFN | ;

CGYXH: TK_VLAOS TK_XQ TK_VLAOS 'O' TK_SHR | TK_SHR;

BU: TK_FBQZZ 'O' 'H' BOLFN | TK_SHR | YHWA | 'O' TK_VLAOS KYF YS BU | CGYXH;

DMS: TK_UDRGI TK_ACLW YHWA | KYF TK_WYRUM UNV | 'H';

YS: KYF TK_ZP | TK_XQ;

YHWA: KYF YS 'O' BOLFN | TK_VLAOS TK_ZP BU UNV;

