root: IEHS IEHS | TK_ZP YHWA TK_SHR;

IEHS: 'N' TK_VLAOS BOLFN | YHWA | TK_UDRGI TK_UDRGI | TK_ZCJFW YS TK_FBQZZ | IEHS UNV;

BOLFN: CGYXH TK_ZP RX TK_FBQZZ | KYF IEHS | TK_VLAOS TK_ZP | EUQ TK_WYRUM 'N' 'H' BOLFN;

UNV: YHWA TK_UDRGI;

EUQ: TK_ZCJFW TK_UDRGI 'O' TK_FBQZZ | TK_WYRUM | CGYXH KYF TK_VLAOS IEHS 'N' | KYF TK_FBQZZ;

RX: TK_FBQZZ TK_UDRGI 'O' UNV;

KYF: TK_ZP TK_ZCJFW RX CGYXH BOLFN | YS 'N' BOLFN TK_VLAOS;

CGYXH: BU EUQ | TK_ZCJFW EUQ DMS CGYXH | TK_ZCJFW 'O' | ;

BU: UNV | DMS CGYXH DMS IEHS;

DMS: KYF YS TK_XQ | UNV TK_WYRUM EUQ | KYF TK_ZCJFW TK_VLAOS UNV;

YS: TK_WYRUM TK_VLAOS TK_ACLW DMS CGYXH | CGYXH | TK_ZCJFW BU;

YHWA: CGYXH RX TK_WYRUM TK_ZCJFW | 'O';

