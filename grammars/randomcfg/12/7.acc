%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: TK_XQ 'H' TK_WYRUM KYF | TK_UDRGI TK_WYRUM;

IEHS: YS IEHS | RX CGYXH TK_UDRGI 'O' | CGYXH BOLFN | 'H' TK_UDRGI EUQ YHWA;

BOLFN: YS TK_ACLW TK_ZCJFW TK_XQ | BU RX DMS | RX KYF TK_ZP TK_WYRUM | UNV TK_ZCJFW BU | EUQ TK_ACLW KYF;

UNV: 'H' | 'N' TK_UDRGI TK_WYRUM IEHS IEHS | TK_XQ EUQ TK_UDRGI IEHS IEHS;

EUQ: TK_SHR;

RX: TK_ZP UNV BOLFN | UNV YHWA | BOLFN TK_ACLW TK_ZCJFW 'H';

KYF: TK_SHR TK_UDRGI TK_ACLW 'H' | DMS TK_VLAOS TK_ZP | TK_ZP UNV UNV EUQ | IEHS;

CGYXH: TK_VLAOS TK_ZCJFW | TK_UDRGI CGYXH DMS UNV | BU RX TK_VLAOS | ;

BU: TK_XQ BOLFN 'O' TK_UDRGI | CGYXH | TK_ZP TK_FBQZZ;

DMS: YS TK_ZP RX | YS TK_WYRUM TK_FBQZZ | KYF YHWA TK_XQ TK_UDRGI | TK_WYRUM CGYXH UNV TK_ZP TK_SHR;

YS: UNV 'N' UNV | TK_XQ IEHS UNV | DMS TK_FBQZZ TK_ZP 'O' | YS TK_ZCJFW CGYXH;

YHWA: DMS TK_UDRGI BOLFN TK_WYRUM | TK_ZP;

