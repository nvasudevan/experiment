%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: IEHS 'N' BU UNV;

IEHS: TK_XQ TK_ZP TK_XQ TK_UDRGI;

BOLFN: TK_WYRUM TK_UDRGI 'H' TK_ZCJFW TK_FBQZZ | TK_ACLW 'O' 'N' KYF TK_ZCJFW | TK_UDRGI EUQ RX TK_UDRGI | TK_ACLW TK_ACLW TK_ZP BU KYF;

UNV: IEHS TK_ZCJFW TK_VLAOS CGYXH | YS 'N' 'O' TK_ZCJFW YHWA;

