%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: RX BU IEHS | BU TK_VLAOS YS | EUQ YHWA TK_VLAOS | YHWA TK_XQ;

IEHS: KYF 'N' YHWA | EUQ YS | KYF DMS | 'N' DMS 'O';

BOLFN: 'H' TK_SHR TK_WYRUM TK_XQ YS | KYF CGYXH EUQ YHWA TK_SHR | TK_UDRGI;

UNV: RX KYF DMS TK_UDRGI 'H' | TK_SHR YHWA KYF CGYXH YS | ;

EUQ: 'N' | RX | BOLFN YHWA TK_XQ TK_FBQZZ TK_WYRUM;

RX: TK_VLAOS UNV TK_UDRGI | 'O';

KYF: BOLFN;

CGYXH: YHWA TK_UDRGI TK_FBQZZ CGYXH TK_XQ | TK_ACLW TK_ACLW 'O' EUQ | 'H' TK_VLAOS EUQ TK_FBQZZ | YHWA YS BOLFN KYF;

BU: DMS CGYXH | 'N' BU YHWA KYF 'N' | RX TK_SHR | TK_WYRUM TK_ZCJFW;

DMS: TK_VLAOS TK_ZCJFW | TK_ZP | TK_SHR IEHS 'H' YS 'N';

YS: IEHS TK_ACLW KYF CGYXH | 'O' KYF TK_VLAOS | 'N' | TK_VLAOS TK_XQ TK_WYRUM UNV | YHWA 'N';

YHWA: TK_UDRGI TK_ZCJFW TK_ZCJFW YHWA | UNV 'H' CGYXH BU BOLFN | YS TK_VLAOS TK_ZCJFW YHWA IEHS;

