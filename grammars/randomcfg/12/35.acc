%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: 'N' BU TK_UDRGI TK_ZCJFW BU | TK_UDRGI RX 'H' TK_WYRUM DMS;

IEHS: TK_UDRGI 'N';

BOLFN: TK_UDRGI UNV TK_ZCJFW YHWA | TK_XQ 'H' YHWA IEHS TK_XQ;

UNV: TK_ACLW TK_FBQZZ TK_UDRGI 'H' | TK_ZP 'N' TK_WYRUM;

EUQ: RX 'H' 'O' IEHS BU | 'N' | BU DMS 'O' EUQ BOLFN;

RX: KYF 'O' | TK_ZCJFW TK_UDRGI TK_WYRUM TK_FBQZZ | BOLFN KYF YHWA EUQ IEHS | UNV | YS BU YHWA 'N' BU;

KYF: BOLFN EUQ DMS TK_UDRGI DMS | KYF TK_ZP | BOLFN TK_XQ 'N' TK_XQ 'N' | TK_ZCJFW YHWA TK_WYRUM TK_ZCJFW RX | KYF;

CGYXH: KYF EUQ 'H' | TK_ACLW BU | DMS | CGYXH DMS;

BU: 'H' DMS | TK_ZP TK_UDRGI TK_WYRUM TK_UDRGI | 'H' YS BU BOLFN | ;

DMS: TK_WYRUM EUQ BOLFN RX YS | EUQ UNV 'H' BOLFN RX | BOLFN TK_ZP IEHS CGYXH TK_WYRUM | KYF BOLFN BU TK_VLAOS;

YS: BU 'N' RX;

YHWA: TK_XQ 'H' | TK_FBQZZ TK_SHR | YS TK_VLAOS TK_ZP;

