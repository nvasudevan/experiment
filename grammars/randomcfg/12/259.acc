%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: 'N' TK_XQ TK_ZP YS TK_ZP | RX CGYXH RX UNV UNV;

IEHS: RX TK_ACLW 'N' | 'O' TK_VLAOS | CGYXH EUQ | RX | KYF BOLFN IEHS;

BOLFN: 'H' TK_ACLW BOLFN YHWA TK_VLAOS | UNV | ;

