KYF: 'N' 'H' | TK_WYRUM | TK_ZCJFW TK_SHR TK_FBQZZ BU | TK_ACLW 'O' | TK_VLAOS 'H' YHWA;

CGYXH: BU CGYXH | TK_SHR;

