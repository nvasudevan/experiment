%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: TK_ZP IEHS KYF TK_ACLW;

IEHS: CGYXH IEHS BOLFN TK_SHR TK_ZP | YS TK_WYRUM | TK_WYRUM UNV;

BOLFN: TK_UDRGI TK_UDRGI 'N';

UNV: 'N' IEHS YS TK_ACLW | RX IEHS BOLFN YHWA BOLFN | TK_VLAOS RX EUQ UNV BU;

EUQ: 'N' RX;

RX: TK_WYRUM YHWA RX | TK_XQ | EUQ YS;

KYF: RX 'O' RX | KYF KYF TK_ZCJFW IEHS TK_ACLW;

CGYXH: DMS BU | RX IEHS BU IEHS | TK_ZCJFW | BOLFN BU | TK_VLAOS TK_FBQZZ 'H' TK_SHR BOLFN;

BU: TK_UDRGI | TK_ZP TK_XQ TK_FBQZZ CGYXH | EUQ;

DMS: TK_XQ 'O' TK_WYRUM TK_XQ BU | YHWA TK_XQ | 'H' | YS KYF;

YS: CGYXH | KYF TK_ACLW | RX CGYXH YS | ;

YHWA: TK_ZCJFW TK_XQ EUQ | 'H' | 'H' TK_ACLW TK_SHR EUQ TK_XQ | BU CGYXH;

