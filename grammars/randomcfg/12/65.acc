%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: TK_XQ 'O' TK_FBQZZ IEHS 'H';

IEHS: YS TK_FBQZZ YS TK_FBQZZ | 'O' TK_FBQZZ 'N' RX TK_XQ;

BOLFN: RX UNV | TK_SHR;

UNV: KYF IEHS YS UNV | TK_UDRGI BOLFN YHWA | TK_XQ 'N' BOLFN | DMS TK_ZCJFW EUQ;

EUQ: YS YHWA BU | KYF | TK_FBQZZ TK_ZP TK_VLAOS | TK_UDRGI TK_XQ EUQ | BOLFN;

RX: IEHS DMS;

KYF: DMS IEHS BOLFN KYF | TK_ZP 'H' TK_VLAOS BU | TK_UDRGI;

CGYXH: TK_SHR TK_ACLW IEHS YS;

BU: DMS 'H' BU RX | 'H' TK_WYRUM TK_ACLW TK_WYRUM | DMS RX | ;

DMS: TK_XQ 'H' 'N' CGYXH KYF | 'H' TK_SHR YS TK_FBQZZ EUQ;

YS: YS CGYXH | IEHS UNV YS TK_ACLW | TK_UDRGI 'H';

YHWA: TK_ACLW;

