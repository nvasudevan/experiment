%token TK_UDRGI, TK_ACLW, TK_VLAOS, TK_ZCJFW, TK_FBQZZ, TK_ZP, TK_WYRUM, TK_XQ, TK_SHR;

%nodefault

root: IEHS KYF KYF 'N' | KYF UNV TK_SHR | CGYXH YHWA TK_VLAOS 'N' TK_XQ | TK_XQ TK_ZCJFW 'H';

IEHS: CGYXH TK_VLAOS CGYXH;

BOLFN: EUQ | ;

