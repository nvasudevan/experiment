%token TK_DLQ, TK_CDPMX, TK_TT, TK_CQCH, TK_IPC, TK_VTKPC, TK_MGBQ, TK_XOQ, TK_FMJ, TK_XPF, TK_KQ, TK_FKC, TK_UPX, TK_GDD;

%nodefault

root: 'I' TK_KQ TK_MGBQ TK_FMJ | TK_FKC;

OADPU: XHIQ TK_UPX | OADPU TR 'N' TK_MGBQ RO;

HAI: TK_XOQ | 'T' XRZ HAI | TK_XPF 'V' MOH | QKHN QMCVV;

C: 'T' XYNZD TR | TK_VTKPC TK_CQCH;

HTGXU: PAH TK_CQCH YO | PAH TK_DLQ TK_FMJ | C XYNZD | ;

