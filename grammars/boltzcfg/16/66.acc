%token TK_MH, TK_YSBB, TK_SAKV, TK_XUPUY, TK_KZ, TK_OW, TK_CQ, TK_GD, TK_EKRPO, TK_LFAX;

%nodefault

root: OXSQ TK_MH | 'N' TK_YSBB | VSWZS TK_MH | KFSP VSWZS;

BIME: URF;

URF: BIME FGTNZ | TK_EKRPO;

MLA: WQRJ;

WQRJ: URF PZRPI HIQO TK_MH PZRPI 'H' | BIME |  | NS URF TK_OW | PZRPI TK_YSBB;

KFSP: 'R';

HIQO: 'N' | FI TK_XUPUY HIQO TK_MH TK_LFAX LK;

LK: TK_MH HIQO TK_YSBB FI 'H';

PZRPI: TK_OW TK_XUPUY TK_EKRPO NORKI TK_EKRPO | FGTNZ NS;

OXSQ: TK_XUPUY TK_MH | 'P' NORKI 'N' TK_OW;

FGTNZ: KFSP | 'N' FGTNZ B B TK_OW TK_LFAX OXSQ;

B: MLA | 'R' | NORKI HIQO TK_XUPUY B TK_MH PZRPI PZRPI FI MLA B TK_GD TK_XUPUY HIQO;

VSWZS: NORKI;

NORKI: TK_XUPUY TK_LFAX FI TK_OW | TK_GD | TK_YSBB FI TK_EKRPO HIQO TK_KZ TK_XUPUY | TK_EKRPO 'Z' B TK_XUPUY TK_SAKV 'H';

FI: TK_KZ | WQRJ TK_SAKV 'P' TK_CQ 'J' KFSP | TK_YSBB | URF TK_YSBB TK_EKRPO TK_CQ | WQRJ;

NS: FI MLA | WQRJ 'Z' WQRJ 'H' | URF TK_GD MLA TK_EKRPO TK_EKRPO TK_YSBB 'J' | TK_KZ PZRPI BIME TK_OW;

