FI: VSWZS NS 'P' 'J' URF KFSP TK_YSBB 'J' | TK_LFAX TK_KZ | KFSP B | TK_MH NORKI TK_SAKV TK_CQ FGTNZ;

NS: TK_XUPUY 'J' MLA;

